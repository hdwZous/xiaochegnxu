.wrap {
  overflow: hidden;
}
.coupon_go {
  position: relative;
  color: #ff1e19;
  font-size: 20rpx;
  border: 1px solid #ff1e19;
  border-radius: 4rpx;
  align-items: center;
  height: 32rpx;
  line-height: 34rpx;
}
.coupon_tag_left {
  padding: 0 10rpx;
  align-items: center;
}
.coupon_tag_left_bg {
  background: rgba(255, 245, 244, 0.7);
}
.coupon_tag_right {
  position: relative;
  width: 74rpx;
  height: 28rpx;
  background: rgba(255, 245, 244, 0.7);
  justify-content: center;
  align-items: center;
  border-left: 1px dashed rgba(255, 143, 141, 0.6);
}
.coupon_tag_icon {
  color: #ff1e19ff;
  font-size: 18rpx;
  position: relative;
  top: 1rpx;
}
.coupon_go::before {
  z-index: 2;
  content: "";
  position: absolute;
  left: -10rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 12rpx;
  height: 12rpx;
  border-radius: 12rpx;
  background: #ffffff;
  border: 1px solid #ff1e19;
}
.coupon_go::after {
  z-index: 2;
  content: "";
  position: absolute;
  right: -10rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background: #ffffff;
  border: 1px solid #ff1e19;
}
.coupon_loading {
  position: absolute;
  width: 10px;
  height: 10px;
  background: url("https://storage.360buyimg.com/wximg/coupon/coupon_tag_loading.png")
      no-repeat center;
  background-size: 100% 100%;
  transform: rotate(0deg);
  animation: rotateClass 2s linear infinite;
  transform-origin: center;
}
@keyframes rotateClass {
  0% {
      transform: rotate(0deg);
      transform-origin: center;
  }
  100% {
      transform: rotate(360deg);
      transform-origin: center;
  }
}
