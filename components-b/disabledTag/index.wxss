.wrap {
  overflow: hidden;
}
.coupon_go {
  position: relative;
  font-size: 20rpx;
  border: 1px solid #ccc;
  border-radius: 4rpx;
  align-items: center;
  height: 32rpx;
  line-height: 34rpx;
}
.coupon_tag_left {
  padding: 0 10rpx;
  align-items: center;
}

.coupon_go::before {
  z-index: 2;
  content: "";
  position: absolute;
  left: -10rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 12rpx;
  height: 12rpx;
  border-radius: 12rpx;
  background: #ffffff;
  border: 1px solid #ccc;
}
.coupon_go::after {
  z-index: 2;
  content: "";
  position: absolute;
  right: -10rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background: #ffffff;
  border: 1px solid #ccc;
}

