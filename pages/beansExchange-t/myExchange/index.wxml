<template name="renderCodeItem"><block><view wx:key="index" class="{{style.exchangeBlock}}"><view class="{{style.img}}"><image src="{{item.skuImg || item.couponImg}}" class="{{style.skuImg}}"></image></view><view class="{{style.msgBlock}}"><view class="{{[style.title, style.code_title]}}">{{couponTitle || item.skuTitle}}</view><view class="{{style.des}}">{{availableDate || item.exchangeTime}}</view><view class="{{style.availableDate}}">券码：{{cdKey}}</view></view><view class="{{style.btn}}" bindtap="copyCode" data-e-tap-so="this" data-e-tap-a-a="{{item}}"><text class="{{style.toUse}}">复制券码</text></view></view></block></template>
<template name="renderExchangingItem"><block><view wx:key="index" class="{{style.exchangeBlock}}"><view class="{{style.img}}"><image src="{{item.skuImg || item.couponImg}}" class="{{style.skuImg}}"></image></view><view class="{{style.msgBlock}}"><view class="{{style.beansNum}}"><text class="{{style.beansText}}">¥</text>{{item.amount}}</view><view class="{{style.title}}">{{item.couponTitle || item.skuTitle}}</view><view class="{{style.des}}">{{item.limitRule}}</view><view class="{{style.availableDate}}">{{item.availableDate || item.exchangeTime}}</view></view><block><block wx:if="{{item.status == 1}}"><view class="{{style.btn}}" bindtap="toStore" data-e-tap-so="this" data-e-tap-a-a="{{item}}"><text class="{{style.toUse}}">去使用</text></view></block><block wx:else><block wx:if="{{item.status == 0}}"><view class="{{style.btn + ' ' + style.toExchangeBtn}}"><text class="{{style.exchanging}}">兑换中</text></view></block></block></block></view></block></template>
<template name="renderExchangedItem"><block><view wx:key="index" class="{{style.exchangeBlock + ' ' + style.exchangeBlockDrak}}"><view class="{{style.img}}"><image src="{{item.skuImg || item.couponImg}}" class="{{style.skuImg}}"></image></view><view class="{{style.msgBlock}}"><view class="{{style.beansNum}}"><text class="{{style.beansText}}">¥</text>{{item.amount}}</view><view class="{{style.title}}">{{item.couponTitle}}</view><view class="{{style.des}}">{{item.limitRule}}</view><view class="{{style.availableDate}}">{{item.availableDate || item.exchangeTime}}</view><image src="https://storage.360buyimg.com/wximg/vipRedPackage/dash.png" class="{{style.dash}}"></image></view><block><block wx:if="{{item.status == 2}}"><view class="{{style.used}}"><image src="https://storage.360buyimg.com/wximg/vipRedPackage/used.png" class="{{style.usedImg}}"></image></view></block><block wx:else><block wx:if="{{item.status == 3}}"><view class="{{style.overTime}}"><image src="https://storage.360buyimg.com/wximg/vipRedPackage/overTime.png" class="{{style.overTimeImg}}"></image></view></block></block></block></view></block></template>
<block wx:if="{{$taroCompReady}}"><view><view class="{{style.myExchangePage}}"><block><block wx:if="{{showEmpty}}"><default compid="{{$compid__93}}"></default></block><block wx:else><view class="{{style.exchangeList}}"><block wx:if="{{exchangeList && exchangeList.length > 0}}"><block wx:for="{{loopArray0}}" wx:for-item="item" wx:for-index="index"><block wx:if="{{item.isCdKey}}"><template is="renderCodeItem" data="{{...item.$loopState__temp2}}"></template></block><block wx:else><block><block wx:if="{{item.$original.status == 1 || item.$original.status == 0}}"><template is="renderExchangingItem" data="{{...item.$loopState__temp4}}"></template></block><block wx:else><template is="renderExchangedItem" data="{{...item.$loopState__temp6}}"></template></block></block></block></block></block></view></block></block></view></view></block>
