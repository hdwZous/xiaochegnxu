<block wx:if="{{$taroCompReady}}"><swiper indicator-color="{{indicatorColor}}" indicator-active-color="{{indicatorActiveColor}}" circular="{{circular}}" current="{{current}}" duration="{{duration}}" interval="{{interval}}" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" bindchange="onChange" style="{{anonymousState__temp}}" data-e-change-so="this"><block wx:if="{{imgArr.length > 0}}"><swiper-item style="width:100%" wx:key="item" wx:for="{{imgArr}}" wx:for-item="item" wx:for-index="index"><block><block wx:if="{{form == 'vip'}}"><image mode="widthFix" class="{{style.img}}" src="{{item.bannerUrl}}" bindtap="jumpPage" data-e-tap-so="this" data-e-tap-a-a="{{item}}"></image></block><block wx:else><image mode="widthFix" class="{{style.img}}" src="{{item}}"></image></block></block></swiper-item></block></swiper></block>
