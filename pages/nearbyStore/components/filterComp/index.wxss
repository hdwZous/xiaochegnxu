}

.text {
    height: 106rpx;
    position: relative;
    margin: 0 24rpx;
    font-size: 28rpx;
    max-width: 346rpx;
    display: inline-block;
}

.subtext {
    height: 106rpx;
    line-height: 106rpx;
    display: inline;
    white-space: nowrap;
    /* float: left; */
}

.subtext:nth-child(2) {
    font-weight: 600;
}

.countDownText {
    position: relative;
    color: #333;
    font-size: 26rpx;
    height: 106rpx;
    line-height: 106rpx;
}

.countDownTime {
    display: flex;
    align-items: center;
    position: relative;
}

.countDownTime_text {
    width: 32rpx;
    height: 32rpx;
    background: #FF4060;
    border-radius: 4rpx;
    color: #fff;
    font-size: 22rpx;
    font-weight: 500;
    line-height: 32rpx;
    margin-right: 10rpx;
    text-align: center;
    position: relative;
}

.hour::after,
.minute::after {
    content: ':';
    width: 8rpx;
    height: 32rpx;
    color: #FF4060;
    font-size: 22rpx;
    font-weight: 500;
    line-height: 32rpx;
    position: absolute;
    right: -8rpx;
    top: 0;
}

.tab_box{
    margin: 16rpx 0 18rpx 0;
}
.fadeInLeft {
    -webkit-animation: fadeInLeft .5s 1;
    animation: fadeInLeft .5s 1;
}

.fadeCommon {
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: -1;
    width: 100%;
    height: 8rpx;
    background: #4BD662;
    overflow: hidden;
}

.fadeInRight {
    -webkit-animation: fadeInRight .5s 1;
    animation: fadeInRight .5s 1;
}

@keyframes fadeInLeft {
    from {
        opacity: 0;
        -webkit-transform: translate(-100rpx, 0);
        transform: stranslate(-100rpx, 0);
    }

    to {
        opacity: 1;
        -webkit-transform: translate(0, 0);
        transform: stranslate(0, 0);
    }
}

@keyframes fadeInRight {
    from {
        opacity: 0;
        -webkit-transform: translate(100rpx, 0);
        transform: stranslate(100rpx, 0);
    }

    to {
        opacity: 1;
        -webkit-transform: translate(0, 0);
        transform: stranslate(0, 0);
    }
}

@keyframes moveScale {
    0% {
        transform: scale(0.7);
    }

    100% {
        transform: scale(1);
    }
}