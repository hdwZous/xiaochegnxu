    height: 100%;
    position: relative;
    box-sizing: border-box;
}

.price_bg {
    position: absolute;
    left: 0;
    top: 20rpx;
    z-index: 10;
}

.unit {
    position: absolute;
    left: 22rpx;
    top: 46rpx;
    color: #FF2E48;
    font-size: 24rpx;
    z-index: 15;
}

.unit.qi {
    position: absolute;
    left: 18rpx;
    top: 46rpx;
    color: #FF2E48;
    font-size: 24rpx;
    z-index: 15;
}

.priceText {
    position: absolute;
    color: #FF2E48;
    font-size: 48rpx;
    height: 106rpx;
    line-height: 106rpx;
    z-index: 15;
    left: 34rpx;
}

.priceTagText {
    position: absolute;
    right: 12rpx;
    bottom: 32rpx;
    right: -10rpx;
    color: #FF2E48;
    font-size: 12rpx;
    z-index: 20;
