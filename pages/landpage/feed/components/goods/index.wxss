.ellipsis2 {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
  }
  
  .container {
    padding: 24rpx;
    background: #000;
    min-height: 100vh;
  }
  
  .store_header_wrap {
    display: flex;
    background: #fff;
    justify-content: space-between;
    align-items: center;
    box-shadow: 0px 2px 8px 0px rgba(0, 0, 0, 0.06);
    border-radius: 12px;
    padding: 24rpx 24rpx 24rpx 16rpx;
    position: sticky;
    top: 16rpx;
    z-index: 10;
  }
  
  .header_left {
    display: flex;
  }
  
  .store_logo {
    width: 56rpx;
    height: 56rpx;
    background: #ccc;
    border-radius: 8rpx;
    margin-right: 8rpx;
    border: 1rpx solid rgba(0, 0, 0, 0);
  }
  
  .store_name {
    font-size: 24rpx;
    color: #333;
    font-weight: bold;
  }
  
  .store_friegth {
    font-size: 20rpx;
    color: #666;
  }
  
  .store_icon {
    width: 40rpx;
    height: 40rpx;
  }
  
  .list_wrap {
    margin-top: 16rpx;
  }
  
  .list {
    display: flex;
    background: #fff;
    padding: 16rpx;
    border-radius: 32rpx;
    margin-bottom: 16rpx;
  }
  
  .list_img {
    width: 320rpx;
    height: 320rpx;
    background: #f4f4f4;
    border-radius: 12rpx;
  }
  
  .list_right {
    display: flex;
    justify-content: space-between;
    flex-direction: column;
    flex: 1;
    overflow: hidden;
  }
  
  .name {
    font-size: 32rpx;
    color: #333;
    font-weight: bold;
    /* display: flex; */
    /* align-items: center; */
  }
  
  .desc {
    font-size: 24rpx;
    color: #999;
    word-wrap: break-word
  }
  
  .tel {
    display: flex;
    color: #666;
    font-size: 24rpx;
    /* align-items: top; */
  }
  
  .tel_icon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 4rpx;
    /* background: #ccc; */
    position: relative;
    top: 4rpx;
  }
  
  
  .skuTag {
    height: 32rpx;
    line-height: 32rpx;
    background: linear-gradient(90deg, #0077EE 0%, #40A0FF 100%);
    border-radius: 2px;
    color: #FFFFFF;
    font-size: 20rpx;
    padding: 0 8rpx;
    margin-right: 4rpx;
    margin-top: 6rpx;
    display: inline-block;
  }
  
  .list_left {
    position: relative;
    width: 320rpx;
    height: 320rpx;
    margin-right: 16rpx;
    font-size: 0;
  }
  
  .top_left_tag {
    position: absolute;
    top: 4rpx;
    left: 4rpx;
    border-radius: 4rpx;
    font-size: 20rpx;
    height: 32rpx;
    line-height: 32rpx;
    display: flex;
    align-items: center;
    padding: 0 8rpx 0 4rpx;
  }
  
  .top_left_tag-icon {
    width: 20rpx;
    height: 20rpx;
  }
  
  .bottom-line {
    font-size: 20rpx;
    color: #FFFFFF;
    margin-top: 32rpx;
    margin-bottom: 200rpx;
    text-align: center;
  }
  
  .list_right_bottom {
    width: 100%;
    position: relative;
  }
  
  .price_cmp {
    position: relative;
    /* top: -6rpx; */
  }
  
  .select_case {
    position: absolute;
    right: 6rpx;
    top: 6rpx;
  }
  
  .footer {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 10
  }
  
  .coupon-wrapper {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding: 0;
    font-size: 0;
    margin-top: 8rpx;
  }
  .feed_good {
    display: flex;
    background: #fff;
    padding: 16rpx;
    padding-right: 0;
    border-radius: 32rpx;
    margin-bottom: 16rpx;
  }