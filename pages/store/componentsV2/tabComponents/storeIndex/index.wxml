<block wx:if="{{showEmpty}}">
  <!-- 获取门店接口无结果 || 接口异常 -->
  <new-default type="{{type}}" 
                height="true"
                tips="{{tips}}" 
                btnText="{{btnText}}" 
                src="{{defaultSrc}}" 
                bind:defaultBtnEvent="onDefaultBtnEvent"
                fromPosition="store-tabComponents-storeIndex"
                optionsPos="{{options}}">
  </new-default>
</block>
<block wx:else>
  <scroll-view scroll-anchoring 
                enable-flex 
                scroll-with-animation 
                scroll-y 
                class="store-scroll-view container" 
                bindscroll="productScroll" 
                bindscrolltolower="productTouchBottom" 
                bindscrolltoupper="productTouchTop" 
                id="scrollId"
  >
    <block wx:for="{{floorList}}"
          wx:for-item="floor"
          wx:key="index"
    >
      <!-- 分类球 -->
      <main-cate wx:if="{{floor.floorStyle == 'mainCate' && (floor.tpl == 'tplm4' || floor.tpl == 'tplm3' || floor.tpl == 'tplm2')}}"
                data="{{floor.data}}"
                floorStyle="{{floor.tpl}}"
                bind:tellParent="tellParent"
                traceId="{{traceId}}"
                buriedObj="{{buriedObj}}"
                options="{{options}}">
      </main-cate>
      <!-- 活动轮播图 -->
      <banner-v2  wx:if="{{floor.floorStyle == 'banner' && floor.tpl == 'tplb2'}}"
                  orgCode="{{orgCode}}"
                  data="{{floor.data}}"
                  traceId="{{traceId}}"
                  buriedObj="{{buriedObj}}"
                  options="{{options}}">
      </banner-v2>

      <!-- 我常买楼层 -->
      <view class="floor-wrapper" wx:if="{{floor.floorStyle == 'product' && floor.tpl == 'tpl1-2'}}">
        <often-buy  
                    floor="{{floor}}"
                    updateNum="{{updateNum}}"
                    traceId="{{traceId}}"
                    buriedObj="{{buriedObj}}"
                    options="{{options}}"
                    tabName="{{tabName}}">
        </often-buy>
      </view>
       <!-- 爆品自提楼层 -->
       <view class="floor-wrapper" 
             wx:if="{{floor.floorStyle == 'self' && floor.tpl == 'tpl1-3.5'}}">
            <self-pick-hot-good  bgColor="{{floor.bgColor}}" 
                                 floor="{{floor}}"
                                 floorTitle="{{floor.floorTitle}}"
                                 subTitle="{{floor.subTitle}}"
                                 skuList="{{floor.data}}"
                                 pageSource="storeinfo"
                                 traceId="{{traceId}}"
                                 tag="{{floor.leftTips}}"
                                 buriedObj="{{buriedObj}}"
                                 fromPosition="store-tabComponents-storeIndex"
                                 optionsPos="{{options}}"
                                 recommendObj="{{buriedObj}}" />
        </view>

      <!-- 新人专享楼层 -->
      <view class="floor-wrapper"  wx:if="{{floor.floorStyle == 'product' && floor.tpl == 'tpl-coupon'}}">
        <newer-v2  
                  floor="{{floor}}"
                  orgCode="{{orgCode}}"
                  storeId="{{storeId}}"
                  updateNum="{{updateNum}}"
                  traceId="{{traceId}}"
                  buriedObj="{{buriedObj}}"
                  options="{{options}}"
                  tabName="{{tabName}}">
        </newer-v2>
      </view>
      <!-- 爆品楼层、分类楼层、cms楼层 -->
      <view  class="floor-wrapper" wx:if="{{floor.floorStyle == 'product' && (floor.tpl == 'tpl1-3' || floor.tpl == 'tpl2-3' || floor.tpl == 'tplN-3')}}">
        <three-in-two-row 
                          floor="{{floor}}"
                          updateNum="{{updateNum}}"
                          bind:tellParent="tellParent"
                          traceId="{{traceId}}"
                          buriedObj="{{buriedObj}}"
                          options="{{options}}"
                          tabName="{{tabName}}">
        </three-in-two-row>
      </view>
      <!-- 商家会员楼层 -->
      <view class="floor-wrapper" wx:if="{{floor.floorStyle == 'member' && floor.tpl == 'tpl-member'}}">
        <member-floor floor="{{floor}}" storeId="{{storeId}}"  bind:pageEvent="widgetEvent" traceId="{{traceId}}" buriedObj="{{buriedObj}}" options="{{options}}" />
      </view>
      <!-- 超惠买楼层 -->
      <view class="floor-wrapper" wx:if="{{floor.floorStyle == 'coupon' && floor.tpl == 'tplC-1'}}">
           <coupon-floor floor="{{floor}}" storeId="{{storeId}}" orgCode="{{orgCode}}" traceId="{{traceId}}"  buriedObj="{{buriedObj}}"  options="{{options}}" />
       </view>

      <!-- 满额促销、多买优惠楼层 -->
      <view  class="floor-wrapper" wx:if="{{floor.floorStyle == 'product' && floor.tpl == 'tpl1-3-h'}}">
        <three-in-row floor="{{floor}}" traceId="{{traceId}}" buriedObj="{{buriedObj}}"  options="{{options}}" tabName="{{tabName}}"></three-in-row>
      </view>

      <!-- 套装 -->
       <view  class="floor-wrapper" wx:if="{{floor.floorStyle == 'combuy' && floor.tpl == 'tpl1'}}">
        <suit floor="{{floor}}" updateNum="{{updateNum}}" traceId="{{traceId}}" buriedObj="{{buriedObj}}" options="{{options}}"  tabName="{{tabName}}">
        </suit>
      </view>

      <!-- 为你优选 -->
      <view  class="floor-wrapper" wx:if="{{floor.floorStyle == 'product' && floor.tpl == 'tplN-1'}}">
        <choose-for-you traceId="{{traceId}}"
                        floor="{{floor}}"
                        updateNum="{{updateNum}}"
                        buriedObj="{{buriedObj}}"
                        options="{{options}}" tabName="{{tabName}}">
        </choose-for-you>
      </view>
    </block>
    <view class="tomore padTB" bindtap="anchorProduct">
      <view>查看全部商品</view>
      <image class='more-icon' src="https://storage.360buyimg.com/wximg/common/next_green.png" />
    </view>
    <view  style="height: 72rpx"></view>
  </scroll-view>
</block>