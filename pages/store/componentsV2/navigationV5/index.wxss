.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.flex{
    display: -webkit-flex;
    display: flex;
}
.flex_row {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
}
.just_content_center{
    justify-content: center;
    -webkit-justify-content: center;
}
.text_center{
    text-align:center;
}

.flex_col {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
}

.flex_none {
    -webkit-flex: none;
    flex: none;
}

.flex_auto {
    -webkit-flex: auto;
    flex: auto;
}
.align_item_center{
    -webkit-align-items:center;
    align-items:center;
}
.search_box{
    flex: 1;
    height:100%;
    width: 0;
    /*filter:blur(4rpx);*/
    /*-webkit-filter:blur(4rpx);*/
    color: white;
    position: relative;
}
.search_opacity{
    border-radius:60rpx;
    /* background:rgba(0,0,0,0.15); */
    background:rgba(0,0,0,0.3);
    /*opacity:0.2;*/
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: -1;


}

.input_box {
  flex: 1;
  width: 0;
    border-radius: 60rpx;
    /* padding: 0 20rpx; */
    padding: 0 10rpx 0 20rpx;
    box-sizing: border-box;
    align-items: center;
    -webkit-align-items: center;
}

.icon_search {
    width: 28rpx;
    height: 28rpx;
    background: url("https://storage.360buyimg.com/wximg/storev1/search_white.png") center no-repeat;
    background-size: cover;
    margin-right: 10rpx;
    flex-shrink: 0;
}

.place_holder {
  flex: 1;
    font-size: 14px;
    font-weight: 400;
    color: white;
    line-height: 60rpx;
    white-space: nowrap;
    height: 60rpx;
    overflow: hidden;
}

.follow {
    width: 124rpx;
    height: 100%;
    background: rgba(0, 0, 0, 0.3);
    border-radius: 32rpx;
    backdrop-filter: blur(6rpx);
    font-size: 28rpx;
    font-weight: 500;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-left: 16rpx;
    color: #fff;
}

.icon_scan {
    background: url('https://storage.360buyimg.com/wximg/common/scan.png') no-repeat;
    /* width: 58rpx;
    height: 58rpx; */
    /* background-position: 16rpx; */
    background-size: 32rpx 32rpx;
    width: 52rpx;
    height: 52rpx;
    background-position: 7rpx;
    flex-shrink: 0;
}

.coupon-btn {
    width: 96rpx;
    height: 64rpx;
}
.coupon-btn image {
    width: 100%;
    height: 100%;
}