<view class="newer-wrapper" style="background: linear-gradient(to bottom, {{floor.bgColor}}, #F8F8F8);">
  <view class='store-floor-tit'>
    <image class="title-icon" src="{{floor.titleIcon}}" aria-hidden="true"></image>
    <view class="title" aria-role='text' aria-label="{{floor.floorTitle}}">{{floor.floorTitle}}</view>

    <view class='newer-tomore'
          wx:if="{{floor.moreBtnDesc}}" 
          data-to="{{floor.to}}" 
          data-params="{{floor.params}}" 
          bindtap="jumpPromoList">
      <view aria-role='link' aria-label="{{floor.moreBtnDesc}}"  class='more-text'>{{floor.moreBtnDesc}}</view>
      <image aria-hidden="true" class='more-icon' src="https://storage.360buyimg.com/wximg/common/store-next.png" />
    </view>
  </view>
  <view class="sub-title" wx:if="{{floor.subTitle}}" aria-hidden="true">{{floor.subTitle}}</view>

  <view class='pro-coupon-wrapper'>
    <view class='coupon-wrapper' wx:if="{{floor.couponModelList.length > 0}}">
      <view class='one-coupon' wx:if="{{floor.couponModelList.length == 1}}">
        <coupon-item  type="1"  
                      circleBgColor="{{floor.bgColor}}" 
                      item="{{floor.couponModelList[0]}}"
                      bind:rightBtnClick="onClickCoupon"
                      traceId="{{traceId}}"
                      pageId="{{buriedObj.pageIdFirstPage}}"
                      currentPageName="{{buriedObj.currentPageName}}"
                      prePageName="{{buriedObj.prePageName}}"
                      buriedObj="{{buriedObj}}"
                      fromPosition="store-newerSV2"
                      optionsPos="{{options}}"
                      recommendObj="{{recommendObj}}">
        </coupon-item>
      </view>
      <view class="coupons" wx:else>
        <scroll-view scroll-x="true"  enable-flex  class='some-coupons'>
          <block wx:for="{{couponModelList}}"
                 wx:key="index"
                 wx:for-item="item">
            <view class='some-coupon-item'>
              <coupon-item  type="1" 
                            circleBgColor="{{floor.bgColor}}" 
                            item="{{item}}"
                            bind:rightBtnClick="onClickCoupon"
                            traceId="{{traceId}}"
                            pageId="{{pageId}}"
                            currentPageName="{{buriedObj.currentPageName}}"
                            prePageName="{{buriedObj.prePageName}}"
                            buriedObj="{{buriedObj}}"
                            fromPosition="store-newerSV2"
                            optionsPos="{{options}}">
              </coupon-item>
            </view>
          </block>
        </scroll-view>
      </view>
    </view>
    <view wx:if="{{floor.data.length > 2}}" class='pro-list-wrapper' style="background: {{floor.data.length <= 3 ? '#fff' : 'none'}}">
      <scroll-view class="scroll-wrapper" scroll-x="true">
        <view class="scroll-block">
          <block wx:for="{{list}}"
                 wx:key="index"
                 wx:for-item="item">
                  <view class="item-wrapper ep_newer-pro" 
                        data-item="{{item}}" 
                        data-sku-id="{{item.skuId}}"
                        data-spu-id="{{item.spuId}}"
                        data-store-id="{{item.storeId}}"
                        data-org-code="{{item.orgCode}}"
                        data-user-action="{{item.userAction}}"
                        data-trace-id="{{traceId}}"
                        data-page-id="{{buriedObj.pageIdFirstPage}}"
                        data-current-page-name="{{buriedObj.currentPageName}}"
                        data-pre-page-name="{{buriedObj.prePageName}}"
                        bindtap="jumpDetail">
                    <view class='pro-pic'>
                      <webp-img webp-img="img" src="{{(item.showModel && item.spuImg) ? item.spuImg : item.imgUrl}}" mode="widthFix" needOverlay="{{true}}" skuName="{{item.skuName}}"></webp-img>
                      <!-- 冷冻or冷藏 -->
                      <view class='freeze-wrap' aria-hidden="true">
                        <freeze-tag topLeftCornerTag="{{item.topLeftCornerTag}}" />
                      </view>
                      <!-- 底部资源位 -->
                      <bottom-corner-tag wx:if="{{item.bottomCornerTag}}" infos="{{item.bottomCornerTag}}" moduleId="SkuYhsOne" showHandle="{{true}}" />
                      <block wx:else>
                        <view aria-hidden="true" wx:if="{{item.tags.length > 0 }}" class="pro-sale-slogan" style="background: linear-gradient(to bottom right, {{item.tags[0].startColorCode}} 0%,{{item.tags[0].endColorCode}} 100%);">
                          {{item.tags[0].iconText}}
                        </view>
                      </block>
                    </view>
                    <view class="bottom">
                      <view class="pro-title" aria-role="link" aria-label="{{item.skuName}}">{{item.skuName}}</view>
                      <view class='price-car-wrapper'>
                        <view class='price-group'  aria-role='link' aria-label="现价{{item.majorPrice.price}}元.{{item.toHandPrice ? item.toHandPrice.rightText + '.' + item.toHandPrice.leftText : item.minorPrice ? '原价' + item.minorPrice.price + '元' : ''}} ">
                          <!-- major-price -->
                          <coupon-price majorPrice="{{item.majorPrice}}" 
                                        majorPriceFz="28rpx" 
                                        majorPriceWhFz="24rpx" 
                                        intervalPrice="{{item.intervalPrice}}" />
                          <!-- minor-price -->
                          <coupon-price minorPrice="{{item.minorPrice}}" />
                        </view>
                        <view class="car-btn">
                          <view class="add-group" wx:if="{{item.showCartButton}}">
                            <add-min-controller-v1 showType="{{1}}"  
                                                   count="{{item.showModel ? item.spuCartCount :  item.incartCount}}"
                                                   skuId="{{item.skuId}}" 
                                                   spuId="{{item.spuId}}" 
                                                   orgCode="{{item.orgCode}}" 
                                                   storeId="{{item.storeId}}"   
                                                   weighting="{{item.weighting}}"
                                                   weight="{{item.cartWeight}}"
                                                   userAction="{{item.userActionSku || item.userAction}}"
                                                   biPageName="storeinfo"
                                                   pageId="{{buriedObj.pageIdFirstPage}}"
                                                   currentPageName="{{buriedObj.currentPageName}}"
                                                   prePageName="{{buriedObj.prePageName}}"
                                                   traceId="{{traceId}}"
                                                   iconType="{{item.iconType}}"
                                                   transmitData="{{item.transmitData}}"
                                                   to="{{item.to}}"
                                                   params="{{item.params}}"
                                                   tabName="{{tabName}}"
                                                   showModel="{{item.showModel}}">
                           </add-min-controller-v1>
                          </view>
                          <view wx:else class="go-detail-wrapper">
                            <image class='go-detail' src="https://storage.360buyimg.com/wximg/common/next_green.png" />
                          </view>
                        </view>
                      </view>  
                    </view>
                  </view>
          </block>
        </view>
      </scroll-view>
    </view>
  </view>
</view>