.padTB {
  padding: 30rpx 0;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
/*一行打点*/
.ellipsis {
  height: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/*两行打点*/
.ellipsis2 {
  height: 60rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.wrapper {
  width: 100%;
  border-radius:20rpx 0px 0px 0px;
  /* margin-bottom: 52rpx; */
}
.store-floor-tit {
  position: relative;
  padding: 0rpx 0 0rpx 20rpx;
  /* margin-bottom: 30rpx; */
  display: flex;
  justify-content: space-between;
}
.store-floor-tit .logo-title {
    display: flex;
    align-items: center;
    max-width: 78%;
    height: 96rpx;
}
.store-floor-tit .title {
  width: 450rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 36rpx;
  /* line-height: 36rpx; */
  color: #333;
  font-weight: bold;
  margin-left: 4rpx;
  height: 100%;
  line-height: 96rpx;
}
.tomore1 {
  display: flex;
  justify-content: center;
  align-items: center; 
  margin-right: 20rpx;
  height: 96rpx;
}
.more-text {
  color: #333;
  font-size: 24rpx;
}
.more-icon {
  width: 26rpx;
  height: 26rpx;
  /* vertical-align: middle; */
  /* margin-left: 4rpx; */
}
.abs {
  /* position: absolute;
  right: 20rpx;
  bottom: 28rpx; */
}
.store-floor-tit .title-icon {
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}

/* 商品列表 */
.pro-list-wrapper {
  /* width:690rpx; */
  width: 710rpx;
  /* padding-left: 20rpx; */
  background:#fff;
  border-radius:12rpx 8rpx 8rpx 12rpx;
  margin: 18rpx 0 0 20rpx;
}
.pro-content {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-evenly;
}
.floorTail {
  display: flex;
