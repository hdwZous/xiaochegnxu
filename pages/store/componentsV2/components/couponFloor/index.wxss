.coupon-wrapper {
    width: 710rpx;
    margin: 0 auto;
}
.coupon-item {
    width: 100%;
    height: 206rpx;
    display: flex;
    justify-content: flex-start;
    margin-bottom: 16rpx;
    border-radius: 12rpx;
    overflow: hidden;
}
.coupon-item .left {
    flex: 1;
    background: #fff
}

.coupon-item .coupon-name {
    display: flex;
    justify-content: flex-start;
    margin-top: 16rpx;
    margin-left: 32rpx;
}
