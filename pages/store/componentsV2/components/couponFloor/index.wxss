.cp-yhy-tag {
  padding: 0 8rpx;
  margin-right: 8rpx;
  font-size: 20rpx;
  height: 32rpx;
  font-weight: 400;
  position: relative;
  overflow: initial;
  display: flex;
  align-items: center;
}
.cp-yhy-tag-text {
  line-height: 24rpx;
  max-width: 100%;
  overflow: hidden;
}
.cp-yhy-tag-border {
  position: absolute;
  width: 200%;
  height: 200%;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%) scale(0.5);
  transform-origin: center;
  border-radius: 8rpx;
}
.coupon-item .coupon-title {
    font-size: 24rpx;
    font-weight: bold;
    color: #333333;
}
.coupon-item .sku-list {
    margin-top: 16rpx;
    margin-left: 32rpx;
    display: flex;
    justify-content: flex-start;
}
.sku-list .sku {
    width: 120rpx;
    height: 120rpx;
    border-radius: 8rpx;
    position: relative;
    overflow: hidden;
    margin-right: 34rpx;
}
.sku-list .sku:last-child {
    margin-right: 0rpx;
}
.sku-list .sku-img {
    display: block;
    width: 120rpx;
    height: 120rpx;
    position: relative;
    border-radius: 8rpx;
}
.sku-img::after {
    position: absolute;
    content: '';
    background: rgba(51, 51, 51, 0.03);
    z-index: 2;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}
.sku-price {
    font-size: 24rpx;
    font-weight: 400;
    width: 100%;
    text-align: center;
    color: #333333;
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 3;
    font-family: regular;
    background: rgba(255, 255, 255, 0.85);
}
.coupon-item .center {
    width: 40rpx;
    height: 206rpx;
    background: url('https://storage.360buyimg.com/wximg/couponFloor.png') no-repeat;
    background-size: 100% 100%;
}
.coupon-item .right {
    width: 196rpx;
    background: #FFFCF9;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    /* align-items: center; */
    align-items: flex-end;
}
.coupon-item .price {
    font-family: regular;
    color: #FF1E1A;
    width: 110%;
    text-align: center;
}
.coupon-item .limit {
    font-size: 24rpx;
    font-weight: 400;
    color: #333333;
    margin-top: 8rpx;
    width: 110%;
    text-align: center;
}
.coupon-item .button {
    border-radius: 24rpx;
    /* width: 152rpx; */
    text-align: center;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 24rpx;
    font-weight: 500;
    padding: 0 40rpx;
    margin-top: 16rpx;
    margin-right: 32rpx;
}
.floor-tail {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 8rpx 0 24rpx 0;
  border-top: solid 1rpx #f6f6f6;
  font-size: 28rpx !important;
  font-weight: 500;
}