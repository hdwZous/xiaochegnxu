<view class='good-item ep_one-item' 
        style="background: {{item.checked ? 'rgba(71, 179, 79, 0.05)' : '#fff'}}"
        bindtap="goToDetail" 
        data-item="{{item}}"
        data-trace-id="{{traceId}}"
        data-user-action="{{item.userAction}}"
        data-page-id="{{buriedObj.pageIdFirstPage}}"
        data-current-page-name="{{buriedObj.currentPageName}}"
        data-pre-page-name="{{buriedObj.prePageName}}">
    <view class="left">
        <view class='good-pic'>
            <webp-img webp-img="good-pic" src="{{(item.showModel && item.spuImg) ? item.spuImg : item.imgUrl}}" mode="widthFix" skuName="{{item.skuName}}"></webp-img>
        </view>
        <view wx:if="{{!item.fixedStatus && item.skuType != 1}}" class="store_stockCount_tag" aria-label="{{item.skuName}}" aria-role='link'>已下架</view>
        <view wx:if="{{item.stockCount < 1  && item.skuType != 1}}" class="store_stockCount_tag"  aria-label="{{item.skuName}}" aria-role='link'>已抢光</view>
        <view wx:if="{{item.stockCount <= 3 && item.stockCount>0}}"   
              class="store-sku-remain" aria-label="{{item.skuName}}" aria-role='link'>
            仅剩
            <text>{{item.stockCount}}</text>
            件
        </view>
        <!-- 业务标 -->
        <view class='sky-yhe-one-business-tag'>
        <biz-tag bizType="horizontal" businessTag="{{item.businessTag}}"/>
        </view>
    </view>
    <view class="right">
        <view class='flex-auto'>
            <view class="good-title ellipsis" aria-label="{{item.skuName}}" aria-role='link'>
                <!-- <view aria-hidden="true" class='sku-name-tag' wx:if="{{item.skuNameTag && item.skuNameTag.iconText}}" style="color: {{item.skuNameTag.iconTextColorCode}};background: linear-gradient(135deg, {{item.skuNameTag.startColorCode}} 0%,  {{item.skuNameTag.endColorCode}} 100%);">
                    {{item.skuNameTag.iconText}}
                </view> -->
                 <sku-name-tag wx:if="{{item.skuNameTag}}" infos="{{item.skuNameTag}}"></sku-name-tag>
                 <text class="good-title-name">{{item.skuName}}</text>
            </view>
            <view class='description ellipsis' wx:if="{{item.skuDesc}}" aria-role='link' aria-label="{{item.skuDesc}}">
                <image class='desc-icon' 
                        wx:if="{{item.skuDescIcon}}" 
                        src="{{item.skuDescIcon}}"
                        aria-hidden="true" />
                {{item.skuDesc}}
            </view>
            <view class='description ellipsis' 
                  wx:elif="{{item.funcIndicatinsOrAdWord}}"
                  aria-role='link' aria-label="{{item.funcIndicatinsOrAdWord}}">
                {{item.funcIndicatinsOrAdWord}}
            </view>
            <view class='description ellipsis' 
                  wx:elif="{{item.aword}}"
                  aria-role='link' aria-label="{{item.aword}}">
                  {{item.aword}}
            </view>
            <view class="good-sale-highopinion" aria-role='link' aria-laabel="月售{{item.monthSales}}.{{item.highOpinion}}">
                <text>月售{{item.monthSales}}</text>
                <text class='hign-opinion'>{{item.highOpinion}}</text>
            </view>
            <view class="flex-row tag_case">
              <!-- 不可用券标 -->
              <view class="flex-none disabled-tag" 
                    wx:if="{{item.disabledTags && item.disabledTags.length}}">
                <block wx:for="{{item.disabledTags}}"
                      wx:key="item.disabledTags"
                      wx:for-item="disabledTag">
                  <disabled-tag data="{{disabledTag}}" />
                </block>
              </view>
              <!-- 促销标 -->
              <view class="flex-none good-tag-list" 
                    wx:if="{{item.tags.length > 0}}">
                  <block wx:for="{{item.tags}}" 
                          wx:for-item="tag" 
                          wx:key="index">
                      <view class='good-tag' 
                            style="color: {{tag.strokeNameColorCode}};border: solid 2rpx {{tag.strokeNameColorCode}}">
                          {{tag.iconText}}
                      </view>
                  </block>
              </view>
              <!-- 优惠券标 -->
              <view class="flex-none">
                  <coupon-go-tag data="{{item.couponVOList[0]}}" 
                                sku-id="{{item.skuId || ''}}" 
                                org-code="{{item.orgCode || ''}}" 
                                store-id="{{item.storeId || ''}}" 
                                from="promotionPage"
                                user-action="{{item.userActionSku}}"
                                buriedObj="{{buriedObj}}"
                                fromPosition="store-oneItem">
                  </coupon-go-tag>
              </view>
            </view>
        </view>
        <view class='price-car-wrapper'>
            <view class='price-group' aria-role='link'  aria-label="现价{{item.majorPrice.price}}元，{{item.toHandPrice && item.toHandPrice.rightText ? item.toHandPrice.rightText + '.' + item.toHandPrice.leftText : item.minorPrice && item.minorPrice.price && (item.minorPrice.priceType == 3 || item.minorPrice.priceType == 4 || item.minorPrice.priceType == 10) ? '会员价' + item.minorPrice.price + '元' :  item.minorPrice &&  item.minorPrice.price ? '原价' + item.minorPrice.price + '元' : ''}} ">
                <!-- major-price -->
                <coupon-price majorPrice="{{item.majorPrice}}" 
                              majorPriceFz="28rpx" 
                              majorPriceWhFz="24rpx" 
                              intervalPrice="{{item.intervalPrice}}" />
                <!-- minor-price || 预估到手价-->
                <coupon-price minorPrice="{{item.minorPrice}}"
                              toHandPrice="{{item.toHandPrice}}" />
            </view>
            <view class="car-btn" 
                  style="margin-bottom: {{item.minorPrice && item.minorPrice.price ? '0rpx' : '-8rpx'}}">
              <view class="add-group" 
                  wx:if="{{item.showCartButton}}">
                  <view class="select_case">
                      <add-min-controller count="{{item.showModel ? item.spuCartCount :  item.incartCount}}"  
                                          skuId="{{item.skuId}}" 
                                          spuId="{{item.spuId}}" 
                                          data-sku-id="{{item.skuId}}" 
                                          orgCode="{{item.orgCode}}" 
                                          storeId="{{item.storeId}}" 
                                          longitude="{{longitude}}" 
                                          latitude="{{latitude}}" 
                                          isAddCart="{{item.isAddCart}}" 
                                          tags="{{item.tags}}"
                                          weighting="{{item.weighting}}" 
                                          weight="{{item.cartWeight}}"
                                          userAction="{{item.userActionSku || item.userAction}}"
                                          biPageName="storeinfo"
                                          addCartText="{{item.weightPriceStr}}" 
                                          showType="2"
                                          pageId="{{buriedObj.pageIdFirstPage}}"
                                          currentPageName="{{buriedObj.currentPageName}}"
                                          prePageName="{{buriedObj.prePageName}}"
                                          traceId="{{traceId}}" 
                                          iconType="{{item.iconType}}" 
                                          transmitData="{{item.transmitData}}"
                                          to="{{item.to}}" 
                                          params="{{item.params}}"
                                          tabName="{{tabName}}"
                                          showModel="{{item.showModel}}">
                      </add-min-controller>
                  </view>
              </view>
              <view wx:else class="go-detail-wrapper">
                  <image class='go-detail' 
                      src="https://storage.360buyimg.com/wximg/common/next_green.png" />
              </view>
                
            </view>
        </view>
    </view>
</view>