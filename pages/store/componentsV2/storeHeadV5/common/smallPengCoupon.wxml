
<!-- 膨胀券未膨胀 -->
<view class="coupon-item bg2" aria-hidden="true">
  <view class="coupon-left_p left-bg2" aria-hidden="true">
    <image class='p' src="https://storage.360buyimg.com/wximg/common/store-expand-icon.png" />
  </view>
  <!-- 券标题 -->
   <view class='coupon-center-peng color2' aria-hidden="true">
    <block wx:for="{{couponItem.showTitleList}}"
           wx:for-item="subItem"
           wx:key="index">
        <view wx:if="{{subItem.status == 0}}" 
              class=" {{subItem.type ? 'price-peng' : 'font20'}}">
          {{subItem.text}}
        </view>
    </block>
  </view>
  <view class="coupon-line" aria-hidden="true">
    <image class="coupon-line-img" src="https://storage.360buyimg.com/wximg/common/couponbg2.png" />
  </view>
  <view class="coupon-right color2" aria-hidden="true">{{couponItem.couponButton.title}}</view>
</view>