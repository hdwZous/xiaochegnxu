.half_bg{
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background:rgba(0,0,0,0.7);
    z-index:20;
}
.half_content {
    /* transition: all 0.3s ease;
    -webkit-transition: all 0.3s ease; */
    background: #fff;
    position: fixed;
    bottom: 0;
    left:0;
    right: 0;
    top: 100%;
    z-index: 21;
    width: 100%;
    border-radius: 20rpx 20rpx 0 0;
    /* height: 0; */
    height: 80vh;
}
.show_content{
    /* height: 80vh;
    top: 20%; */
    /* transform: translateY(0)!important; */
    animation: upShow 0.5s ease;
    transform: translateY(-100%);
}
@keyframes upShow {
    from{
      transform: translateY(0%);
    }
    to{
      transform: translateY(-100%);
    }
}
.hide_cont{
    animation: downShow 0.5s ease;
    transform: translateY(0%);
}
@keyframes downShow {
    from{
        transform: translateY(-100%);
    }
    to{
        transform: translateY(0%);
    }
}
