.wrap {
    background: white;
    width: 100%;
    height: 100%;
    position: absolute;
    /*background: #9C0B2E;*/
    top:0;
    left: 0;
}
.show{
    transform: translateY(0);
}

.wrap .search {
    -webkit-flex: none;
    flex: none;
    padding: 20rpx 30rpx;
    background: #47B34F;
}

.wrap .search .input {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
    padding: 0 20rpx;
    height: 60rpx;
    border-radius: 60rpx;
    background: #F4F4F4;
}

.wrap .search .icon_search {
    margin-right: 10rpx;
    width: 30rpx;
    height: 30rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}

.wrap .search .placeholder {
    color: #999999;
    font-size: 24rpx;
}

.wrap .map {
    position: relative;
    -webkit-flex: none;
    flex: none;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
    height: 104rpx;
    padding: 0 30rpx;
}

.wrap .map:before {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    width: 200%;
    height: 1px;
    background: #E8E8E8;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}

.wrap .map .icon_location {
    -webkit-flex: none;
    flex: none;
    margin-right: 20rpx;
    width: 40rpx;
    height: 40rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}

.wrap .map .txt {
    -webkit-flex: auto;
    flex: auto;
    color: #47B34F;
    font-size: 32rpx;
}

.wrap .map .icon_arrow {
    -webkit-flex: none;
    flex: none;
    width: 32rpx;
    height: 32rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}

.wrap .title {
    -webkit-flex: none;
    flex: none;
    color: #333333;
    font-size: 34rpx;
    padding: 34rpx 0 0 20rpx;
}

.wrap .address {
    position: relative;
    -webkit-flex: auto;
    flex: auto;
}

.wrap .address .scroll {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
}
.scroll::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.wrap .address .item {
    position: relative;
    padding: 30rpx 0 30rpx 20rpx;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    overflow: hidden;
}

.wrap .address .item:before {
    content: "";
    position: absolute;
    bottom: 0;
    left: 20rpx;
    width: 200%;
    height: 1px;
    background: #E8E8E8;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}

.wrap .check {
    position: relative;
    left: -20rpx;
    top: -30rpx;
    width: 46rpx;
    height: 46rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
    flex: none;
    -webkit-flex: none;
}

.wrap .address .info {
    -webkit-flex: auto;
    flex: auto;
}

.wrap .address .txt {
    color: #999999;
    font-size: 26rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-top: 18rpx;

}

.wrap .address .name {
    padding-right: 20rpx;
}

.wrap .address .location {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    overflow: hidden;
}

.wrap .address .tag {
    -webkit-flex: none;
    flex: none;
    width: 60rpx;
    padding-left: 6rpx;
    padding-right: 6rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    color: #FFFFFF;
    font-size: 20rpx;
    border-radius: 4rpx;
    background: #4BC855;
    position: relative;
    top: 4rpx;
    margin-right: 20rpx;
}

.wrap .address .text {
    display: -webkit-box;
    -webkit-flex: auto;
    flex: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    color: #333333;
    font-size: 26rpx;
}

.wrap .address .edit {
    -webkit-flex: none;
    flex: none;
    width: 70rpx;
    background: url("https://storage.360buyimg.com/wximg/medicine/edit.png") center no-repeat;
    -webkit-background-size: 31rpx 31rpx;
    background-size: 31rpx 31rpx;
}

.wrap .address .disabled_title {
    height: 70rpx;
    line-height: 70rpx;
    padding-left: 20rpx;
    color: #999999;
    font-size: 28rpx;
    background: #f4f4f4;
}

.wrap .address .disabled .tag {
    color: rgba(255, 255, 255, .8);
    background: #d6d6d6;
}

.wrap .address .disabled .text {
    color: #999999;
}

.wrap .address .disabled .edit {
    width: 70rpx;
    height: 31rpx;
    background: url("https://storage.360buyimg.com/wximg/medicine/edit.png") center no-repeat;
    background-size: 31rpx;
    /*-webkit-background-size: 28rpx 26rpx;*/
    /*background-size: 28rpx 26rpx;*/
}
