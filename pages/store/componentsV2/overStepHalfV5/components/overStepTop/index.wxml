<view class="top">
  <view class="close_icon" bindtap="hidePop" aria-label="关闭弹窗" aria-role='button'></view>
  <view class="title" aria-label="{{mainTitle}}" aria-role='text'>
    {{mainTitle || '本店超出配送范围'}}
  </view>
  <view class="sub_title" wx:if="{{subtitle}}" aria-label="{{subtitle}}" aria-role='text'>
    {{subtitle}}
  </view>
  <view class="address_box" wx:if="{{isLogin && poi}}">
    <view class="add_content" bindtap="handelAddress" aria-label="更换地址" aria-role='link'>
      <view class="change_icon "></view>
      <view class="title_text ellipsis" aria-label="{{poi}}" aria-role='text'>
        {{poi}}
      </view>
      <view class="right_text" aria-label="更换地址" aria-role='link'>更换地址</view>
      <view class="go_address" aria-label="更换地址" aria-role='link'></view>
    </view>
  </view>
  <view class="radius-left-bottom"></view>
  <view class="radius-right-bottom"></view>
</view>
