<view class='top2'>
  <image class="top2-default-image" 
         wx:if="{{imageUrl}}" 
         src="{{imageUrl}}"
         aria-label="{{mainTitle}}" 
         aria-role='text' />
  <view class="top2-default-title2" 
        wx:if="{{mainTitle}}" 
        aria-label="{{mainTitle}}" 
        aria-role='text'>
        {{mainTitle}}
  </view>
  <view class="top2-default-subtitle2" 
        wx:if="{{subtitle}}"
        aria-label="{{subtitle}}" 
        aria-role='text'>
        {{subtitle}}
  </view>
</view>