.border {
  position: relative;
}
.border:after {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 200%;
  height: 200%;
  border-width: 1px;
  border-style: solid;
  border-color: #fff;
  -webkit-transform-origin: left top;
  transform-origin: left top;
  -webkit-transform: scale(0.5, 0.5);
  transform: scale(0.5);
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 20;
  background: rgba(0, 0, 0, 0.79);
}
.pop-container {
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
