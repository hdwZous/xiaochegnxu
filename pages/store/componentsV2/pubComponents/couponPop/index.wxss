    display: flex;
    justify-content: center;
    align-items: center;
}
.pro-and-coupon-wrap .coupon-rule {
    /* display: inline-block; */
    /* max-width: 240rpx; */
    max-width: 252rpx;
    height: 56rpx;
    background: #F03038;
    border-radius: 28rpx;
    line-height: 56rpx;
    text-align: center;
    padding: 0 22rpx;
    font-size: 28rpx;
    font-weight: 400;
