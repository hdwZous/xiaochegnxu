.pro-and-coupon-wrap {
    position: fixed;
    width: 562rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 13;
}
.pro-and-coupon-wrap .close-icon {
    width: 82rpx;
    height: 82rpx;
    background: url('https://storage.360buyimg.com/wximg/common/close_circle.png') no-repeat;
    background-size: 100% 100%;
    position: absolute;
    bottom: -126rpx;
    left: 50%;
    transform: translateX(-50%);
}
.pro-and-coupon-wrap .content {
    width: 100%;
    min-height: 320rpx;
    padding-bottom: 58rpx;
    border-radius: 32rpx;
    position: relative;
    border: 6rpx solid #FFCD92;
    background-color: #F03038;
    z-index: -1;
    box-sizing: border-box;
}

.pro-and-coupon-wrap .header {
    border-radius: 38rpx 38rpx 0 0;
    width: 100%;
    height: 320rpx;
    background: url('https://storage.360buyimg.com/wximg/common/hot-header.png') no-repeat;
    background-size: 100% 100%;
    position: absolute;
    left: 0;
    top: -2rpx;
    z-index: -1;
}
.pro-and-coupon-wrap .opacity-blank {
    width: 100%;
    height: 186rpx;
}

.pro-and-coupon-wrap .buy-button {
    width: 260rpx;
    height: 63rpx;
    border-radius: 32rpx;
    background: url('https://storage.360buyimg.com/wximg/common/quick-buy.png') no-repeat;
    background-size: 100% 100%;
    position: absolute;
    left: 50%;
    bottom: 0;
    transform: translate(-50%, 50%);
}
.pro-and-coupon-wrap .select {
    width: 109rpx;
    height: 48rpx;
    background: linear-gradient(0deg, #FCCD6F, #FAE198);
    border-radius: 24rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #7C4B05;
    text-align: center;
    line-height: 48rpx;
}
.marR12 {
    margin-right: 12rpx !important;
}
.marT12 {
    margin-top: 12rpx !important;
}
.marAuto {
    margin: 0 auto;
}
.h24 {
    height: 24rpx;
}
.font24 {
    font-size: 24rpx;
}
.font44 {
    font-size: 44rpx;
}
.pro-and-coupon-wrap .price {
    display: flex;
    align-items: baseline;
    justify-content: center;
    color: #FF1E19;
    font-family: regular;
}
.pro-and-coupon-wrap .unit {
    font-size: 24rpx;
}
.pro-and-coupon-wrap .value {
    font-size: 34rpx;
}
.pro-and-coupon-wrap .slogan {
    width: 90rpx;
    height: 90rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.pro-and-coupon-wrap .one {
    width: 466rpx;
    height: 168rpx;
    background: #FFFFFF;
    border: 6rpx solid #FFCD92;
    box-shadow: 0rpx 4rpx 8rpx 0rpx #D50303;
    border-radius: 20rpx;
    margin: 0 auto;
    display: flex;
    align-items: center;
}
.one > .left {
    padding-left: 40rpx;
    width: 140rpx;
    height: 100%;
    display: flex;
    align-items: center;
    position: relative;
}
.one > .right {
    margin-left: 32rpx;
}
.one .sku-img {
    width: 139rpx;
    height: 140rpx;
    border-radius: 15rpx
}
.one .sku-name {
    width: 218rpx;
    height: 60rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #333333;
    line-height: 30rpx;
    margin-bottom: 26rpx;
}
.one .sku-price {
    display: flex;
    align-items: center;
}

.pro-and-coupon-wrap .two {
   width: 478rpx;
   margin: 0 auto;
   /* height: 327rpx; */
   display: flex;
   justify-content: space-between;
}
.pro-and-coupon-wrap  .two-item {
    width: 218rpx;
    /* height: 315rpx; */
    background: #FFFFFF;
    border: 6rpx solid #FFCD92;
    box-shadow: 0px 4rpx 8rpx 0px #D50303;
    border-radius: 20rpx;
    padding-bottom: 12rpx;
    position: relative;
}
.two .sku-img {
    width: 160rpx;
    height: 160rpx;
