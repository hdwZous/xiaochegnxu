.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.flex {
    display: -webkit-flex;
    display: flex;
}

.flex_row {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
}

.just_content_center {
    justify-content: center;
    -webkit-justify-content: center;
}

.text_center {
    text-align: center;
}

.flex_col {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
}

.flex_none {
    -webkit-flex: none;
    flex: none;
}

.flex_auto {
    -webkit-flex: auto;
    flex: auto;
}

.align_item_center {
    -webkit-align-items: center;
    align-items: center;
}

.search_box {
    height: 100%;
    color: white;
    position: relative;
}

.search_opacity {
    border-radius: 60rpx;
    background-color: #f6f6f6;
    position: absolute;
    left: 0;
    top: 0;
    z-index: -1;
    width: 100%;
    height: 100%;
}

.input_box {
    border-radius: 60rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    align-items: center;
    -webkit-align-items: center;
    background-color: rgb(244, 244, 244);
}

.icon_search {
    width: 28rpx;
    height: 28rpx;
    background: url("https://s.360buyimg.com/wximg/common/icon_search.png") center no-repeat;
    background-size: cover;
    margin-right: 10rpx;
}

.place_holder {
    font-size: 14px;
    font-weight: 400;
    color: #999;
    line-height: 60rpx;
    height: 60rpx;
    overflow: hidden;
}

.search-btn {
    width: 68rpx;
    font-size: 28rpx;
    color: #333;
    margin-left: 10rpx;
}

.title {
    font-size: 18px;
    text-align: center;
    color: #333;
    padding-left: 6vh;
    width: 100%;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.69);
    z-index: 2;
}