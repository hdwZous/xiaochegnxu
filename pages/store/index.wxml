<block wx:if="{{showLocDefault < 0}}">
  <location-default locationError="{{locationError}}"
                    pageId="{{recommendObj.pageIdFirstPage}}" currentPageName="{{recommendObj.currentPageName}}" prePageName="{{recommendObj.prePageName}}"
                    buriedObj="{{recommendObj}}"
                    fromPosition="store"
                    optionsPos="{{options}}">
  </location-default>
</block>
<!-- loading图 -->
<block wx:elif="{{showLocDefault == 0}}">
  <new-default type="{{type}}" tips="{{tips}}" btnText="{{btnText}}" bind:defaultBtnEvent="onDefaultBtnEvent"  fromPosition="store"
  optionsPos="{{options}}"/>
</block>
<view wx:else class="wrap container">
  <block wx:if="{{!showDefault}}">
    <!-- 新版门店 -->
    <store-v5 refreshStoreHeadCoupons="{{refreshStoreHeadCoupons}}" 
                orgCode="{{orgCode}}" 
                couponId="{{couponId}}" 
                isGain="{{isGain}}" 
                storeId="{{storeId}}" 
                skuId="{{skuId}}" 
                activityId="{{activityId}}" 
                promotionType="{{promotionType}}" 
                longitude="{{longitude}}" 
                latitude="{{latitude}}" 
                businessAction="{{businessAction}}" 
                isShowGoodsDetail="{{isShowGoodsDetail}}" 
                showCart="{{showCart}}" 
                isAddCart="{{isAddCart}}" 
                upDateGoodsNum="{{upDateGoodsNum}}" 
                source="{{source}}" 
                cateName="{{cateName}}" 
                channelBusiness="{{channelBusiness}}" 
                channelId="{{channelId}}" 
                isFollow="{{isFollow}}" 
                keyword="{{keyword}}"
                keyWord="{{keyWord}}" 
                autoShowCouponPop='{{autoShowCouponPop}}' 
                bind:onUpdateShareInfo="onUpdateShareInfo" 
                bind:onPagePerformance="onPagePerformance" 
                popup="{{popup}}" 
                outPromotion="{{outPromotion}}" 
                isNeedReFresh="{{isNeedReFresh}}" 
                strategyCode="{{strategyCode}}" 
                popupCoupon="{{popupCoupon}}" 
                spreadChannel="{{spreadChannel}}"
                asyncRefresh="{{asyncRefresh}}"
                buriedObj="{{recommendObj}}"
                id="store"
                needAnchorSku="{{needAnchorSku}}"
                options="{{options}}"
                miniCartMustOrder="{{miniCartMustOrder}}"
                pageIsGetInfoOnShow="{{pageIsGetInfoOnShow}}"
                bind:onStoreCouponPopAb="onStoreCouponPopAb"
                bind:getMiniCartMustOrder="getMiniCartMustOrder"
                bind:clearMustOrder="clearMustOrder">
    </store-v5>
    <!-- 返回app -->
    <back-to-app />
  </block>
  <new-default wx:else type="{{type}}" tips="{{tips}}" btnText="{{btnText}}" bind:defaultBtnEvent="onDefaultBtnEvent" fromPosition="store"
  optionsPos="{{options}}" />
</view>