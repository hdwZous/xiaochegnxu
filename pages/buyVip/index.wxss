
page {
    background: #f5f5f5;
}

.top {
    text-align: center;
    display: flex;
    display: -webkit-flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    padding-top: 60rpx;
    padding-bottom: 50rpx;
    background-color: white;
}

.top image {
    display: block;
    width: 150rpx;
    height: 150rpx;
    margin-left: 30rpx;
}

.pay_success {
    display: flex;
    display: -webkit-flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    align-items: flex-start;
    -webkit-align-items: flex-start;
    margin-left: 30rpx;
}

.pay_success .statusTxt {
    color: #333333;
    font-size: 26rpx;
}

.pay_success .desc {
    color: #666666;
    font-size: 24rpx;
    margin-top: 30rpx;
    text-align: left;
}

.see_vip {
    color: #555555;
    font-size: 26rpx;
    height: 90rpx;
    background-color: white;
    margin-top: 20rpx;
    display: flex;
    display: -webkit-flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    justify-content: space-between;
    -webkit-justify-content: space-between;
}

.see_vip .desc {
    line-height: 90rpx;
    margin-left: 30rpx;
}

.see_vip .arrow {
    width: 7px;
    height: 7px;
    border-top: 2px solid #c3c3c3;
    border-right: 2px solid #c3c3c3;
    transform: rotate(45deg);
    margin: auto 20rpx auto 0;
}



