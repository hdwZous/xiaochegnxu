<block wx:if="{{$taroCompReady}}"><view><block><block wx:if="{{data && data.type === 0 && exchangeCardFlag === 1}}"><view class="{{style.pop}}" catchtap="onClickClosePop"><view class="{{[style.content, style.pop_exchange]}}" catchtap="onClosePopContent"><view class="{{style.time}}">{{countDown}}后鸡蛋消失</view><view class="{{style.buttons + ' flex-row'}}"><view class="{{style.item + ' flex-auto'}}" catchtap="onClickLeftBtn"></view><view class="{{style.item + ' flex-auto'}}" catchtap="onClickRightBtn"></view></view><view class="{{style.close}}" catchtap="onUseCardClosePop"></view></view></view></block><block wx:elif="{{data && data.type === 1}}"><view class="{{style.pop}}" catchtap="onClickClosePop"><view class="{{[style.content, style.pop_success]}}" catchtap="onClosePopContent"><view class="{{style.avatar_case}}"><view class="{{style.avatar}}"><image src="{{data.nickHeadUrl}}" class="{{style.avatar_img}}"></image></view><view class="{{style.name}}">{{data.nickName}}帮助了你</view></view><view class="{{[style.pop_success_btn, 'flex-auto']}}" catchtap="onClickClosePop"></view><view class="{{style.close}}" catchtap="onClickClosePop"></view></view></view></block><block wx:elif="{{data && data.type === 2 && exchangeCardFlag === 1}}"><view class="{{style.pop}}" catchtap="onClickClosePop"><view class="{{[style.content, style.pop_invite]}}" catchtap="onClosePopContent"><view class="{{style.title}}">{{data.title}}</view><view class="{{style.sub_title}}">邀请<text class="{{style.red}}">1</text>个好友首次游戏<text class="{{style.red}}">自动补领</text></view><view class="{{style.invite_time}}">{{countDown}}后鸡蛋消失</view><button class="{{[style.pop_invite_btn]}}" open-type="share" data-params="{{data}}" catchtap="onInviteFriend"></button><view class="{{style.close}}" catchtap="onInviteFriendClosePop"></view></view></view></block></block></view></block>
