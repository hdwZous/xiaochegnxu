<block wx:if="{{$taroCompReady}}"><view class="{{[style.wrap, isPhoneX ? style.isPhoneX : '']}}"><block><block wx:if="{{isError}}"><template is="renderError" data="{{...anonymousState__temp}}"></template></block><block wx:else><template is="renderResult" data="{{...anonymousState__temp2}}"></template></block></block><template is="renderBtn" data="{{...anonymousState__temp3}}"></template><block wx:if="{{!isError}}"><template is="renderBanner" data="{{...anonymousState__temp4}}"></template></block><block wx:if="{{!isError}}"><template is="renderGoodsList" data="{{...anonymousState__temp5}}"></template></block><block wx:if="{{lastPage}}"><view class="{{[style.center, style.font_gray, style.bg_gray]}}">我们是有底线的~</view></block></view></block>
<template name="renderError"><block><view class="{{style.center}}"><image class="{{style.noDataImg}}" src="https://storage.360buyimg.com/wximg/common/errorbar_icon_nothing.png"></image><view class="{{style.noData}}">支付异常，请稍后重试~</view></view></block></template>
<template name="renderResult"><block><view class="{{style.resultBox}}"><view class="{{style.center}}"><image class="{{style.successImg}}" src="https://storage.360buyimg.com/wximg/order/paySuccess.png"></image></view><view class="{{style.successTxt}}">{{orderDetails.orderStatusDesc || ''}}</view><view class="{{style.flex_2}}"><view class="{{[style.flex_item, style.txt_right]}}">支付金额：</view><view class="{{style.flex_item}}">￥{{orderDetails.orderAmount || ''}}</view></view><view class="{{style.flex_2}}"><view class="{{[style.flex_item, style.txt_right]}}">充值号码：</view><view class="{{style.flex_item}}">{{orderDetails.rechargeMobile || ''}}</view></view><view class="{{style.flex_2}}"><view class="{{[style.flex_item, style.txt_right]}}">充值面额：</view><view class="{{style.flex_item}}">￥{{orderDetails.faceValue || ''}}</view></view></view></block></template>
<template name="renderBtn"><block><view class="{{style.btn_box}}"><view class="{{style.btn}}" bindtap="onClickBtn" data-e-tap-so="this" data-e-tap-a-a="chargeCenter">返回充值中心</view><view class="{{style.btn}}" bindtap="onClickBtn" data-e-tap-so="this" data-e-tap-a-a="home">逛逛首页</view></view></block></template>
<template name="renderBanner"><block><block wx:if="{{firstGridData}}"><view class="{{style.bannerBox}}"><grid compid="{{$compid__133}}"></grid></view></block></block></template>
<template name="renderGoodsList"><block><block wx:if="{{goodsList.length}}"><view class="{{style.bg_gray}}"><view class="{{style.title}}">你可能还喜欢</view><view class="{{style.goodsBox}}"><view bindtap="goToStore" wx:key="$loopState__temp7" class="{{style.goodsItem}}" wx:for="{{loopArray14}}" wx:for-item="item" wx:for-index="i" data-e-tap-so="this" data-e-tap-a-a="{{item.$original}}"><view class="{{style.logo}}"><block wx:if="{{item.$original.orgLogo}}"><image class="{{style.logo_img}}" src="{{item.$original.orgLogo}}"></image></block></view><view class="{{style.center}}"><image class="{{style.goodsImg}}" src="{{item.$original.imageUrl || ''}}"></image></view><block wx:if="{{item.$original.tagUrl}}"><image class="{{style.labelImg}}" src="{{item.$original.tagUrl}}"></image></block><view class="{{style.goods_title}}">{{item.$original.productName || ''}}</view><view class="{{[style.new_price, item.$original.orgTagUrl ? style.txt_blue : '', item.$original.basicPrice ? '' : style.martop10]}}">¥{{item.$original.realPrice}}<block wx:if="{{item.$original.orgTagUrl}}"><image class="{{style.shan_mu_img}}" src="{{item.$original.orgTagUrl}}"></image></block></view><view class="{{[style.old_price]}}">{{item.$original.basicPrice ? '¥' + item.$original.basicPrice : ''}}</view></view></view></view></block></block></template>
