.wrap:after {
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: .5px;
  background: #f5f5f5;
  width: 200%;
  -webkit-transform-origin: left top;
  transform-origin: left top;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
}

.image {
  -webkit-flex: none;
  flex: none;
  position: relative;
  width: 180rpx;
  height: 180rpx;
  margin-right: 16rpx;
  background: #f4f4f4;
}

.img {
  border-radius: 8rpx;
  vertical-align: mathematical;
}

.freeze-wrap {
  position: absolute;
  left: 4rpx;
  top: 4rpx;
}

.tag {
  position: relative;
  display: inline-block;
  margin-top: 8rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  font-size: 22rpx;
  padding: 2rpx 8rpx;
  margin-left: 1rpx;
  color: #FF1E1A;
  border: 1rpx solid #FF1E19;
  border-radius: 6rpx;
  padding: 2rpx 8rpx;
}

.info {
  flex: 1;
  position: relative;
  overflow: hidden;
  font-size: 0;
  padding-left: 2rpx;
}

.name {
  color: #333333;
  font-size: 28rpx;
  line-height: 40rpx;
  overflow: hidden;
  font-weight: 500;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  box-orient: vertical;
}

.name-tag {
  display: inline-block;
  font-size: 20rpx;
  height: 28rpx;
  line-height: 28rpx;
  padding: 0 8rpx;
  white-space: nowrap;
  border-radius: 4rpx;
  position: relative;
  top: -4rpx;
}

.description {
  color: #666;
  font-size: 24rpx;
  line-height: 30rpx;
  margin-top: 8rpx;
}

.location {
  margin-top: 10rpx;
}

.icon_home {
  margin-right: 10rpx;
  width: 20rpx;
  height: 20rpx;
  background: url("data:image/png;base64,") no-repeat center;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}

.text {
  color: #999999;
  font-size: 22rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.sub-select {
  margin-right: 10rpx;
}

.price-wrap {
  flex: 1;
  align-items: center;
}

.dd-one-price-group {
  display: flex;
  align-items: center;
  margin-top: 12rpx;
  margin-bottom: 12rpx;
  flex: 1;
}

.btn {
  width: 144rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  color: #FFFFFF;
  font-size: 24rpx;
  font-weight: bold;
  background: linear-gradient(270deg, #FF2A26 0%, #FF6461 100%);
  border-radius: 28rpx;
}

.dd-three-sub-select {
  /* margin-right: 12rpx; */
}

.dd-three-sub-select .go-buy {
  background: linear-gradient(270deg, #FF2A26 0%, #FF6461 100%);
  border-radius: 28rpx;
  font-size: 24rpx;
  padding: 12rpx 28rpx;
  color: #FFFFFF;
  text-align: center;
  width: 152rpx;
}
.dd-warp-flex {
  display: flex;
  padding: 20rpx 0;
  height: 188rpx;
  position: relative;
  border-bottom: 1px solid #F5F5F5;
}
.dd-one-store-logo {
  margin-right: 16rpx;
  border: 1rpx solid #e9e9e9;
  height: 56rpx;
  border-radius: 4rpx;
}
.dd-one-store-logo image {
  width: 56rpx;
  height: 56rpx;
}
.dd-one-store-name {
  font-size: 22rpx;
  color: #333333;
  margin-bottom: 2rpx;
  font-weight: 500;
}
.dd-one-store-left {
  flex: 1;
}
.dd-one-store-right {
    display: flex;
    justify-content: center;
    flex-direction: column;
}
.dd-one-store-time {
  font-size: 22rpx;
  color: #666666;
  text-align: right;
}
.dd-one-store {
  padding: 20rpx 0 18rpx 0;
  padding-left: 0;
}
.dd-one-store-distance {
  font-size: 22rpx;
  color: #333333;
  align-items: center;
}
.dd-one-store-distance-time {
  font-size: 22rpx;
  color: #666666;
}
.dd-one-store-distance-number {
    color: #666666;
}
.dd-one-store-line {
  width: 2rpx;
  height: 16rpx;
  background-color: #E1E1E1;
  margin-left: 8rpx;
  margin-right: 8rpx;
}
.dd-good-row-one {
  /* height: 320rpx; */
  /* width: 700rpx; */
  border-radius: 24rpx;
  background-color: #FFFFFF;
  padding-left: 20rpx;
  padding-right: 12rpx;
  margin: 0 auto;
  margin-bottom: 26rpx;
}
.dj_tag {
  position: absolute;
  top: 0;
  left: 0;
}
.dd-one-store-rank text {
  font-size: 22rpx;
  color: #666666;
}