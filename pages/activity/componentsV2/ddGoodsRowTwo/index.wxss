.row_two {
  margin: 0 20rpx;
  background: #FFFFFF;
  box-sizing: border-box;
  padding-top: 20rpx;
  margin-top: -1rpx;
}
.row_two .wrap {
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
  margin: 0 15rpx;
  box-sizing: border-box;
}
.row_two .item {
  /* margin-bottom: 20rpx; */
  width: 50%;
  padding: 0 6rpx;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.row_two .door {
  margin: 0 20rpx;
  color: #333333;
  font-size: 28rpx;
  text-align: center;
  padding: 30rpx 0;
  position: relative;
}
.row_two .door:before {
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  height: 1px;
  background: #E4E4E4;
  width: 200%;
  -webkit-transform-origin: left top;
  transform-origin: left top;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
}
.row_two .door:after {
  position: relative;
  top: -1rpx;
  margin-left: 5rpx;
  display: inline-block;
  content: "";
  width: 14rpx;
  height: 14rpx;
  border-right: 1rpx solid #333333;
  border-bottom: 1rpx solid #333333;
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
}