.items {
  border-radius: 12rpx;
  background: #FAFAFA;
  height: 100%;
  margin-right: 24rpx;
  text-align: center;
}

.items .store-back {
  font-size: 0;
  position: relative;
}

.items .store-back .background {
  width: 204rpx;
  height: 136rpx;
}

.items .store-back .logo-box {
  width: 84rpx;
  height: 84rpx;
  border: 1px solid #f6f6f6;
  background: #fff;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  bottom: 0;
  transform: translate(-50%, 50%);
}

.items .store-back .logo {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
}

.items .store-name {
  max-width: 172rpx;
  margin: 0 auto;
  margin-top: 70rpx;
  margin-bottom: 8rpx;
  height: 34rpx;
  line-height: 34rpx;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: bold;
  color: #333333;
}

.closeTip {
  display: inline-block;
  max-width: 148rpx;
  height: 32rpx;
  line-height: 32rpx;
  box-sizing: border-box;
  padding: 0 8rpx;
  border-radius: 4rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: rgb(36, 134, 255);
  background-color: rgba(36, 134, 255, 0.1)
}

.rating-box .star_number {
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FF9415;
  margin-left: 4rpx;
}

.rating-box .star_txt {
  color: rgb(153, 153, 153);
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
}

.coupon {
display: inline-block;
padding: 0 8rpx;
font-size: 20rpx;
font-family: PingFangSC-Regular, PingFang SC;
font-weight: 400;
position: relative;
}

.coupon .border {
position: absolute;
width: 200%;
height: 200%;
left: 50%;
top: 50%;
border-width: 1px;
border-style: solid;
border-radius: 8rpx;
transform-origin: center;
-webkit-transform-origin: center;
transform: translate(-50%, -50%) scale(0.5);
-webkit-transform: translate(-50%, -50%) scale(0.5);
}
.expand-image {
  min-width: 32rpx;
  height: 32rpx;
  background-image: url(https://storage.360buyimg.com/wxmini/newuser/pengzhangicon.png);
  background-repeat: no-repeat;
  background-size: 100% auto;
  margin-right: 6rpx;
}
.expand {
  display: inline-flex;
  height: 32rpx;
  background: linear-gradient(142deg, #FF6963 0%, #FF4141 100%);
  border-radius: 4rpx;
  margin: 0 auto;
  align-items: center;
  margin-top: 12rpx;
  width: 182rpx;
  padding-right: 2rpx;
}
.expand-text {
  font-size: 22rpx;
  color: #fff;
  margin-right: 2rpx;
  width: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.expand-price {
  font-size: 24rpx;
  color: #fff;
  font-family: regular;
  line-height: 29rpx;
}

.dj_tag {
  position: absolute;
  top: 0;
  left: 0;
}