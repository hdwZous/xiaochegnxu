.wrap{
    padding: 40rpx 0;
    display: flex;
    height: 310rpx;
    justify-content: center;
    align-items: center;
}
.box{
    width:710rpx;
    height: 310rpx;
    /* background-color: #afa; */
    border-radius: 12rpx;
    position: relative;
}
.bg-img{
    position: absolute;
    width:710rpx;
    height: 310rpx;
    left: 0;
    top: 0;
}
.btn-box{
    position: absolute;
    width: 144rpx;
    height: 52rpx;
    right: 36rpx;
    top: 28rpx;
}
.btn-img{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}
.list-box{
    position: absolute;
    height: 78rpx;
    width: 100%;
    padding: 0 44rpx;
    top: 166rpx;
    left: 0;
    box-sizing: border-box;
}
.lists{
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
}
.lists::after{
    content: '';
    width: 95%;
    height: 4rpx;
    position: absolute;
    left: 10rpx;
    top: 20rpx;
    background: #F5F5F5;
    z-index: 5;
}
.list_item{
   height: 100%;
   display: flex;
   flex-direction: column;
   align-items: center;
   width: 60rpx;
   justify-content: space-between;
   z-index: 10;
   position: relative;
}
.item_img{
    width: 44rpx;
    height:  44rpx;
}
.text{
    font-size: 20rpx;
    font-weight: 400;
    color: #666666;
    line-height: 26rpx;
}
.toast{
    position: absolute;
    width: 80rpx;
    height: 40rpx;
    top: -50rpx;
    font-size: 10px;
    line-height: 40rpx;
    text-align: center;
}
.toast_img{
    width: 80rpx;
    height: 40rpx;
}
.toast_text{
    position: absolute;
    width: 80rpx;
    height: 26rpx;
    line-height: 26rpx;
    top: 4rpx;
    color: #fff;
}
.time-text{
    position: absolute;
    left: 36rpx;
    bottom: 20rpx;
    font-size: 20rpx;
    color: #666666;
    line-height: 13px;
}
.rule{
    position: absolute;
    width: 66rpx;
    height: 26rpx;
    right: 36rpx;
    bottom: 20rpx;
    font-size: 20rpx;
    color: #666666;
    line-height: 26rpx;
}
.icon{
    position: absolute;
    width: 18rpx;
    height: 18rpx;
    right: 0rpx;
    bottom: 6rpx;
}
.placeholder-img{
    width: 100%;
    height: 100%;
}

.mask{
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    z-index: 9999;
}
.sign-mask.success{
    background:rgba(0,0,0,0.7);
}

.content{
    width: 594rpx;
    height: 502rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.success_text{
    font-size: 16px;
    font-weight: 400;
    color: #FFFFFF;
    line-height: 44rpx;
    position: absolute;
    top: 116rpx;
    left: 50%;
    transform: translate(-50%,0);
}
.success_btn{
    width: 392rpx;
    height: 72rpx;
    position: absolute;
    bottom: 40rpx;
    left: 50%;
    transform: translate(-50%,0);
}
.img_box{
    position: absolute;
    top: 180rpx;
    left: 50%;
    transform: translate(-50%,0);
    /* background-color: #afa; */
}
.img_box.bean{
    width: 280rpx;
    height: 180rpx;
}
.img_box.ticket{
    width: 530rpx;
    height: 162rpx;
}
.ticket_left{
    float: left;
    width: 354rpx;
    height: 162rpx;
    padding: 20rpx 30rpx;
    background: #FFFFFF;
    border-radius: 4rpx 12rpx 12rpx 4rpx;
    box-sizing: border-box;
    position: relative;
}
.ticket_left_title{
    font-size: 32rpx;
    font-weight: 500;
    color: #333333;
    line-height: 40rpx;
    margin-bottom: 6rpx;
}
.ticket_left_desc{
    font-size: 24rpx;
    font-weight: 300;
    color: #333333;
    line-height: 30rpx;
    margin-bottom: 18rpx;
}
.ticket_left_date{
    font-size: 22rpx;
    font-weight: 300;
    color: #666666;
    line-height: 30rpx;
    margin-bottom: 18rpx;
    width: 318rpx;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}
.ticket_left_dash{
    width: 2rpx;
    position: absolute;
    right: -1rpx;
    top: 30rpx;
    height: 110rpx;
    border-left: 2rpx dashed #FFE3E3;
}
.ticket_right{
    float: right;
    width: 176rpx;
    height: 162rpx;
    padding: 20rpx 30rpx;
    background: #FFFFFF;
    border-radius:  12rpx 4rpx 4rpx 12rpx ;
    box-sizing: border-box;
    position: relative;
}
.ticket_right_unit{
    position: absolute;
    left: 58rpx;
    top: 44rpx;
    font-size: 28rpx;
    color: #FF5757;
    height: 34rpx;
    line-height: 34rpx;
    font-family: regular;
}
.ticket_right_num{
    position: absolute;
    left: 74rpx;
    top: 4rpx;
    font-size: 68rpx;
    color: #FF5757;
    line-height: 82rpx;
    height: 82rpx;
    font-weight: bold;
    font-family: regular;
}
.ticket_right_img{
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    width: 88rpx;
    height: 88rpx;
    /* background-color: #afa; */
}
.ticket_right_btn{
    position: absolute;
    left: 30rpx;
    top: 92rpx;
    width: 112rpx;
    height: 48rpx;
    background: linear-gradient(315deg, #FF5138 0%, #FF7D7D 100%);
    box-shadow: 0px 2px 4px 0px rgba(255, 88, 67, 0.21);
    border-radius: 24rpx;
    font-size: 24rpx;
    color: #FFFFFF;
    height: 48rpx;
    text-align: center;
    line-height: 48rpx;
}
.fail_content{
    width: 204rpx;
    height: 144rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background:rgba(0,0,0,0.7);
    border-radius: 8rpx;
}
.icon.fail{
    top: 24rpx;
    left: 50%;
    transform: translate(-50%,0);
    width: 48rpx;
    height: 48rpx;
}
.fail-img{
    width: 48rpx;
    height: 48rpx;
}
.failtext{
    width: 156rpx;
    font-size: 14px;
    font-weight: 400;
    color: #FFFFFF;
    line-height: 36rpx;
    position: absolute;
    top: 88rpx;
    left: 50%;
    transform: translate(-50%,0);
    text-align: center;
}
.rule-mask{
    background:rgba(0,0,0,0.7);
}
.rule_content{
    width: 100%;
    height: 80%;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    background: #FFFFFF;
    border-radius: 12px 12px 0px 0px;
}
.rule_header{
    position: absolute;
    height: 100rpx;
    text-align: center;
    font-size: 32rpx;
    font-weight: 500;
    color: #333333;
    line-height: 100rpx;
    width: 100%;
    border-bottom: 1rpx solid #F4F4F4;
}
.rule-tips{
    padding: 36rpx 36rpx 48rpx;
    margin-top: 108rpx;
    height: 90%;
    box-sizing: border-box;
}
.rule_close{
    position: absolute;
    width: 50rpx;
    height: 50rpx;
    right: 24rpx;
    top: 24rpx;
    /* background-color: #afa; */
}
.rule_close_img{
    position: absolute;
    left: 0;
    top: 0;
}
.rule_text{
    font-size: 14px;
    font-weight: 300;
    color: #000000;
    line-height: 20px;
}