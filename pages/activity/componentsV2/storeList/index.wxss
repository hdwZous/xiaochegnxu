.store_case {
    padding-top: 30rpx;
}
.store_left {
    padding: 0 16rpx 0 20rpx;
}
.store_logo {
    position: relative;
    width: 120rpx;
    height: 120rpx;
    border-radius: 4px;
    border: 1px solid rgba(0, 0, 0, 0.08);
}
.store_img {
    width: 100%;
    height: 100%;
    border-radius: 4px;
}
.store_num {
    position: absolute;
    right: -15rpx;
    top: -15rpx;
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    color: #ffffff;
    font-size: 20rpx;
    font-weight: 500;
    border-radius: 50%;
    background: rgba(255, 87, 87, 1);
}
.store_num_more {
    padding: 0 4rpx;
    min-width: 38rpx;
    border-radius: 18rpx;
    position: absolute;
    right: -15rpx;
    top: -15rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    color: #ffffff;
    font-size: 20rpx;
    font-weight: 500;
    background: rgba(255, 87, 87, 1);
}
.rest_tag {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    text-align: center;
    color: #ffffff;
    font-size: 20rpx;
    background: rgba(204, 204, 204, 1);
    border-radius: 0 0 8rpx 8rpx;
}
.store_right {
    padding-bottom: 30rpx;
    margin-right: 20rpx;
    overflow: hidden;
    position: relative;
    /* border-bottom: 1px solid #f5f5f5; */
}
.store_name_case {
    align-items: center;
}
.store_name {
    color: #333333ff;
    font-size: 32rpx;
    font-weight: bold;
    overflow: hidden;
}
.store_badge {
    width: 40rpx;
    height: 40rpx;
}
.store_book_case {
    margin-top: 6rpx;
    display: inline-block;
}
.store_book {
    border-radius: 4rpx;
}
.store_book_left {
    padding: 0 8rpx;
    color: #ffffff;
    font-size: 24rpx;
}
.store_book_right {
    padding: 0 8rpx;
    font-size: 24rpx;
}
.line_one {
    margin-top: 8rpx;
    align-items: center;
}
.line_one_left {
    align-items: center;
}
.star_txt {
    color: #999999;
    font-size: 20rpx;
}
.star_number {
    margin-left: 4rpx;
    color: #ff9415ff;
    font-size: 22rpx;
    font-weight: bold;
}
.sale_number {
    margin-left: 16rpx;
    color: #666666;
    font-size: 22rpx;
}
.line_one_right {
    text-align: right;
    color: #666666;
    font-size: 22rpx;
}
.split {
    margin: 0 11rpx;
    color: #666666;
}
.line_two {
    align-items: center;
    margin-top: 12rpx;
}
.line_two_left {
    color: #666666;
    font-size: 22rpx;
}
.line_two_right {
    font-size: 20rpx;
    display: flex;
    align-items: center;
    color: #666666;
}
.line_two_right_tag {
    width: 88rpx;
    height: 28rpx;
    color: #666666;
    text-align: center;
}
.line_three {
    flex-wrap: wrap;
    margin-top: 20rpx;
    padding-right: 60rpx;
    position: relative;
    overflow: hidden;
    align-items: flex-start;
}
.line_three_icon {
    position: absolute;
    right: 0;
    top: 0;
    width: 60rpx;
    font-size: 14rpx;
    color: #666666;
    text-align: right;
    height: 32rpx;
    line-height: 32rpx;
}
.coupon_tag_case {
    overflow: hidden;
    margin-right: 10rpx;
    margin-bottom: 10rpx;
}
.coupon_tag {
    height: 30rpx;
    line-height: 30rpx;
    padding: 0 10rpx;
    position: relative;
    border-radius: 4rpx;
    font-size: 20rpx;
}
.left_radio {
    position: absolute;
    width: 10rpx;
    height: 10rpx;
    left: -8rpx;
    top: 50%;
    transform: translateY(-50%);
    border-radius: 50%;
    background: #ffffff;
}
.right_radio {
    position: absolute;
    width: 10rpx;
    height: 10rpx;
    right: -8rpx;
    top: 50%;
    transform: translateY(-50%);
    border-radius: 50%;
    background: #ffffff;
}
.line_four {
    padding-right: 60rpx;
    position: relative;
    margin-top: 10rpx;
}
.promotion_tag {
    align-items: center;
}
.line_four_tag {
    min-width: 52rpx;
    text-align: center;
    margin-right: 10rpx;
    color: #ffffff;
    font-size: 20rpx;
    border-radius: 8rpx 2rpx 8rpx 2rpx;
}
.line_four_txt {
    color: #333333ff;
    font-size: 22rpx;
}
.line_five {
    padding-right: 60rpx;
    position: relative;
    margin-top: 20rpx;
}
.line_five_head {
    align-items: center;
}
.line_five_icon {
    color: #666666;
    font-size: 24rpx;
}
.line_five_title {
    margin-left: 10rpx;
    color: rgb(51, 51, 51);
    font-size: 22rpx;
}
.nearby_store {
    padding: 16rpx 0;
    border-top: 1px solid rgb(245, 245, 245);
}
.nearby_store_name {
    color: rgb(51, 51, 51);
    font-size: 28rpx;
}
.nearby_store_distance {
    color: rgb(102, 102, 102);
    font-size: 22rpx;
}

.expand-image {
    min-width: 32rpx;
    height: 32rpx;
    background-image: url(https://storage.360buyimg.com/wxmini/newuser/pengzhangicon.png);
    background-repeat: no-repeat;
    background-size: 100% auto;
    margin-right: 8rpx;
}
.expand {
    display: inline-flex;
    height: 32rpx;
    background: linear-gradient(142deg, #FF6963 0%, #FF4141 100%);
    border-radius: 4rpx;
    margin: 0 auto;
    align-items: center;
    margin-top: 12rpx;
    min-width: 232rpx;
    padding-right: 2rpx;
}
.expand-text {
    font-size: 24rpx;
    color: #fff;
    margin-right: 2rpx;
}
.expand-price {
    font-size: 32rpx;
    color: #fff;
    font-family: regular;
    line-height: 29rpx;
}
.expand-left {
    margin: 0;
    margin-top: 12rpx;
}
.store_biz_tag {
    position: absolute;
    top: 0;
    right: 0;
}