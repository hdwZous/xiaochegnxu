<block wx:if="{{$taroCompReady}}"><view class="{{style.container}}"><view class="{{style.head}}"><view>可提现(元）<text class="{{style.txt}}">单次最高可提100元</text></view><view class="{{[style.amount, 'flex-row']}}"><view class="{{style.money}}">{{leftMoney / 100}}</view><view class="{{[style.btn, leftMoney <= 0 || todayWithdrawTimesOut ? style.gray : '']}}" bindtap="anonymousFunc0">提现</view></view><block wx:if="{{totalMoney}}"><view>累计奖励：{{totalMoney / 100}}元</view></block></view><view class="{{style.withdrawList}}"><view class="{{[style.title, 'flex-row']}}"><text class="{{style.line}}"></text><text>提现明细</text></view><block wx:if="{{recordList}}"><view class="{{style.listItem}}" wx:key="index" wx:for="{{recordList}}" wx:for-item="item" wx:for-index="index"><view class="{{[style.cont, 'flex-row']}}"><view class="{{style.left}}"><view class="{{style.amount}}">{{item.money / 100}}元</view><view class="{{style.date}}">{{item.operTime}}</view></view><image class="{{style.flag}}" src="{{flag[item.withDrawStatus + 1]}}"></image></view><block wx:if="{{item.failReason}}"><view class="{{style.failReason}}">{{item.failReason}}</view></block></view></block><block><block wx:if="{{showLoading}}"><view class="{{style.loading}}">加载中…</view></block><block wx:else><view class="{{style.loading}}">到底了</view></block></block></view><block wx:if="{{showAuthPop}}"><auth-pop compid="{{$compid__30}}"></auth-pop></block></view></block>
