.animation-modal {
    position: fixed;
    top: 0;
    left: 0;
    height: 100vh;
    width: 100vw;
    display: flex;
    align-items: center;
    justify-content: center;
}

.black-bg {
    background-color: #000;
}

.transparent-bg {
    background-color: transparent;
}

.red-bg {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 962rpx;
    width: 604rpx;
    position: relative;
}

.red-bg-top {
    width: 585rpx;
    height: 700rpx;
    background: url("https://storage.360buyimg.com/wxmini/cash/redpackget_top.png") center no-repeat;
    background-size: cover;
    position: absolute;
    top: 0;
    z-index: 2;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.red-bg-top-animation {
    transition: all 1.5s;
    transform: translateY(-1400rpx) scale(2);
    opacity: 0;
}

.red-open {
    width: 170rpx;
    height: 170rpx;
    border-radius: 50%;
    background: url("https://storage.360buyimg.com/wxmini/cash/open_red_button.png") center no-repeat;
    background-size: cover;
    position: absolute;
    bottom: 180rpx;
    z-index: 3;
    box-shadow: 0 0 20rpx 0 rgba(101, 7, 6, 0.31);
    animation: breath 0.5s ease-in-out infinite;
}

.show {
    display: inline-block;
    width: 170rpx;
    height: 170rpx;
    background: url('data:image/png;base64,') center no-repeat;
    background-size: cover;
    animation: run 0.5s infinite steps(1, start);
}

.red-xuanzhuan {
    display: inline-block;
    width: 170rpx;
    height: 170rpx;
    position: absolute;
    bottom: 180rpx;
    z-index: 3;
    background: url('data:image/png;base64,') center no-repeat;
    background-size: 170rpx 170rpx;
}

@keyframes run {
    0% {
        background: url('data:image/png;base64,') center no-repeat;
        background-size: 170rpx 170rpx;
    }

    17% {
        background: url('data:image/png;base64,') center no-repeat;
        background-size: 170rpx 170rpx;

    }

    34% {
        background: url('data:image/png;base64,') center no-repeat;
        background-size: 170rpx 170rpx;

    }

    51% {
        background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAIsAAACLAQMAAABr1/i8AAAABlBMVEUAAADrzZsZWWVBAAAAAXRSTlMAQObYZgAAACJJREFUSMftyDEBAAAIAyCbWd1oNtgKwMkEe0oppZRSSpV6BBaJdwGEto4AAAAASUVORK5CYII=') center no-repeat;
        background-size: 170rpx 170rpx;

    }

    68% {
        background: url('data:image/png;base64,') center no-repeat;
        background-size: 170rpx 170rpx;

    }

    85% {
        background: url('data:image/png;base64,') center no-repeat;
        background-size: 170rpx 170rpx;
    }

    100% {
        background: url('data:image/png;base64,') center no-repeat;
        background-size: 170rpx 170rpx;
    }
}

.hide {
    display: none;
}

.red-bg-bottom {
    width: 585rpx;
    height: 355rpx;
    background: url("https://storage.360buyimg.com/wxmini/cash/redpackget_bottom.png") center no-repeat;
    background-size: cover;
    position: absolute;
    bottom: 0;
    z-index: 1;
}

.red-bg-bottom-animation {
    transition: all 1.5s;
    transform: translateY(750px) scale(2);
    opacity: 0.3;
}

.money-title {
    height: 47rpx;
    font-size: 50rpx;
    font-family: PingFang SC;
    font-weight: bold;
    color: #fec69f;
    line-height: 48rpx;
    margin-top: 170rpx;
}

.money-tips {
    height: 46rpx;
    font-size: 46rpx;
    font-family: PingFang SC;
    font-weight: bold;
    color: #fec69f;
    line-height: 48rpx;
    margin-top: 44rpx;
}

@keyframes breath {
    0% {
        transform: scale(0.96);
    }

    50% {
        transform: scale(1.05);
    }

    100% {
        transform: scale(0.96);
    }
}

.login-tips {
    font-size: 46rpx;
    font-family: PingFang SC;
    font-weight: bold;
    color: #FEC69F;
    padding-top: 210rpx;
}