<block wx:if="{{$taroCompReady}}"><block><block wx:if="{{showDefault}}"><default compid="{{$compid__220}}"></default></block><block wx:else><template is="renderMain" data="{{...anonymousState__temp}}"></template></block></block></block>
<template name="renderMain"><block><view class="{{[style.grayBg]}}"><view><image mode="widthFix" class="{{style.banner}}" bindtap="toUse" src="{{bannelLogo}}" data-e-tap-so="this"></image><block wx:if="{{buttionId == 1}}"><view class="{{[style.infoBox]}}"><view bindtap="getCouponFn" class="{{[style.btn, style.btn_normal]}}" data-e-tap-so="this">免费领取</view></view></block><block wx:if="{{buttionId == 0 && !gotSuccess}}"><view class="{{[style.infoBox]}}"><block wx:if="{{title}}"><view class="{{[style.description, style.mTop40]}}">{{title}}</view></block><view bindtap="getCouponFn" class="{{[style.btn, style.btn_normal]}}" data-e-tap-so="this">去京东到家逛逛</view></view></block><block wx:if="{{gotSuccess}}"><view class="{{[style.infoBox]}}"><view class="{{style.descriptionBig}}">{{title || ''}}</view><block wx:if="{{phone}}"><view class="{{style.description}}"> 会员已放入您的账户：<text class="{{style.phone}}">{{phone}}</text></view></block></view></block><block wx:if="{{vipLogo}}"><image mode="widthFix" class="{{style.goods}}" src="{{vipLogo}}"></image></block></view></view></block></template>
