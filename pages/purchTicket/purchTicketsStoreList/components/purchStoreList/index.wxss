@import "/common/css/global";
/*门店头*/
.btn_save_formid {
  padding-left: 0 !important;
  padding-right: 0 !important;
  border-radius: 4rpx !important;
  background: transparent !important;
}
.btn_save_formid:after {
  border: 0;
}
.wrap {

}
.item {
    padding: 30rpx 0 15rpx 15rpx;
    position: relative;
}
.logo {
    width: 128rpx;
    height: 128rpx;
    /* background-color: #e4e4e4; */
    border-radius: 10rpx;
    /* border: 1rpx solid #e4e4e4; */
}
.amount {
    position: absolute;
    left: 115rpx;
    top: -15rpx;
    border-radius: 50%;
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    color: white;
    background: #ff5757;
    font-size: 20rpx;
    -webkit-justify-content: center;
    justify-content: center;
}
.flag {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    left: 0rpx;
    top: -1rpx;
}
.info {
    overflow: hidden;
    padding-left: 20rpx;
}
.declaration {
    padding-right: 30rpx;
    justify-content: center;
    align-items: center;
    position: relative;
}
.name {
    width: 100%;
    color: #333;
    font-size: 30rpx;
}
.declaration_img {
    height: 39rpx;
    width: 39rpx;
    margin-left: 5rpx;
    margin-top: 7rpx;
}
.second_line {
    padding-right: 30rpx;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    align-items: center;
    font-size: 20rpx;
    color: #999;
    margin-top: 16rpx;
    position: relative;
}
.star .stars {
    color: #ff9415;
    padding-left: 6rpx;
}
.send_time .split {
    padding: 0 4rpx;
}

.starSale .sale_num {
    padding-left: 10rpx;
}
.rating {
    width: 20rpx;
    height: 20rpx;
    margin-right: 3rpx;
    color: #999;
}
.freight {
    padding-right: 30rpx;
    color: #999;
    font-size: 22rpx;
    margin-top: 18rpx;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    position: relative;
    height: 40rpx;
}
.summary text {
    height: 22rpx;
    line-height: 22rpx;
}
.dash {
    margin-right: 30rpx;
    position: relative;
    overflow: hidden;
}
.dash:before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 1px;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}
.blue {
    color: #20acff;
    font-size: 20rpx;
    border: 1rpx solid #20acff;
    border-radius: 6rpx;
    padding-left: 2rpx;
    padding-right: 2rpx;
}
.grap {
    color: #999;
    font-size: 20rpx;
    border: 1rpx solid #999;
    padding-left: 2rpx;
    padding-right: 2rpx;
    border-radius: 6rpx;
}

.store_item_line {
    position: relative;
    background-color: #f5f5f5;
    height: 1px;
    width: 200%;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}

/*促销标*/
.coupon_wrap {
    padding-right: 30rpx;
    position: relative;
    padding-top: 8rpx;
}

.coupon_item {
    line-height: 44rpx;
    width: 500rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    color: #999;
}

.coupon_icon {
    display: inline-block;
    padding-left: 6rpx;
    padding-right: 6rpx;
    margin-right: 8rpx;
    margin-top: 10rpx;
    border-radius: 8rpx 2rpx 8rpx 2rpx;
    color: #fff;
    height: 28rpx;
    line-height: 28rpx;
    background-color: #fdc44c;
    font-size: 20rpx;
}

.coupon_desc {
    font-size: 22rpx;
    color: #999;
    flex: 1;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;

}

.coupon_arrow_icon {
    width: 10rpx;
    height: 10rpx;
    border: 1rpx solid #b2b2b2;
    border-top-width: 0;
    border-left-width: 0;
    -webkit-transform-origin: center center;
    -webkit-transform: rotate(45deg);
    -webkit-transition: all ease-in 400ms;
    margin-left: 50rpx;
    margin-top: 5rpx;
}
.coupon_arrow_layout{
    width:80rpx;height:80rpx;position: absolute; top: 8rpx; right: -5rpx;
    
}
.arrow-up {
    top: 35rpx;
    -webkit-transform: rotate(225deg);
}
.show {
    display: block;
}
.hide {
    display: none;
}

/*附近商家*/
.nearby_store {
    margin-top: 20rpx;
}
.nearby_store .dash_line {
    margin-right: 30rpx;
    position: relative;
}
.nearby_store .dash_line:before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 1px;
    border-top: 1px dashed #dddddd;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}
.nearby_store .title {
    padding: 30rpx 30rpx 15rpx 0;
    -webkit-align-items: center;
    align-items: center;
}
.nearby_store .icon_store {
    margin-right: 10rpx;
    width: 20rpx;
    height: 20rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}
.nearby_store .text {
    color: #333333;
    font-size: 24rpx;
}
.nearby_store .icon_case {
    position: relative;
    width: 20rpx;
    height: 20rpx;
}
.nearby_store .icon_arrow {
    position: absolute;
    right: 5rpx;
    top: 0;
    width: 14rpx;
    height: 14rpx;
    border: 1rpx solid #b2b2b2;
    border-top-width: 0;
    border-left-width: 0;
    -webkit-transform-origin: center center;
    -webkit-transform: rotate(45deg);
    -webkit-transition: all ease-in 400ms;
}
.nearby_store .icon_arrow_up {
    top: 9rpx;
    -webkit-transform: rotate(225deg);
}
.nearby_store .item {
    padding: 15rpx 30rpx 15rpx 0;
    -webkit-align-items: center;
    align-items: center;
    position: relative;
}
.nearby_store .item:last-child:before {
    display: none;
}
.nearby_store .store_name {
    overflow: hidden;
    font-size: 24rpx;
    color: #333333;
}
.nearby_store .deliver_msg {
    font-size: 20rpx;
    color: #999999;
}
.nearby_store .split {
    margin: 0 10rpx;
}

