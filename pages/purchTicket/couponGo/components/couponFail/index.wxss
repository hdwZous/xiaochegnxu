.no-coupon-wrapper {
  height: 90vh;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.default-pic {
  margin-top: 20vh;
}
.has-coupon-wrapper {
  height: 79vh;
  overflow: scroll;
}
.has-coupon-wrapper .default {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 38rpx;
}

.default-image {
  width: 240rpx;
  height: 240rpx;
}
.default-fail-text {
  margin-top: 16rpx;
  color: #333;
  font-size: 28rpx;
}
.no-coupon-back-btn {
  margin-top: 40rpx;
  width:192rpx;
  height:64rpx;
  background:linear-gradient(to right,#45DC6D,#00CF37);
  border-radius:32rpx;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height: 64rpx;
}
.has-coupon-back-btn {
  margin-top: 40rpx;
  width:710rpx;
  height:80rpx;
  background:linear-gradient(to right,#45DC6D,#00CF37);
  border-radius:40rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 80rpx;
  position: fixed;
  bottom: 50rpx;
  left: 20rpx;
  z-index: 2;
}
.coupon-list {
  margin-top: 48rpx;
}
.coupon-list .coupon-item {
  width:710rpx;
  height:160rpx;
  margin: 0 auto;
  margin-top: 16rpx;
}