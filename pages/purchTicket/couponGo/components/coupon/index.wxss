::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.case {
    overflow: hidden;
    padding: 0 20rpx;
}
.wrap {
    height: 160rpx;
    background:rgba(255,255,255,1);
    border:1px solid rgba(255,87,87,0.2);
    align-items: center;
    border-radius: 4rpx;
}
.left {
    width: 200rpx;
    text-align: center;
}
.price_case {
    display: inline;
}
.symbol {
    color: #FF5757FF;
    font-size: 28rpx;
    font-family: regular;
}
.price {
    color: #FF5757FF;
    font-size: 64rpx;
    line-height: 1em;
    font-family: regular;
}
.low-price {
    color: #FF5757FF;
    font-size: 28rpx;
    line-height: 1em;
    font-family: regular;
}
.sub {
    color: #333333FF;
    font-size: 22rpx;
    font-weight: 300;
}
.middle {
    padding-right: 10rpx;
    height: 120rpx;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    justify-content: space-evenly;
}
.title {
    margin-bottom: 0rpx;
    color: #333333FF;
    font-size: 28rpx;
    font-weight: bold;
}
.mix-tag-wrap {
  display: flex;
  flex-wrap: wrap;
  /* height: 34rpx; */
  height: 38rpx;
  overflow: hidden;
  justify-content: flex-start;
  align-items: center;
}
.mix-tag {
  font-size: 20rpx;
  color: #999;
  font-weight: 400;
  height: 32rpx;
  line-height: 32rpx;
  padding: 0 8rpx;
  border-radius: 8rpx;
  /* display: inline-block; */
  border-width: 1rpx;
  border-style: solid;
  box-sizing: border-box;
  margin-right: 8rpx;
}
.time {
    color: #FF1E19FF;
    font-size: 22rpx;
}
.dashed {
  width: 2rpx;
  height: 127rpx;
  background: url('https://storage.360buyimg.com/wximg/common/dashed2.png') no-repeat;
  background-size: 2rpx 127rpx;
  margin-top: 18rpx;
}
.right {
    position: relative;
    height: 100%;
    /* border-left: 1px dashed #0000001A; */
    width: 170rpx;
}
.right::before {
    content: "";
    position: absolute;
    top: -13rpx;
    left: -15rpx;
    width: 24rpx;
    height: 24rpx;
    border: 1px solid rgba(255,87,87,0.2);
    border-radius: 50%;
    background: #ffffff;
}
.btn {
    position: relative;
    top: 56rpx;
    margin: 0 auto;
    color: #999999FF;
    font-size: 24rpx;
    text-align: center;
    line-height: 48rpx;
    width: 112rpx;
    height: 48rpx;
    border-radius:30rpx;
    border:1px solid rgba(153,153,153,1);
}
.tag {
    width: 88rpx;
    height: 88rpx;
    position: absolute;
    right: 0;
    top: 0;
}
.img {
    width: 100%;
    height: 100%;
}
.cp-yhy-tag {
  padding: 0 8rpx;
  margin-bottom: 4rpx;
  margin-right: 8rpx;
  font-size: 24rpx;
  height: 32rpx;
  font-weight: 400;
  position: relative;
  top: 2rpx;
  left: 2rpx;
  overflow: initial;
  display: flex;
  align-items: center;
}
.cp-yhy-tag-text {
  line-height: 32rpx;
  overflow: hidden;
}
.cp-yhy-tag-border {
  position: absolute;
  width: 200%;
  height: 200%;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%) scale(0.5);
  transform-origin: center;
  border-radius: 16rpx;
}