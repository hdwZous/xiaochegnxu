@-webkit-keyframes moveShow {
  0% {
      transform: translateX(0%);
  }
  16.67% {
    transform: translate(0%);
  }
  49.99% {
    transform: translate(-35%);
  }
  83.32% {
    transform: translate(0%);
  }
  100% {
      transform: translate(0%);
  }
}
@-webkit-keyframes tips {
  0% {
      transform: translateX(0%);
  }
  50% {
    transform: translate(-24rpx);
  }
  100% {
      transform: translate(0%);
  }
}
.wrap {
    position: relative;
    padding: 24rpx 70rpx 0 24rpx;
    background: #ffffff;
    border-radius: 24rpx 24rpx 0 0 ;
}
.more_case {
    position: absolute;
    right: 0;
    top: 24rpx;
    width: 70rpx;
    height: 100rpx;
    background: #ffffff;
    box-shadow: -10rpx 0rpx 10rpx -10rpx #ffffff;
}
.more_icon {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    color: #666666FF;
    font-size: 20rpx;
}
.bubble {
    width: 280rpx;
    height: 80rpx;
    background: url('https://storage.360buyimg.com/wximg/common/couponGo.png') no-repeat;
    background-size: 100% 100%;
    position: absolute;
    right: 60rpx;
    top: 30%;
    z-index: 3;
    border-radius: 8rpx;
    transform: translate(0px, -50%);
    animation: tips 1.1s ease-in-out infinite;
}
/* 多个的样式 */
.scroll_case {
    position: relative;
    height: 112rpx;
    width: 100%;
}
.store_animation {
  animation: moveShow 1.8s ease-in-out both;
}
.item {
    margin-right: 16rpx;
    width: 276rpx;
    height: 112rpx;
}
.content {
    position: relative;
    padding: 0 16rpx;
    height: 100rpx;
    background:rgba(248,248,248,1);
    border-radius:8rpx;
    align-items: center;
}
.active {
    background: #32D75EFF;
}
.active::before {
    content: "";
    width: 42rpx;
    height: 18rpx;
    position: absolute;
    left: 50%;
    bottom: -11rpx;
    transform: translateX(-50%);
    background: url('https://storage.360buyimg.com/wximg/coupon/coupon_arrow_down_1.png') no-repeat center;
    background-size: 100% 100%;
}
.active .title {
    color: #ffffff
}
.active .sub_title {
    color: #ffffff;
}
.img_case {
    margin-right: 8rpx;
    width: 68rpx;
    height: 68rpx;
    border-radius:8rpx;
}
.img {
    width: 100%;
    height: 100%;
    border-radius:8rpx;
}
.info {
    color: #FFFFFFFF;
    overflow: hidden;
}
.title {
    font-size: 28rpx;
    font-weight: bold;
    color: #333333FF
}
.sub_title {
    display: flex;
    font-size: 20rpx;
}
.split {
    margin: 0 6rpx;
}
/* 弹层样式 */
.store_pop {
    position: fixed;
    left: 0;
    top: 122rpx;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.45);
    z-index: 4;
}
.store_pop_content {
    background: #ffffff;
    position: fixed;
    left: 0;
    top: 122rpx;
    width: 100%;
    z-index: 4;
    border-radius:0px 0px 24rpx 24rpx;
}
.scroll_case_pop {
    max-height: 80vh;
    padding: 0 12rpx;
    box-sizing: border-box;
}
.store_pop_item {
    padding: 0 12rpx 24rpx;
    width: 50%;
    display: inline-block;
    box-sizing: border-box;
}
.title_case {
    padding: 24rpx 24rpx 20rpx;
    align-items: center;
}
.nav_icon {
    color: #666666FF;
    font-size: 20rpx;
}
.store_pop_item_case {
    background:rgba(248,248,248,1);
    border-radius:8rpx;
    padding: 24rpx;
}
.pop_active {
    background-color: rgba(0, 207, 55, 0.1)
}
.store_pop_item_left {
    margin-right: 8rpx;
    width: 72rpx;
    height: 72rpx;
    border-radius:8rpx;
    border:1px solid rgba(0,0,0,0.08);
}
.store_pop_item_img {
    width: 100%;
    height: 100%;
    border-radius:8rpx;   
}
.store_pop_item_right {
    overflow: hidden;
}
.store_pop_item_title {
    color: #333333FF;
    font-size: 28rpx;
}
.store_pop_item_sub_title {
    color: #666666FF;
    font-size: 20rpx;
}
.store_pop_item_split {
    margin: 0 8rpx;
}
/* 一个的样式 */
.content_one {
