<block wx:if="{{$taroCompReady}}"><block><block wx:if="{{showWithDrawRecord}}"><view><view class="{{style.dark_bg}}" bindtap="anonymousFunc0"></view><image class="{{style.close_icon}}" src="https://storage.360buyimg.com/wximg/taro/common/close.png" bindtap="anonymousFunc1"></image><view class="{{style.pop_con}}"><view class="{{style.title}}">提现记录</view><scroll-view class="{{style.content}}" scroll-y="{{true}}" scroll-with-animation="{{true}}" bindscrolltolower="anonymousFunc2"><block><block wx:if="{{withdrawRecordList.length > 0}}"><view class="{{[style.item_cont, ele.state == -1 ? style.item_fail : '']}}" wx:for="{{withdrawRecordList}}" wx:for-item="ele" wx:for-index="index"><view class="{{['flex-row', style.item]}}"><view class="{{[style.left, 'flex-row']}}"><block><block wx:if="{{ele.state == -1}}"><view class="{{style.flag_cont}}"><image class="{{style.wx_flag}}" src="https://storage.360buyimg.com/wxmini/cash/wx_error.png"></image></view></block><block wx:else><view class="{{style.flag_cont}}"><image class="{{style.wx_flag}}" src="https://storage.360buyimg.com/wxmini/cash/wechat_green.png"></image></view></block></block><view class="{{style.record_info}}"><view class="{{style.record_state}}">{{ele.state == 0 ? "微信提现（提现中）" : ele.state == 1 ? "微信提现（提现成功）" : "微信提现（提现失败）"}}</view><view class="{{style.record_date}}">{{ele.operTime}}</view><block wx:if="{{ele.state == -1}}"><view class="{{style.record_fail}}">{{ele.failReason}}</view></block></view></view><view class="{{[style.right, ele.state == -1 ? style.right_fail : '']}}">{{ele.money}}</view></view><view class="{{style.line}}"></view></view></block><block wx:else><view class="{{style.txt}}">暂无提现记录</view></block></block></scroll-view></view></view></block></block></block>
