<block wx:if="{{$taroCompReady}}"><view class="{{style.targetBox}}"><view class="{{style.cont}}"><view class="{{style.innerBox}}"><swiper class="{{style.swiper}}" autoplay="{{true}}" vertical="{{true}}" circular="{{true}}" interval="1500"><swiper-item wx:key="index" wx:for="{{txtArr}}" wx:for-item="ele" wx:for-index="index"><view class="{{style.swiper_item}}">{{ele}}</view></swiper-item></swiper><view class="{{[style.txtCenter, style.targetMoneyTxt, 'flex-row']}}"><view class="{{style.targetMoney}}">{{cutAmount}}</view><view>元</view></view><view class="{{style.progressLineCont}}"><view class="{{style.progressLine}}"><view class="{{['flex-row', style.progressLineInnerCont]}}" style="{{anonymousState__temp3}}"><view class="{{style.progressLineInner}}"></view><image src="https://storage.360buyimg.com/wximg/hackOrder/arrow.png?1" class="{{style.flag}}"></image></view><view class="{{[style.target, 'flex-col']}}"><view class="{{style.text}}">返现:{{targetAmount}}元</view><image src="https://storage.360buyimg.com/wximg/hackOrder/money.png" class="{{style.icon}}"></image></view></view></view></view><block><block wx:if="{{isSelf}}"><template is="renderSelfInfo" data="{{...anonymousState__temp}}"></template></block><block wx:else><template is="renderOthersInfo" data="{{...anonymousState__temp2}}"></template></block></block></view></view></block>
<template name="renderSelfInfo"><block><block><view class="{{style.selfCont}}"><block wx:if="{{cutStatus == 9}}"><view class="{{style.description}}">{{description}}</view></block><block wx:if="{{cutStatus == 10}}"><view class="{{style.resonBox}}"><view class="{{style.description1}}">{{description}}</view><view class="{{style.orangeBtn}}" bindtap="viewActivety" data-e-tap-so="this"> 查看活动 </view></view></block><block wx:if="{{cutStatus == 1 || cutStatus == 2 || cutStatus == 3}}"><view class="{{[style.time, style.txtCenter, 'flex-row']}}">剩{{countDown.HH}}时{{countDown.MM}}分{{countDown.SS}}秒  奖励消失</view></block><block><block wx:if="{{cutStatus == 1 || cutStatus == 3 || cutStatus == 2 || cutStatus == 9 || cutStatus == 10}}"><template is="renderShareBtn" data="{{...anonymousState__temp4}}"></template></block><block wx:else><block><block wx:if="{{cutStatus == 4}}"><template is="renderCutEnd" data="{{...anonymousState__temp5}}"></template></block><block wx:else><block wx:if="{{cutStatus == 5 || cutStatus == 6 || cutStatus == 7 || cutStatus == 8}}"><template is="renderTopUpInfo" data="{{...anonymousState__temp6}}"></template></block></block></block></block></block><block wx:if="{{cutStatus == 1 || cutStatus == 2 || cutStatus == 3}}"><view class="{{[style.txtCenter, style.shareTips]}}">邀请新用户助力金额更大哦～</view></block></view><block wx:if="{{kddSocial == 'new1' && friendInfos.length > 0 && isHelping}}"><template is="renderFriend" data="{{...anonymousState__temp7}}"></template></block></block></block></template>
<template name="renderOthersInfo"><block><view><view class="{{[style.description, style.margin_btm_null]}}">{{description}}</view><view class="{{[style[currentInfo.class], style.marBtm30, helpStatus == 0 ? style.animation : '']}}" bindtap="anonymousFunc0">{{currentInfo.txt}}</view></view></block></template>
<template name="renderShareBtn"><block><view><block><block wx:if="{{cutStatus == 9 || cutStatus == 10}}"><view class="{{[style.shareFriendGray, style.marBtm30]}}">邀请好友助力</view></block><block wx:else><button open-type="share" bindtap="anonymousFunc1" class="{{[style.shareFriend, style.marBtm30]}}">{{cutStatus == 1 || cutStatus == 2 ? '分享给好友，再领一次' : nCardBtn ? nCardBtn : '邀请好友助力'}}</button></block></block></view></block></template>
<template name="renderCutEnd"><block><view class="{{style.shareFriendGray + ' ' + style.marBtm30}}">砍价已结束</view></block></template>
<template name="renderTopUpInfo"><block><view class="{{[classC, style.marTop86, style.marBtm30]}}" bindtap="anonymousFunc2">{{currentInfo.btnTxt}}</view></block></template>
<template name="renderFriend"><block><view class="{{[style.friendCont, friendInfos.length == 3 ? style.friendCont2 : '', friendInfos.length < 3 ? style.friendCont3 : 'flex-row']}}"><block><block wx:if="{{friendInfos.length < 3}}"><view class="{{[style.friendOne, 'flex-row']}}" wx:for="{{loopArray79}}" wx:for-item="ele" wx:for-index="__index3"><view class="{{[style.left, 'flex-row']}}"><image src="{{ele.$original.headImg}}" class="{{style.headImg}}"></image><view class="{{style.nameCont}}"><view class="{{[style.name, 'ellipsis']}}">{{ele.$original.nickname}}</view><view class="{{style.num}}">可抵{{ele.$original.peopleNum}}人助力</view></view></view><button open-type="share" class="{{style.friendBtn}}" bindtap="anonymousFunc3" data-e-tap-so="this" data-e-tap-a-a="{{ele._$indexKey}}"><view class="{{style.btnInner}}"><image src="https://storage.360buyimg.com/wximg/hackOrder/weixinwhite.png" class="{{style.wxIcon}}"></image><view>分享给TA</view></view></button></view></block><block wx:else><view class="{{[style.friendItem]}}" wx:for="{{loopArray80}}" wx:for-item="ele" wx:for-index="__index4"><view class="{{style.headImgCont}}"><image src="{{ele.$original.headImg}}" class="{{style.headImg}}"></image><view class="{{style.headFlag}}">抵{{ele.$original.peopleNum}}人助力</view></view><view class="{{[style.friendTxt, 'ellipsis']}}">{{ele.$original.nickname}}</view><button open-type="share" class="{{style.friendBtn}}" bindtap="anonymousFunc4" data-e-tap-so="this" data-e-tap-a-a="{{ele._$indexKey2}}"><image src="https://storage.360buyimg.com/wximg/hackOrder/weixinwhite.png" class="{{style.wxIcon}}"></image><view>分享给TA</view></button></view></block></block></view></block></template>
