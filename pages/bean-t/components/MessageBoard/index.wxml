<block wx:if="{{$taroCompReady}}"><view><view class="{{style.entry}}" bindtap="handleClickEntry" data-e-tap-so="this"></view><template is="renderMB" data="{{...anonymousState__temp}}"></template></view></block>
<template name="renderMB"><block><block wx:if="{{showMB}}"><view><view class="{{style.MB_bg}}" bindtap="handleClickBG" data-e-tap-so="this"></view><view class="{{style.MB_box}}"><view class="{{style.close}}" bindtap="handleClickClose" data-e-tap-so="this"></view><textarea class="{{style.MB_textarea}}" maxlength="200" placeholder="{{placeholder}}" placeholder-class="{{style.MB_placeholder}}" bindinput="handleInput" data-e-input-so="this"></textarea><view class="{{style.MB_words}}">{{numbers}}/200</view><view class="{{style.MB_btn}}" bindtap="handleClickBtn" data-e-tap-so="this"></view></view></view></block></block></template>
