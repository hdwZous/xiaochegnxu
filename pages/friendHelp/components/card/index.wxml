<block wx:if="{{$taroCompReady}}"><view><block><block wx:if="{{data.status == 1}}"><template is="renderNotStart" data="{{...anonymousState__temp}}"></template></block><block wx:else><block><block wx:if="{{data.helpStatus == 2}}"><template is="renderHelpSuccess" data="{{...anonymousState__temp2}}"></template></block><block wx:else><block><block wx:if="{{data.helpStatus === 1 && data.status == 2}}"><template is="renderHelping" data="{{...anonymousState__temp3}}"></template></block><block wx:else><block><block wx:if="{{data.status == 2 && data.helpStatus !== 1}}"><template is="renderStartNoHelp" data="{{...anonymousState__temp4}}"></template></block><block wx:else><block wx:if="{{data.helpStatus == 3}}"><template is="renderFailed" data="{{...anonymousState__temp5}}"></template></block></block></block></block></block></block></block></block></block></view></block>
<template name="renderNotStart"><block><view class="{{style.listCouponWrap + ' flex-row'}}" bindtap="onClickCard" data-e-tap-so="this"><view class="{{style.image_case + ' flex-none'}}"><view class="{{style.image}}"><image mode="widthFix" class="{{style.img}}" src="{{data.newImageUrl}}" alt="商品图"></image><block wx:if="{{data.imageType && (data.imageType == 1 || data.imageType == 2)}}"><view class="{{style.leftMoneyBox}}">¥<text class="{{style.leftMoney}}">{{data.amount}}</text></view></block></view></view><view class="{{'' + style.info}}"><view class="{{style.name + ' ellipsis'}}">{{data.couponTitle || ''}}</view><view class="{{[style.take_info_box]}}"><text class="{{style.already_help}}">{{data.totalMemberNum}}人助力</text><block wx:if="{{data.alreadyTakeCount < 99}}"><text class="{{style.already_take}}">已领百+</text></block></view></view><view class="{{[style.couponRt]}}"><view class="{{style.price}}"><text class="{{style.amount_unit}}">￥</text><text class="{{style.amount}}">{{data.amount}}</text></view><view class="{{style.couRule}}">{{data.limitRule}}</view>{{btn}}</view></view></block></template>
<template name="renderStartNoHelp"><block><view class="{{style.listCouponWrap + ' flex-row'}}" bindtap="onClickCard" data-e-tap-so="this"><block wx:if="{{data.topState == 1}}"><image class="{{style.hotTip}}" src="https://storage.360buyimg.com/wximg/friendhelp/hot.png"></image></block><view class="{{style.image_case + ' flex-none'}}"><view class="{{style.image}}"><image mode="widthFix" class="{{style.img}}" src="{{data.newImageUrl}}" alt="商品图"></image><block wx:if="{{data.imageType && (data.imageType == 1 || data.imageType == 2)}}"><view class="{{style.leftMoneyBox}}">¥<text class="{{style.leftMoney}}">{{data.amount}}</text></view></block></view></view><view class="{{'' + style.info}}"><view class="{{style.name + ' ellipsis'}}">{{data.couponTitle || ''}}</view><view class="{{style.take_info_box}}"><text class="{{style.already_help}}">{{data.totalMemberNum}}人助力</text><block wx:if="{{data.alreadyTakeCount > 99}}"><text class="{{style.already_take}}">已领百+</text></block></view></view><view class="{{[style.couponRt]}}"><view class="{{style.price}}"><text class="{{style.amount_unit}}">￥</text><text class="{{style.amount}}">{{data.amount}}</text></view><view class="{{style.couRule}}">{{data.limitRule}}</view><view class="{{'' + style.btn}}">免费领</view></view></view></block></template>
<template name="renderHelping"><block><view class="{{style.listCouponWrap + ' flex-row'}}" bindtap="onClickCard" data-e-tap-so="this"><view class="{{style.helpTip}}">助力中</view><view class="{{style.image_case + ' flex-none'}}"><view class="{{style.image}}"><image mode="widthFix" class="{{style.img}}" src="{{data.newImageUrl}}" alt="商品图"></image><block wx:if="{{data.imageType && (data.imageType == 1 || data.imageType == 2)}}"><view class="{{style.leftMoneyBox}}">¥<text class="{{style.leftMoney}}">{{data.amount}}</text></view></block></view></view><view class="{{'' + style.info}}"><view class="{{style.name + ' ellipsis'}}">{{data.couponTitle || ''}}</view><view class="{{style.info_case}}"><block wx:if="{{data.groupRemainTime && data.groupRemainTime > 0}}"><count-down compid="{{$compid__302}}"></count-down></block><view class="{{style.process}}"><view class="{{style.cover}}" style="{{anonymousState__temp7}}"></view></view><text class="{{style.up_word}}">仅差{{data.leftMembers}}人</text></view></view><view class="{{[style.couponRt]}}"><view class="{{style.price}}"><text class="{{style.amount_unit}}">￥</text><text class="{{style.amount}}">{{data.amount}}</text></view><view class="{{style.couRule}}">{{data.limitRule}}</view><button open-type="share" class="{{[style.btn, style.btnGoOn]}}" data-params="{{data}}" catchtap="onStopPropagation" data-e-tap-so="this">继续邀请</button></view></view></block></template>
<template name="renderFailed"><block><view class="{{style.listCouponWrap + ' flex-row'}}" bindtap="onClickCard" data-e-tap-so="this"><view class="{{style.image_case + ' flex-none'}}"><view class="{{style.image}}"><image mode="widthFix" class="{{style.img}}" src="{{data.newImageUrl}}" alt="商品图"></image><block wx:if="{{data.imageType && (data.imageType == 1 || data.imageType == 2)}}"><view class="{{style.leftMoneyBox}}">¥<text class="{{style.leftMoney}}">{{data.amount}}</text></view></block></view></view><view class="{{'' + style.info}}"><view class="{{style.name + ' ellipsis'}}">{{data.couponTitle || ''}}</view><view class="{{[style.info_case, style.mar_top80]}}"><view class="{{style.process}}"><view class="{{style.cover}}" style="{{anonymousState__temp8}}"></view></view><text class="{{style.up_word}}">仅差{{data.leftMembers}}人</text></view></view><view class="{{[style.couponRt, style.grayFont]}}"><view class="{{style.price}}"><text class="{{style.amount_unit}}">￥</text><text class="{{style.amount}}">{{data.amount}}</text></view><view class="{{style.couRule}}">{{data.limitRule}}</view><image class="{{style.grayImg}}" src="{{'https://storage.360buyimg.com/wximg/friendhelp/' + (robedFlag ? 'robed' : 'finished') + '.png'}}"></image></view></view></block></template>
<template name="renderHelpSuccess"><block><view class="{{style.listCouponWrap + ' flex-row'}}" bindtap="onClickCard" data-e-tap-so="this"><view class="{{style.image_case + ' flex-none'}}"><view class="{{style.image}}"><image mode="widthFix" class="{{style.img}}" src="{{data.newImageUrl}}" alt="商品图"></image><block wx:if="{{data.imageType && (data.imageType == 1 || data.imageType == 2)}}"><view class="{{style.leftMoneyBox}}">¥<text class="{{style.leftMoney}}">{{data.amount}}</text></view></block></view></view><view class="{{'' + style.info}}"><view class="{{style.name + ' ellipsis'}}">{{data.couponTitle || ''}}</view><view class="{{[style.info_case, style.mar_top80]}}"><view class="{{style.process}}"><view class="{{style.cover}}" style="{{anonymousState__temp9}}"></view></view><text class="{{style.up_word}}">助力成功</text></view></view><view class="{{[style.couponRt, data.couponExpired ? style.grayFont : '']}}"><view class="{{style.price}}"><text class="{{style.amount_unit}}">￥</text><text class="{{style.amount}}">{{data.amount}}</text></view><view class="{{style.couRule}}">{{data.limitRule}}</view><block><block wx:if="{{data.couponExpired}}"><image class="{{style.grayImg}}" src="https://storage.360buyimg.com/wximg/friendhelp/overtime.png"></image></block><block wx:else><view class="{{style.btn + ' ' + style.test}}" catchtap="onClickBtn" data-e-tap-so="this">去使用</view></block></block></view></view></block></template>
