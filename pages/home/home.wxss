}
.container.show_default{
    background: #fff;
}
button {
    padding-left: 0 !important;
    padding-right: 0 !important;
    border-radius: 4rpx !important;
    background: transparent !important;
}

button:after {
    border: 0;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.store-cont {
    background: #fff;
    border-radius: 32rpx 32rpx 0 0;
    padding-top: 30rpx;
    margin-top: 20rpx;
    min-height: 100px;
}

.cont-title {
    color: #333333;
    font-size: 40rpx;
    padding-left: 18rpx;
    font-weight: bold;
}

.store-cont .store-title {
    margin-bottom: 10rpx;
}

.stepTips {
    z-index: 3;
    position: fixed;
    width: 165rpx;
    height: 64rpx;
    bottom: 8rpx;
    left: 25%;
    margin-left: 15rpx;
    transform-origin: bottom;
    -webkit-transform-origin: bottom;
    animation: stepIncomePopAnimate 1.5s linear infinite;
    -webkit-animation: stepIncomePopAnimate 1.5s linear infinite;
}

@keyframes stepIncomePopAnimate {
    0% {
        transform: translateY(0) scaleY(1);
    }

    25% {
        transform: translateY(4rpx) scaleY(1);
    }

    50% {
        transform: translateY(8rpx) scaleY(0.9);
    }

    75% {
        transform: translateY(4rpx) scaleY(1);
    }

    100% {
        transform: translateY(0) scaleY(1);
    }
}

/*测试自动切换样式*/
.test_auto_env {
    border: 10px solid gray;
    border-right-width: 0;
    border-left-width: 40rpx;
    border-left-color: #00df9d;
    z-index: 20;
    position: fixed;
    right: -240rpx;
    top: 500rpx;
    background: gray;
    text-align: center;
    transition: right ease-out 0.2s;
}

.test_auto_active {
    right: 0;
}

.test_auto_env_item {
    padding: 0 20rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #ffffff;
    font-size: 30rpx;
    font-weight: bold;
}

.test_auto_env_item_active {
    background: red;
}

.nearby_wrap {
    padding: 20rpx 20rpx 10rpx;
    color: #333333;
    font-size: 38rpx;
    margin-top: 20rpx;
    background: #f6f6f6;
}

.nearby_item {
    position: relative;
    margin-right: 60rpx;
}

.nearby_bold {
    font-weight: bold;
}

.border_bottom {
    display: none;
}

.nearby_active {
    position: relative;
    font-weight: bold;
}

.nearby_active .border_bottom {
    display: block;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: -4px;
    width: 96%;
    height: 2px;
    background: linear-gradient(
        135deg,
        rgba(39, 220, 10, 1) 0%,
        rgba(8, 193, 42, 1) 100%
    );
    border-radius: 2rpx 2rpx 0 0;
}
