.sku_tab {
    z-index: 3;
    position: relative;
    background: rgb(255, 255, 255);
    width: 100%;
}
.sku_tab_pos{
    height: 140rpx;
}
.top_banner{
    z-index: 3;
    position: fixed;
    width: 750rpx;
    height: 80rpx;
    /* background-color: #afa; */
}
.top_banner .close_banner{
    position: absolute;
    width: 80rpx;
    height: 80rpx;
    top: 0;
    right: 0;
}
.sku_wrap {
    height: 140rpx;
    align-items: center;
}
.sku_case {
    position: relative;
    width: 175rpx;
    text-align: center;
    box-sizing: border-box;
}
.sku_case::after {
    content: "";
    width: 1px;
    height: 40rpx;
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
    background: rgba(232, 232, 232, 1);
}
.sku_case:last-child::after {
    content: "";
    width: 1px;
    height: 40rpx;
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
    background: transparent;
}
.img_title {
    width: 156rpx;
    height: 48rpx;
    vertical-align: middle;
}
.sku_title {
    font-size: 32rpx;
    color: rgb(51, 51, 51);
    font-weight: bold;
}
.sku_sub_title {
    color: rgb(153, 153, 153);
    font-size: 24rpx;
    height: 34rpx;
    line-height: 34rpx;
}
.sku_title_active {
    color: rgb(35, 219, 49);
}
.sku_sub_title_active {
    background-color: rgba(8, 206, 62);
    border-radius: 20rpx;
    color: #ffffff;
    margin: 0 16rpx;
    padding: 0 20rpx;
}
.list_wrap {
    padding-top: 16rpx;
}
.list_wrap.hasTopBanner{
    padding-top: 96rpx;
}
.list_case {
    flex-wrap: wrap;
    padding: 0 16rpx;
}
.list_item_case {
    width: 50%;
    padding: 20rpx 8rpx 0;
    box-sizing: border-box;
}
.list_item {
    margin-bottom: 16rpx;
}
.ad_banner {
    width: 702rpx;
    height: 180rpx;
    margin: 0 auto 16rpx;
    border-radius: 16rpx;
}
.ad_banner_img {
    width: 100%;
    height: 100%;
    border-radius: 16rpx;
    vertical-align: middle;
}
.list_second_tab {
    margin: 0 24rpx;
    flex-wrap: wrap;
    background: #ffffff;
    border-radius: 24rpx;
    padding: 24rpx 12rpx 1rpx;
}
.list_second_case {
    width: 25%;
    justify-content: center;
}
.list_second_tab_item {
    width: 150rpx;
    height: 64rpx;
    line-height: 64rpx;
    background: #f6f6f6;
    border-radius: 32rpx;
    font-size: 24rpx;
    color: rgba(102, 102, 102, 1);
    text-align: center;
    margin-bottom: 24rpx;
}
.list_second_tab_item_active {
    background: rgba(0, 207, 55, 0.1);
    color: rgba(0, 207, 56, 1);
}
.reach_bottom {
    font-size: 24rpx;
    color: rgb(51, 51, 51);
    text-align: center;
    padding: 60rpx 0;
}
.default_box{
    margin-bottom: 40rpx;
}