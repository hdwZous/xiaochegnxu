<block  
      wx:if="{{newerData.data &&  newerData.data.length && newerData.data[0].floorCellData.imgUrl}}"
      wx:for="{{newerData.data}}"
      wx:key="index"
      wx:for-item="item"
>
      <view 
            class=" newer-container exposure home_floorNewerV1"
            style="background: {{newerData.needFloorBackground && newerData.floorBgColor || ''}} url({{newerData.floorBgImg}}) no-repeat; background-size: 100% 100%"
            data-item="{{item.floorCellData}}"
            data-user-action="{{item.floorCellData.userAction}}"
            data-trace-id="{{pageDataTraceId}}"
            data-page-id="{{buriedObj.pageIdFirstPage}}"
            data-current-page-name="{{buriedObj.currentPageName}}"
            data-pre-page-name='{{buriedObj.prePageName}}'
      >
            <image 
                  src="{{imgLazyLoad[floorId+index] ? 'https://img30.360buyimg.com/mobilecms/jfs/t1/205001/33/10640/175555/61655053Ec2bd0f1f/7ec96c314dd845d9.png'  : 'https://storage.360buyimg.com/wximg/home/default_bg.png'}}"
                  data-lazy-img-id="{{floorId+index}}"
                  style="width: {{item.floorCellData.imgWidth}}; height: {{item.floorCellData.imgHeight}};"
                  class="img lazy-load" 
                  data-item="{{item.floorCellData}}"
                  data-user-action="{{item.floorCellData.userAction}}"
                 
            />
      </view>
</block>