<view  class="scroll_view"> 
    <view class="scroll_container {{isFirstFloor && !isHeaderFloorHasBgImg ? 'isFirstFloorBorderRadius' :''}}" hover-class="none" hover-stop-propagation="false"
      style="{{backgroundUrl ? 'background: url(' + backgroundUrl+') no-repeat center top; background-size: 100% auto' : ''}}"
    >
        <scroll-view scroll-x scroll-anchoring="true" enhanced="true" scroll-with-animation scroll-left="{{scrollLeft}}" bindscroll="onScroll"  class="scroll_case"
        >
            <!--
            <view class="flex-row {{srollFlag ? 'scroll_case' : 'case'}} " >
            -->
            <view class="flex-row case " aria-role="menu"  aria-label="菜单">
                <!-- 第一屏 --> 
                <view 
                    wx:if="{{listOne.length>0 }}" 
                    class="flex-row flex-none item row_box 111 "
                    style="width:{{listOne.length> 5 ? 5*ballWidth : listOne.length * ballWidth}}px">
                    <view wx:for="{{listOne}}" 
                        wx:for-item="item" 
                        wx:for-index="index" 
                        wx:key="item" 
                        data-item="{{item.floorCellData}}" 
                        bindtap="onClickBallItem" 
                        data-trace-id="{{pageDataTraceId}}"
                        data-page-id="{{buriedObj.pageIdFirstPage}}"
                        data-current-page-name="{{buriedObj.currentPageName}}"
                        data-pre-page-name='{{buriedObj.prePageName}}'
                        data-floor="入口球{{index}}" 
                        data-user-action="{{item.floorCellData.userAction}}" 
                        class=" ball_case flex-none exposure home_ball" 
                        style="width:{{ballWidth}}px">
                            <view  aria-role="link" aria-label="{{item.floorCellData.title}}频道">
                                <view class="ball_img" aria-hidden="true">
                                    <image src="{{(imgLazyLoad[floorId+index] && item.floorCellData.imgUrl) ? item.floorCellData.imgUrl : 'https://storage.360buyimg.com/wximg/home/default_bg.png'}}" data-floor="入口球" data-lazy-img-id="{{floorId+index}}" class="img lazy-load"  ></image>
                                    
                                </view>
                                <view class="ball_name ellipsis" style="color: {{item.floorCellData.titleColor}}" aria-hidden="true">
                                    {{item.floorCellData.title}}
                                </view>
                            </view>
                            <view class="ball_tag" wx:if="{{item.floorCellData.words}}" 
                                  aria-role="link" 
                                  aria-label="{{item.floorCellData.title}}频道.{{item.floorCellData.words}}">
                                {{item.floorCellData.words}}
                                <image aria-hidden="true" class='white-border-triangle' src="https://storage.360buyimg.com/wximg/common/whiteBorder.png" />
                            </view>
                    </view>
                </view>
                <view 
                    wx:if="{{listOne_2.length>0 }}" 
                    class="flex-row flex-none  item row_box 222 "
                    style="width:{{listOne_2.length> 5 ? 5*ballWidth : listOne_2.length * ballWidth}}px">
                    <view wx:for="{{listOne_2}}" 
                    wx:for-item="item" 
                    wx:for-index="index" 
                    wx:key="item" 
                    data-item="{{item.floorCellData}}" 
                    bindtap="onClickBallItem" 
                    data-trace-id="{{pageDataTraceId}}"
                    data-page-id="{{buriedObj.pageIdFirstPage}}"
                    data-current-page-name="{{buriedObj.currentPageName}}"
                    data-pre-page-name='{{buriedObj.prePageName}}'
                    data-floor="入口球{{index}}" 
                    data-user-action="{{item.floorCellData.userAction}}" 
                    class=" ball_case flex-none exposure home_ball" 
                    style="width:{{ballWidth}}px"
                    aria-role="link"  
                    aria-label="{{item.floorCellData.title}}频道">
                        <view class="ball_img" aria-hidden="true">
                            <image src="{{(imgLazyLoad[floorId+index] && item.floorCellData.imgUrl) ? item.floorCellData.imgUrl : 'https://storage.360buyimg.com/wximg/home/default_bg.png'}}" data-floor="入口球" data-lazy-img-id="{{floorId+index}}" class="img lazy-load"></image>
                            
                        </view>
                        <view class="ball_name ellipsis" 
                              style="color: {{item.floorCellData.titleColor}}"
                              aria-hidden="true">
                            {{item.floorCellData.title}}
                        </view>
                        <view class="ball_tag" 
                              wx:if="{{item.floorCellData.words}}"
                              aria-hidden="true">
                            {{item.floorCellData.words}}
                            <image aria-hidden="true" class='white-border-triangle' src="https://storage.360buyimg.com/wximg/common/whiteBorder.png" />
                        </view>
                    </view>
                </view>
                <view 
                    wx:if="{{listOne_3.length>0 }}" 
                    class="flex-row flex-none item row_box 333 "
                    style="width:{{listOne_3.length> 5 ? 5*ballWidth : listOne_3.length * ballWidth}}px">
                    <view wx:for="{{listOne_3}}" 
                    wx:for-item="item" 
                    wx:for-index="index" 
                    wx:key="item" 
                    data-item="{{item.floorCellData}}" 
                    bindtap="onClickBallItem" 
                    data-trace-id="{{pageDataTraceId}}"
                    data-page-id="{{buriedObj.pageIdFirstPage}}"
                    data-current-page-name="{{buriedObj.currentPageName}}"
                    data-pre-page-name='{{buriedObj.prePageName}}'
                    data-floor="入口球{{index}}" 
                    data-user-action="{{item.floorCellData.userAction}}" 
                    class=" ball_case flex-none exposure home_ball" 
                    style="width:{{ballWidth}}px"
                    aria-role="link"  
                    aria-label="{{item.floorCellData.title}}频道">
                        <view class="ball_img" aria-hidden="true">
                            <image src="{{(imgLazyLoad[floorId+index] && item.floorCellData.imgUrl) ? item.floorCellData.imgUrl : 'https://storage.360buyimg.com/wximg/home/default_bg.png'}}" data-floor="入口球" data-lazy-img-id="{{floorId+index}}" class="img lazy-load"></image>
                            
                        </view>
                        <view class="ball_name ellipsis" 
                              style="color: {{item.floorCellData.titleColor}}"
                              aria-hidden="true">
                            {{item.floorCellData.title}}
                        </view>
                        <view class="ball_tag" 
                              wx:if="{{item.floorCellData.words}}"
                              aria-hidden="true">
                            {{item.floorCellData.words}}
                            <image aria-hidden="true" class='white-border-triangle' src="https://storage.360buyimg.com/wximg/common/whiteBorder.png" />
                        </view>
                    </view>
                </view>
                
            </view>
        </scroll-view>
    <!-- 进度滑条 -->
    <view class="process_case" 
        wx:if="{{floor.data.length>10}}" 
        style="background: {{floor.changeColor}};width:{{totalLen * 48 /10}}rpx"
    >
        <view class="process" style="left:{{processLeft}};width:{{processWidth}}rpx;background: {{floor.initColor}};"></view>
    </view>
    </view>
   
</view>