}
.logo-wrap-needOverlay {
  width: 128rpx;
  height: 128rpx;
  position: absolute;
  top: 8rpx;
  right: 0;
  left: 4rpx;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.02);
}