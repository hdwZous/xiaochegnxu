                                    <view class="hour countDownTime_text">
                                    {{hour}}
                                    </view>
                                    <view class="minute countDownTime_text">
                                          :{{minute}}
                                    </view>
                                    <view class="timer countDownTime_text">
                                          :{{second}}
                                    </view>
                              </view>
                              <view 
                                    wx:if="{{item.floorCellData.countDownFlag == 1}}"
                                    class=" d-flex">
                                    <view class=" countDownTime_text">
                                         {{item.floorCellData.countDownTime || ''}}
                                    </view>
                              </view>
                              {{item.floorCellData.countDownText || ''}}
