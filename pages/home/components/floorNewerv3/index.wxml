                        <!--倒计时-->
                        <view 
                              aria-hidden="true"
                              class="countDownTime d-flex"
                              wx:if="{{item.floorCellData.countDownText && (item.floorCellData.countDownFlag == 1 || item.floorCellData.countDownFlag == 2 ) && item.floorCellData.countDownTime}}"
