                              aria-role="text"
                              aria-label="{{item.floorCellData.mainTitleText}}"
                              class="friend_backtitle ellipsis" 
                              wx:if="{{item.floorCellData.modelShowType && item.floorCellData.modelShowType == 2 }}">
                              {{item.floorCellData.mainTitleText}}
