                        hover-stop-propagation="false">
                        <!--新人图片-->
                        <image 
                              aria-role="text"
                              aria-label="活动标题"
                              class="titleImg" 
                              wx:if="{{item.floorCellData.modelShowType && item.floorCellData.modelShowType == 1 && item.floorCellData.mainTitleImg && item.floorCellData.mainTitleImgWidth && item.floorCellData.mainTitleImgHeight }}"
