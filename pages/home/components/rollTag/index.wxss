.padT24 {
  padding-top: 24rpx;
}
.floor-good-wrapper {
  width: 654rpx;
  height: 250rpx;
  padding: 24rpx;
  background: #fff;
  position: relative;
}
.floor-good-wrapper .title {
  font-size: 32rpx;
  font-weight: bold;
  color :#333;
  line-height: 40rpx;
}
.floor-good-wrapper .change {
  position: absolute;
  top: 20rpx;
  right: 24rpx;
  font-size: 24rpx;
  color: #333;
  /* padding: 4rpx 14rpx; */
  border-radius: 22rpx;
  border: 2rpx solid #ebebeb;
  display: flex;
  align-items: center;
  width: 124rpx;
  height: 40rpx;
  justify-content: center;

}
.change .img {
  width: 24rpx;
  height: 24rpx;
}
.good-content-wrapper {
  position: relative;
  display: flex;
  margin-top: 20rpx;
  height: 200rpx;
}
.category-swiper-wrapper {
  position: absolute;
  z-index: 2;
  top: 16rpx;
  left: 0;
  width: 284rpx;
  height: 120rpx;
  margin-top: 26rpx;
}
.title-wrapper {
  height:100%;
  display: flex;
  align-items: center;
}
.category-title {
  background:linear-gradient(to right, #FFDB79,#E4B164);
  border-radius:19rpx;
  opacity:0.2;
  font-size:20rpx;
  color: #fff;
  padding: 0 16rpx;
  -webkit-transition: transform 500ms;
  transition: transform 500ms;
  transform: scale(1);
  -webkit-transform: scale(1);
  transform-origin: left center;
}
.bigger1 {
  transform: scale(1.25);
  -webkit-transform: scale(1.25);
  opacity:1;
  background: linear-gradient(to right,#F1D167 ,#EDC12F);
}
.right-good {
  flex: 1;
  margin-left: 174rpx;
}
.good-swiper {
  height: 200rpx;
}
.swiper-item-wrapper {
  display: flex;
  justify-content: flex-start;
}
.good-item {
  text-align: center;
}
.imgurl-wrapper {
  margin-left: 16rpx;
  margin-bottom: 8rpx;
}
.imgurl-wrapper .img {
  width:144rpx;
  height:144rpx;
  border-radius:16rpx;
  background: #f6f6f6;
}
.price {
  font-size: 28rpx;
  font-family: regular;
}
.mask {
  width: 200rpx;
  height: 224rpx;
  background: rgba(0, 0, 0, 0);
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 5;
}
.mask-right{
  height: 224rpx;
  position: absolute;
  left: 200rpx;
  right: 0;
  z-index: 5;
}
.mask-right .rollaaa{
  float: left;
  width: 33%;
  height: 100%;
}