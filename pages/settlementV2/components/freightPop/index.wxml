<view class="freight-cover" wx:if="{{isShowFreightPop && isData}}" catchtouchmove='catchtouchmove'>
  <view class="freight-pop" bindtap="closeFreight"></view>
  <view
    class="freight-container {{isShowFreightPop ? 'freight-movein' : ''}}"
    id="freight-container">
    <view class="freight-close" bind:tap="closeFreight"></view>
    <view class="freight-title">{{freight.breaksFreightLayer.title}}</view>
    <block wx:for="{{freight.breaksFreightLayer.breaksFreightVOS}}" wx:for-item="item">
      <!-- 开通商家会员 -->
      <view class="freight-business" wx:if="{{item.type == 1}}">
        <view class="freight-business-left">
          <view
            aria-role="text"
            aria-label="{{item.title.label}}"
            class="freight-business-text">
            <text
              wx:for="{{item.title.textList}}"
              wx:for-item="ele"
              style="color: {{ele.fontColor}};font-size: {{ele.fontSize}};"
            >{{ele.text}}</text>
          </view>
            <view
            class="freight-business-text2"
            aria-role="text"
            aria-label="{{item.subtitle.label}}"
            ><text 
              wx:for="{{item.subtitle.textList}}"
              wx:for-item="ele"
              bind:tap="jumpAgreement"
              data-item="{{ele}}"
              style="color: {{ele.fontColor}};font-size: {{ele.fontSize}}"
            >{{ele.text}}</text>
          </view>
        </view>
        <view
          data-open-state="{{item.openState}}"
          class="freight-business-right"
          bind:tap="checkBusiness">
          <view class="{{item.openState == 2 ? 'freight-uncheck' : 'freight-check'}}"></view>
        </view>
      </view>
      <!-- 运费立减 -->
      <view class="freight-cut" wx:if="{{item.type == 2}}">
        <view class="freight-cut-title">
          <view
            class="freight-cut-left"
            aria-role="text"
            aria-label="{{item.title.label}}">
            <text
              wx:for="{{item.title.textList}}"
              wx:for-item="ele"
              style="color: {{ele.fontColor}};font-size: {{ele.fontSize}}"
            >{{ele.text}}</text>
          </view>
          <view
            data-item="{{item}}"
            class="freight-cut-right"
            bind:tap="jumpMoreProduct">
            <text>{{item.toName}}</text>
            <image src="https://storage.360buyimg.com/wxmini/settlement/settlemmentV2_pop_arrow.png"/>
          </view>
        </view>
        <view
          class="freight-cut-txt"
          aria-role="text"
          aria-label="{{item.subtitle.label}}">
          <text 
            wx:for="{{item.subtitle.textList}}"
            wx:for-item="ele"
            style="color: {{ele.fontColor}};font-size: {{ele.fontSize}}"
          >{{ele.text}}</text>
        </view>
        <!-- 运费立减列表 -->
        <view class="freight-list">
          <view
            wx:for="{{item.freeFreightProductsList}}"
            wx:for-item="ele"
            class="freight-list-item">
            <webp-img
              webp-img="image-detail"
              width="152"
              height="152"
              needOverlay="{{true}}"
              src="{{ele.img}}"
            ></webp-img>
            <!-- <image class="freight-list-itemImage" src="{{ele.img}}"></image> -->
            <view class="freight-list-itemtext">{{ele.tagName}}</view>
          </view>
        </view>
      </view>
      <!-- 运费凑单 -->
      <view class="freight-gather" wx:if="{{item.type == 3}}">
        <view class="freight-gather-title">
          <view
            aria-role="text"
            aria-label="{{item.title.label}}"
            class="freight-gather-left">
            <text
              wx:for="{{item.title.textList}}"
              wx:for-item="ele"
              style="color: {{ele.fontColor}};font-size: {{ele.fontSize}}"
            >{{ele.text}}</text>
          </view>
          <view
            data-item="{{item}}"
            class="freight-gather-right"
            bind:tap="jumpFreight">
            <text>{{item.toName}}</text>
            <image src="https://storage.360buyimg.com/wxmini/settlement/settlemmentV2_pop_arrow.png"/>
          </view>
        </view>
        <view
          aria-role="text"
          aria-label="{{item.subtitle.label}}"
          class="freight-gather-txt">
          <text 
            wx:for="{{item.subtitle.textList}}"
            wx:for-item="ele"
            style="color: {{ele.fontColor}};font-size: {{ele.fontSize}}"
          >{{ele.text}}</text>
        </view>
      </view>

    </block>
  </view>
</view>