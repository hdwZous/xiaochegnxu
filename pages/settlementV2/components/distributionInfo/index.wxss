/*自提地址模块*/

.distributionInfo {
  position: relative;
}

.lift_head {
  height: 80rpx;
  box-sizing: border-box;
  padding-top: 8rpx;
  background: rgba(246, 246, 246, 1);
  /* border-radius: 24rpx 24rpx 0 0; */
  margin-top: 24rpx;
  transform: translateY(1px);
  -webkit-transform: translateY(1px);
}

.address_left_checked {
  background: url("https://storage.360buyimg.com/wximg/storewin/settle-tab-left.png") no-repeat center;
  background-size: 100% 100%;
}

.address_right_checked {
  background: url("https://storage.360buyimg.com/wximg/storewin/settle-tab-right.png") no-repeat center;
  background-size: 100% 100%;
}

.settlement-module-item {
  box-sizing: border-box;
  background: #fff;
  /* padding: 32rpx 24rpx 30rpx 24rpx; */
  padding: 32rpx 24rpx 0rpx 24rpx;
  border-radius: 24rpx 24rpx 0 0;
  transform: translateY(1px);
  -webkit-transform: translateY(1px);
}

.lift_item {
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
}

.lift_active {
  background: #ffffff;
  border-radius: 24rpx 24rpx 0 0;
  font-weight: bold;
}

.lift_a {
  padding-left: 6rpx;
  align-items: flex-start;
  -webkit-align-items: flex-start;
}

.lift_address {
  padding: 30rpx 24rpx 0;
  color: #333;
  font-size: 40rpx;
  font-weight: bold;
  background: #fff;
  overflow: hidden;
}

.lift_address.has_radius {
  border-radius: 24rpx 24rpx 0 0;
}

.address-name-box .icon_location1 {
  width: 36rpx;
  height: 36rpx;
  margin-right: 16rpx;
}

.address_tag {
  padding: 0 10rpx;
  background: linear-gradient(135deg, rgba(76, 156, 255, 1) 0%, rgba(36, 134, 255, 1) 100%);
  border-radius: 4rpx;
  color: rgba(255, 255, 255, 1);
  font-size: 20rpx;
  margin-right: 10rpx;
  height: 32rpx;
  line-height: 32rpx;
  margin-top: 5rpx;
}

.lift_address .selfAddress_name {
  flex: 1;
  line-height: 48rpx;
  height: 94rpx;
}

.ui_case {
  margin: 0 20rpx;
}

.lift_info {
  color: #333333;
  font-size: 28rpx;
  padding: 30rpx 24rpx;
  align-items: center;
  -webkit-align-items: center;
}

.left {
  font-weight: bold;
  line-height: 34rpx;
}

.middle {
  text-align: right;
}

.default_txt {
  color: #666;
  font-size: 28rpx;
}

.address-tag-box {
  height: 48rpx;
  display: inline-flex;
  align-items: center;
  margin-right: 20rpx;
}

.address-tag {
  display: inline-block;
  height: 32rpx;
  width: 60rpx;
  background: linear-gradient(270deg, rgba(0, 190, 52, 1) 0%, rgba(0, 203, 30, 1) 100%);
  border-radius: 4rpx;
  color: white;
  line-height: 32rpx;
  font-size: 20rpx;
  text-align: center;
  position: relative;
}

.lift_a .address-tag {
  background: transparent !important;
}

.lift_a .address-tag::after {
  position: absolute;
  width: 200%;
  height: 200%;
  border: 1px solid #fff;
  left: 50%;
  top: 50%;
  border-radius: 8rpx;
  transform-origin: center;
  -webkit-transform-origin: center;
  transform: translate(-50%, -50%) scale(0.5);
  -webkit-transform: translate(-50%, -50%) scale(0.5);
  content: '';
}

.lift_a .address-detail, .lift_a .arrow-right, .lift_a .address-contact-box {
  color: #fff !important;
}

.address-detail {
  flex: 1;
  color: #333333;
  font-size: 40rpx;
  font-weight: bold;
  line-height: 48rpx;
}

.time_line_box {
  padding-top: 32rpx;
  background: #fff;
}

.time_line {
  height: 1px;
  background: #f5f5f5;
  transform: scaleY(0.5);
  margin: 0 24rpx;
}

.address-contact-box {
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #666666;
  font-weight: 300;
  line-height: 34rpx;
}

.address-contact-box.bold {
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
  line-height: 48rpx;
  margin-top: 0;
}

.address_person_name {
  margin-right: 15rpx;
  display: inline-block;
}

.address-phone {
  display: inline-block;
}

.arrow-right {
  line-height: 48rpx;
}

.address_desc {
  margin-top: 10rpx;
  font-size: 28rpx;
  font-weight: 300;
  color: #fff;
  line-height: 34rpx;
}

.address_desc.no-self {
  color: #666;
}

/* 送达时间 */

.settlement-delivery-container {
  background: #fff;
  background-size: 100% 6rpx;
  padding: 32rpx 24rpx;
  border-radius: 0 0 24rpx 24rpx;
  overflow: hidden;
}

.settlement-delivery-container.newStyle{
  padding-top: 48rpx;
}
 
.settlement-delivery-box {
  overflow: hidden;
}

.distribution-other {
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

.delivery-time {
  flex: 1;
  color: #348BED;
  font-size: 28rpx;
  text-align: right;
}

.settlement-delivery-container .userTipText {
  margin-top: 10rpx;
  margin-right: 42rpx;
  text-align: right;
  font-size: 24rpx;
  font-family: PingFangSC-Light, PingFang SC;
  font-weight: 300;
  color: rgba(255, 30, 25, 1);
  line-height: 30rpx;
}

.settlement-delivery-container .promiseText {
  margin-top: 10rpx;
  margin-right: 42rpx;
  /* text-align: right; */
  font-size: 24rpx;
  font-family: PingFangSC-Light, PingFang SC;
  font-weight: 300;
  color: rgb(136, 136, 136);
  line-height: 28rpx;
}

.settlement-delivery-container .offPeakTip {
  margin-top: 10rpx;
  margin-right: 42rpx;
  /* text-align: right; */
  font-size: 22rpx;
  font-family: PingFangSC-Light, PingFang SC;
  font-weight: 300;
  color: rgb(136, 136, 136);
}

.settlement-delivery-container .microLabel {
  height: 30rpx;
  position: relative;
  font-size: 20rpx;
  font-family: PingFangSC-Light, PingFang SC;
  font-weight: 300;
  line-height: 30rpx;
  padding: 0 6rpx;
  margin-left: 24rpx;
}

.settlement-delivery-container .microLabel .shin-border {
  position: absolute;
  width: 200%;
  height: 200%;
  border: 1px solid;
  left: 50%;
  top: 50%;
  border-radius: 4rpx;
  transform-origin: center;
  -webkit-transform-origin: center;
  transform: translate(-50%, -50%) scale(0.5);
  -webkit-transform: translate(-50%, -50%) scale(0.5);
}

.delivery-arrow-right {
  margin-top: 1rpx;
}

.no-radius {
  border-radius: 0;
}

/* 8.16.5 */
.self_store_name {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 36rpx;
}

.self_address {
  font-size: 40rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #333333;
  line-height: 48rpx;
  margin-top: 18rpx;
}

.self_left {
  flex: 1;
}

.self_right {
  width: 216rpx;
  box-sizing: border-box;
  position: absolute;
  top: 38rpx;
  right: 24rpx;
}

.self_right_empty {
  width: 216rpx;
  margin-left: 24rpx;
}

.self_distance {
  box-sizing: border-box;
  max-width: 216rpx;
  padding: 0 16rpx;
  height: 48rpx;
  background: #fff;
  border-radius: 32rpx;
  box-shadow: 0px 4rpx 20rpx 0px rgba(0, 0, 0, 0.1);
  position: relative;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
}

.self_distance::after {
  position: absolute;
  content: "";
  border-style: solid;
  border-width: 10rpx 10rpx 0;
  border-color: #fff transparent transparent;
  bottom: -9rpx;
}

.self_distance .self_distance_arrow {
  width: 28rpx;
  height: 28rpx;
  flex-shrink: 0;
  flex-grow: 0;
}

.self_store_icon_wrap {
  width: 80rpx;
  height: 86rpx;
  margin: 0 auto;
  background-image: url("https://storage.360buyimg.com/wximg/storewin/settle_map_zw.png");
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-position: center;
  text-align: center;
  margin-top: 8rpx;
}

.self_store_icon {
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
  margin-top: 8rpx;
  border-radius: 10rpx;
}

.new_deliver_line {
  width: 2rpx;
  height: 80rpx;
  background: linear-gradient(90deg, #F5F5F5 0%, rgba(255, 255, 255, 0) 100%);
}

.new_deliver_left,.new_deliver_right {
  width: 50%;
  overflow: hidden;
}

.ndl_font {
  font-size: 28rpx;
  line-height: 36rpx;
}

.ndl_font_family {
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
}

.ndl_font_family_bold {
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #333333;
}

.ndl_text, .ndr_text {
  margin-bottom: 16rpx;
}

.ndl-arrow-right {
  margin-left: 12rpx;
  margin-right: 16rpx;
}

.new_deliver_right {
  box-sizing: border-box;
  padding-left: 48rpx;
}
.new_deliver_right.no-padding {
  padding-left: 0;
  margin-bottom: 24rpx;
  width: 100%;
}

.new_deliver_right.no-padding .ndr_text {
  margin-bottom: 0;
  margin-right: 48rpx;
}

.ndr_phone {
  height: 36rpx;
  min-height: 36rpx;
  width: 200rpx;
}

.ndr_plac {
  font-weight: 400;
  color: #999999;
}

.ndr_bj {
  width: 24rpx;
  height: 24rpx;
}

.self_radius {
  border-radius: 24rpx 24rpx 0 0;
}

.new_promise_style {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FF1E19;
  line-height: 40rpx;
}

.map_wrap {
  border-radius: 0 0 24rpx 24rpx;
  transform: translateY(-1px);
  -webkit-transform: translateY(-1px);
}

.map_wrap.new_style {
  position: relative;
  background-color: #fff;
  background-image: url("https://storage.360buyimg.com/wximg/storewin/self_map_new.png");
  background-size: 702rpx 230rpx;
  background-position: left 0;
  background-repeat: no-repeat;
}

.map_wrap.new_style .lift_address,
.map_wrap.new_style .settlement-delivery-container {
  background: transparent;
}

.map_wrap.just_zt {
  border-radius: 24rpx 24rpx 0 0;
}