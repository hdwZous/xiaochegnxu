<view class="module_jump">
  <view class="d-flex flex-row justify-content-between">
    <view class="module_left d-flex flex-row align-items-center">
      <block wx:for="{{infos.title.textList}}">
        <text class="module_title" style="color:{{item.fontColor}};font-size:{{item.fontSize*2}}rpx;font-weight:{{item.fontWeight==1?'bold':item.fontWeight==2?'lighter':'normal'}};">{{item.text}}</text> 
      </block>
      <image wx:if="{{infos.smallQuestionMark.type}}" src="https://storage.360buyimg.com/wximg/settlement/tip.png" class="module_icon" data-items="{{infos.smallQuestionMark}}" bindtap="handleClickQuestion"/> 
    </view>
    <view class="right" bindtap="goToH5Page">
      <block wx:for="{{infos.rightText.textList}}">
        <text class="module_txt" style="color:{{item.fontColor}};font-size:{{item.fontSize*2}}rpx;font-weight:{{item.fontWeight==1?'bold':item.fontWeight==2?'lighter':'normal'}};">{{item.text}}</text> 
      </block>
      <text wx:if="{{infos.jumpAgreement}}" class="dj_icon ts_next"></text>
    </view>
  </view>
  <view class="module_content ellipsis2">
    <block wx:for="{{infos.subTitle.textList}}">
      <text class="module_txt" style="color:{{item.fontColor}};font-size:{{item.fontSize*2}}rpx;font-weight:{{item.fontWeight==1?'bold':item.fontWeight==2?'lighter':'normal'}};">{{item.text}}</text> 
    </block>
  </view>
</view>
<tip-dialog types="1" show="{{tipShow}}" readmeList="{{infos.smallQuestionMark.questionLayer.contentList}}" bindtipClose="showRule"></tip-dialog>