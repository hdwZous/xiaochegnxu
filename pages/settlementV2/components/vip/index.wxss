@import "/common/css/iconfont.wxss";

/* 红包套餐 */
.redbag_pack {
  /* background:#FF5040; */
  border-radius: 14rpx 12rpx 14rpx 14rpx;
  border: 1rpx solid #FFE9E9;
  position: relative;
}

.redbag_pack_top {
  display: flex;
}

.redbag_title_bgwrap {
  height: 50rpx;
  line-height: 50rpx;
  background: rgba(255, 247, 247, 1);
}

.redbag_title_wrap {
  width: 160rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: #FF5040;
  border-radius: 12rpx 0 10rpx 0;
  position: relative;
}

.rnImg1 {
  width: 10rpx;
  height: 56rpx;
  position: relative;
  top: 6prx;
}

.rnImg2 {
  height: 50rpx;
  line-height: 50rpx;
  display: inline-block;
  position: relative;
  top: -6rpx;
}

.rnImg3 {
  width: 10rpx;
  height: 56rpx;
}

.rnImg2_img {
  width: 28rpx;
  height: 30rpx;
  position: relative;
  left: 0rpx;
  top: 5rpx;
}

.redbag_title {
  font-size: 24rpx;
  font-weight: bold;
  color: #fff;
}

.redbag_right_empty {
  width: calc(100% - 159rpx);
  /* background: rgba(255,247,247,1); */
  position: relative;
  top: 6rpx;
  border-radius: 8rpx 0 0 0;
  background: linear-gradient(90deg, #FFECDA 0%, rgba(255, 217, 180, 1) 100%);
}

.redbag_content {
  /* background: rgba(255,247,247,1); */
  border-radius: 12rpx 0 12rpx 12rpx;
  display: flex;
  padding-top: 10rpx;
  padding-bottom: 32rpx;
  justify-content: space-between;
  overflow: hidden;
  background: linear-gradient(90deg, rgba(255, 242, 229, 1) 0%, rgba(255, 217, 180, 1) 100%);
}

.redbag_content_left {
  padding-left: 24rpx;
}

.redbag_content_tips_text {
  font-size: 28rpx;
  font-weight: bold;
  color: #333;
  padding-right: 6rpx;
}

.qu_image {
  width: 32rpx;
  height: 32rpx;
  margin-bottom: 4rpx;
  vertical-align: middle;
}

.redbag_content_rules {
  font-size: 24rpx;
  color: #333;
  padding-top: 10rpx;
}

.redbag_content_right {
  display: flex;
  align-items: center;
  padding-right: 14rpx;
}

.redbag_content_price {
  text-align: right;
}

.real_price {
  font-size: 32rpx;
  color: #FF1E19;
  font-family: regular;
  position: relative;
  right: -2rpx;
  /* font-weight: bold; */
}

.base_price {
  font-size: 24rpx;
  color: #999;
  font-family: #999;
  text-decoration: line-through;
}

.redbag_content_btn {
  padding-left: 14rpx;
}

.redbag_btn_unchecked, .redbag_btn_checked {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  -webkit-border-radius: 50%;
}


/* 新红包楼层 */
.wrap {
  background: linear-gradient(180deg, #FFFFFF 0%, #FFF2E6 100%);
  border-radius: 4px 4px 4px 4px;
  border: 1px solid #F6EFEC;
  overflow: hidden;
  position: relative;
}

.bt {
  position: absolute;
  top: 0px;
  height: 2px;
  width: 100%;
  margin-left: 4px;
  background: rgba(64, 54, 73, 1);
  border-radius: 8px 4px 0 0;
}

.wrap_header {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.wrap_header_left {
  /* width: 160rpx; */
  height: 56rpx;
  text-align: left;
  display: flex;
}

.wrap_header_left_icon {
  display: inline-block;
  height: 28px;
  width: 12rpx;
}

.wrap_header_right_icon {
  display: inline-block;
  height: 28px;
  width: 24rpx;
}

.wrap_header_title {
  display: flex;
  background: url('https://img30.360buyimg.com/mobilecms/jfs/t1/88092/3/16080/1050/5e781dfaEff50f82b/3686264aef3e4758.png') no-repeat center center;
  font-size: 24rpx;
  height: 22px;
  line-height: 22px;
  color: #FCE0A5;
  background-size: cover;
  font-weight: bold;
  align-items: center;
}

.wrap_header_title_icon {
  width: 28rpx;
  height: 28rpx;
  position: relative;
  top: 0rpx;
  padding-right: 8rpx;
}

.wrap_header_right {
  flex: auto
}

.wrap_header_right {
  font-size: 20rpx;
  color: #333;
  position: relative;
  top: -4rpx;
  padding-left: 8rpx;
}

.yellow_line {
  width: 1px;
  height: 18rpx;
  background: rgba(255, 223, 191, 1);
  display: inline-block;
  margin-left: 8rpx;
  margin-right: 8rpx;
  transform: scaleX(0.5);
  position: relative;
  top: 2rpx;
}

.yellow_line:last-child {
  background: transparent;
}

.wrap_center {
  padding: 8rpx 16rpx 20rpx 16rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between;
  align-items: baseline;
}

.wrap_center_left {
  word-break: break-all
}

.wrap_center_left_img {
  width: 32rpx;
  height: 32rpx;
  margin-left: 8rpx;
  margin-right: 16rpx;
  position: relative;
  top: 7rpx
}

.wrap_center_right {
  display: flex;
  justify-content: space-between;
  flex: auto;
  align-items: baseline;
  position: relative;
  top: -2rpx;
}

.origin_price {
  font-size: 24rpx;
  color: #999;
  font-family: light;
  text-decoration: line-through;
}

.true_price {
  font-size: 32rpx;
  font-family: regular;
  color: #3A3431;
  margin-left: 6rpx;
}

.btn_unchecked, .btn_checked {
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  -webkit-border-radius: 50%;
  display: inline-block;
  margin-left: 8rpx;
  position: relative;
  top: 8rpx
}

.wrap_bottom {
  padding: 0 16rpx;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
  white-space: nowrap;
  margin-bottom: 20rpx;
}

.wrap_bottom::-webkit-scrollbar {
  width: 0;
  display: none;
}

.coupon_item {
  background: linear-gradient(180deg, #FFE3C6 0%, #FFD2A7 100%);
  display: inline-block;
  border-radius: 8rpx;
  padding: 16rpx 0rpx 8rpx 0rpx;
  text-align: center;
  margin-right: 8rpx;
  width: 96rpx;
}

.coupon_item_price {
  font-size: 36rpx;
  color: #3A3431;
  font-family: regular;
  margin-bottom: 6rpx;
  font-weight: lighter;
}

.coupon_item_price_dash {
  width: 60%;
  margin: 0 auto;
  border: 1px dashed rgba(249, 169, 93, 1);
  transform: scaleY(0.5);
}

.coupon_item_rule {
  font-size: 20rpx;
  font-family: light;
  color: #3A3431;
  font-weight: lighter;
  text-align: center;
  margin: 0 auto;
  margin-top: 8rpx;
  box-sizing: border-box;
  padding: 0 6rpx;
}

.wrap-agreement {
  font-size: 0;
  padding: 0 16rpx;
  margin-bottom: 20rpx;
}

.wrap-agreement .wrap-agreement-txt {
  display: inline-block;
  font-size: 10px;
  font-family: PingFangSC-Regular, PingFang SC;
  color: #999999;
  line-height: 28rpx;
}

.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}