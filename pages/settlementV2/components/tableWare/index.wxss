@import "/common/css/global.wxss";
@import "/common/css/iconfont.wxss";
.tableWare {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 11;
  background: rgba(0, 0, 0, .5);
}
.tableWare_container {
  border-radius: 24rpx 24rpx 0 0;
  background-color: #ffffff;
  position: absolute;
  bottom: 0;
  right: 0;
  left: 0;
  padding: 48rpx 24rpx;
  box-sizing: border-box;
}
.tableWare_title {
  display: flex;
  align-items: center;
  margin-bottom: 24rpx;
}
.tableWare_title_text {
  font-size: 32rpx;
  color: #333333;
  font-weight: 600;
  margin-right: 8rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.tableWare_title_icon {
  width: 24rpx;
  height: 24rpx;
  margin-right: 8rpx;
}
.tableWare_ipx .tableWare_container {
  padding-bottom: 84rpx;
}
.tableWare_select {
  display: flex;
  margin-bottom: 48rpx;
}
.tableWare_select_block {
  width: 334rpx;
  height: 116rpx;
  margin-right: 34rpx;
  line-height: 116rpx;
  text-align: center;
  border-radius: 16rpx;
  border: 1px solid #EEEEEE;
  font-family: light;
  font-weight: 400;
}
.tableWare_selected_block {
  background-color: #F3FFF7;
  border: 1px solid #C0E5CA;
  font-weight: 500;
  font-family: PingFangSC-Medium,PingFang SC;
}
.tableWare_select_block:last-child {
  margin-right: 0;
}
.tableWare_number1 {
  padding: 16rpx 24rpx;
  border-radius: 8rpx;
  font-size: 24rpx;
  height: 64rpx;
  box-sizing: border-box;
  border: 1rpx solid #EEEEEE;
  min-width: 138rpx;
  text-align: center;
  margin-right: 24rpx;
}
.tableWare_number2 {
  margin-right: 24rpx;
  margin-bottom: 24rpx;
}
.tableWare_num {
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 48rpx;
}
.tableWare_number3 {
  display: flex;
  align-items: center;
  border-radius: 8rpx;
  font-size: 24rpx;
  height: 64rpx;
  margin-right: 0!important;
  border: 1rpx solid #EEEEEE;
  padding-left: 0;
  padding-right: 0;
}
.tableWare_reduce {
  margin-left: 20rpx;
  height: 64rpx;
  width: 20rpx;
  line-height: 64rpx;
  background-image: url(https://storage.360buyimg.com/wxmini/settlement/icon1.png);
  background-size: 100% auto;
  background-repeat: no-repeat;
  background-position: center;
}
.tableWare_reduce_able {
  background-image: url(https://storage.360buyimg.com/wxmini/settlement/icon4.png);
}
.tableWare_add {
  margin-right: 20rpx;
  height: 64rpx;
  line-height: 64rpx;
  width: 20rpx;
  background-image: url(https://storage.360buyimg.com/wxmini/settlement/icon3.png);
  background-size: 100% auto;
  background-repeat: no-repeat;
  background-position: center;
}
.tableWare_add_able {
  background-image: url(https://storage.360buyimg.com/wxmini/settlement/icon2.png);
}
.tableWare_followUp_text {
  font-size: 24rpx;
  color: #333333;
  font-family: 'Regular';
}
.tableWare_followUp {
  display: flex;
  align-items: center;
  margin-bottom: 32rpx;
}
.tableWare_followUp_icon {
  margin-right: 12rpx;
  display: flex;
  height: 36rpx;
  width: 36rpx;
  background-size: 100%;
}
.unify-roll-btn-on {
  background-image: url('https://storage.360buyimg.com/wximg/settleV2/check-true-green.png');
  background-repeat:  no-repeat;
}
.unify-roll-btn-off {
  background-image: url('https://storage.360buyimg.com/wximg/settleV2/check-false.png');
  background-repeat:  no-repeat;
}
.tableWare_close {
  position: absolute;
  top: 22rpx;
  right: 30rpx;
  width: 28rpx;
  height: 28rpx;
  color: #cccccc;
}
.tableWare_button {
  background: linear-gradient(270deg, #00CF37 0%, #0FDF46 100%);
}
.tableWare_num_check {
  background-color: #F3FFF7;
  border: 1px solid #C0E5CA;
  font-weight: 500;
  font-family: PingFangSC-Medium,PingFang SC;
}
.tableWare_pop {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.tableWare_pop_content {
  width: 590rpx;
  height: 332rpx;
  background-color: #ffffff;
  border-radius: 24rpx;
  padding: 54rpx 50rpx 0 50rpx;
  box-sizing: border-box;
}
.tableWare_pop_title {
  font-size: 36rpx;
  font-weight: 500;
  text-align: center;
  color: #030303;
  margin-bottom: 46rpx;
  font-family: PingFangSC-Medium,PingFang SC;
}
.tableWare_pop_text {
  color: #666666;
  font-size: 32rpx;
  font-weight: 400;
  font-family: 'Regular';
}
.tableWare_pop_close {
  width: 76rpx;
  height: 76rpx;
  background: url("https://storage.360buyimg.com/wximg/bargain/close.png") no-repeat center;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}
.tableWare_button_disable {
    width: 700rpx;
    height: 80rpx;
    border-radius: 40rpx;
    font-size: 32rpx;
    text-align: center;
    line-height: 80rpx;
    color: #ffffff;
    font-family: PingFangSC-Medium,PingFang SC;
    background-color: #cccccc;
}