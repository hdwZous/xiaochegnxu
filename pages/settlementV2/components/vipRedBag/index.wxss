.reason{
    position:fixed;
    white-space: pre-wrap;
    -webkit-white-space: pre-wrap;
    background: #fff;
    color: #ff8203;
    font-size: 26rpx;
    padding:30rpx;
    width: 360rpx;
    box-shadow: 0 4rpx 24rpx #e5bf98;
    border-radius: 6rpx;
    -webkit-border-radius: 6rpx;
    z-index:33;
    left:78rpx;
}
.reason-triggle{
    position: absolute;
    left:13rpx;
    width: 0;
    height: 0;
    border: 14rpx solid transparent;
    z-index:3;
    bottom:-20rpx;
    border-top-color:#fff;
}

