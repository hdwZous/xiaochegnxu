.confirm-dialog-bg{
    display:-webkit-flex;
    display:flex;
    -webkit-justify-content:center;
    justify-content:center;
    -webkit-align-items:center;
    align-items:center;
    z-index:11;
    position:fixed;
    top:0;
    bottom:0;
    left:0;
    right:0;
    background:rgba(0,0,0,.7);
}
.confirm-dialog-bg .confirm-dialog-body{
    background: white;
    border-radius:20rpx 20rpx 0 0;
    -webkit-border-radius:20rpx 20rpx 0 0;
    position: fixed;
    left:0;
    right: 0;
    /* top:0; */
    /* height: 100%; */
    bottom: -1000px;
    width: 100%;
    transition: all ease 0.3s;
    -webkit-transition: all ease 0.3s;
}

.confirm-dialog-body .title-box{
    text-align: center;
    position: relative;
}
.confirm-dialog-body .title-box .text{
    color: #333;
    font-size: 32rpx;
    text-align: center;
    padding-top: 30rpx;
    padding-left: 20rpx;
    font-weight: bold;
}
.confirm-dialog-bg .confirm-dialog-close{
    position: absolute;
    right: 24rpx;
    top: 30rpx;
    height: 36rpx;
    width: 36rpx;
    font-weight: 300;
    color: #333;
    font-size: 26rpx;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    -webkit-align-items: center;
    justify-content: center;
    -webkit-justify-content: center;
}
.card-box{
    padding-left: 24rpx;
    padding-right: 24rpx;
    align-items: center;
    -webkit-align-items: center;
    /* margin-bottom: 30rpx; */
    box-sizing: border-box;
    padding-bottom: 30rpx;
}

