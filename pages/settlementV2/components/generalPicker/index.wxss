
.flex_row {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
}
.flex_col {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
}

.flex_none {
    -webkit-flex: none;
    flex: none;
}

.flex_auto {
    -webkit-flex: auto;
    flex: auto;
}
.wrap{
    margin-top: 10rpx;
    width: 100%;

}
.scroll_box{
    width: 100%;
    height: 100%;
}
.scroll_item{
    /*height:34rpx;*/
    font-size:28rpx;
    color:#333333;
    line-height:34rpx;
    padding: 20rpx ;
    width: 100%;
    box-sizing: border-box;
}
.circle_grey{
    width:36rpx;
    height:36rpx;
    /*background:rgba(255,255,255,1);*/
    box-shadow:0rpx 2rpx 6rpx 0rpx rgba(201,192,192,0.16);
    border:4rpx solid rgba(204,204,204,1);
    border-radius: 50%;
    box-sizing: border-box;
}
.circle_green{
    display: flex;
    display: -webkit-flex;
