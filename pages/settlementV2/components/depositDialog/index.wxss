.brand_dialog {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 20;
}

.dialog_content {
  width: 540rpx;
  background: #FFFFFF;
  border-radius: 24rpx;
  text-align: center;
}

.dialog_content .dialog_title {
  padding: 80rpx 40rpx 20rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: bold;
  color: #333333;
  line-height: 44rpx;
}

.dialog_content .dialog_agreement {
  padding: 0 40rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Light, PingFang SC;
  font-weight: 300;
  color: #999999;
  line-height: 30rpx;
  text-align: center;
}

.dialog_content .btn-box {
  border-top: 1px solid #F5F5F5;
  height: 100rpx;
  margin-top: 32rpx;
}

.btn-box .btn_line {
  width: 1px;
  height: 100rpx;
  background: #F5F5F5;
}

.btn-box .btn {
  flex: 1;
  font-size: 36rpx;
  text-align: center;
  line-height: 100rpx;
}

.btn-box .btn_left {
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
}

.btn-box .btn_right {
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #00CF30;
}