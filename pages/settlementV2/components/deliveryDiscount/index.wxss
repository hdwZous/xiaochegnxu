}

.bottom-wrap {
  position: fixed;
  bottom: 0;
  left: 0;
}

.bottom-box {
  width: 750rpx;
  height: 112rpx;
  background: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: space-between;
  text-align: center;
  line-height: 80rpx;
  position: relative;
}

.select-content {
  margin-left: 24rpx;
  font-size: 24rpx;
