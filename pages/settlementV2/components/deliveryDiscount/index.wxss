  border-radius: 8rpx 2rpx 8rpx 2rpx;
  border: 1rpx solid rgba(52, 139, 237, 1);
  height: 32rpx;
  line-height: 32rpx;
  transform: scale(0.8);
  position: relative;
  top: -10rpx;
  left: -4rpx;
}

.coupon-avilable-date {
