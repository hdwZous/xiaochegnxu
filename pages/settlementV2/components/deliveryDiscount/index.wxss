.deliver-confirm-dialog-bg {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-align-items: center;
  align-items: center;
  z-index: 12;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, .7);
}

.deliver-confirm-dialog-bg .confirm-dialog-body {
  background: white;
  border-radius: 20rpx 20rpx 0 0;
  -webkit-border-radius: 20rpx 20rpx 0 0;
  position: fixed;
  left: 0;
  right: 0;
  bottom: -1000px;
  /* height: 0%; */
  width: 100%;
  /* transition: height 0.2s;
    -webkit-transition: height 0.2s; */
  transition: all .2s ease-out;
  -webkit-transition: all .2s ease-out;
  height: 80vh;
}

.confirm-dialog-body .title-box {
  text-align: center;
  position: relative;
  margin-bottom: 30rpx;
}

.confirm-dialog-body .title-box .text {
  color: #333;
  font-size: 32rpx;
  text-align: center;
  padding-top: 30rpx;
  padding-left: 20rpx;
  font-weight: bold;
}

.deliver-confirm-dialog-bg .confirm-dialog-close {
  position: absolute;
  right: 24rpx;
  top: 42rpx;
  height: 36rpx;
  width: 36rpx;
  font-weight: 300;
  color: #333;
  font-size: 26rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  -webkit-align-items: center;
  justify-content: center;
  -webkit-justify-content: center;
}

