<view class="vip_card_case" wx:if="{{memberFloorInfoVO.contentVos && memberFloorInfoVO.contentVos.length}}">
  <view class="vip_card flex-row" 
        style="background-color: #FFE4B5;background-image:url({{memberFloorInfoVO.bgImgUrl}});background-size:100% 100%;background-repeat: no-repeat;" 
        bindtap="clickCard" 
        data-item="{{memberFloorInfoVO}}">
    <view class="vip_left flex-row flex-auto" aria-hidden="true">
      <image class="vip_img" src="{{memberFloorInfoVO.logo}}" wx:if="{{memberFloorInfoVO.logo}}" />
      <!--  style="max-width: {{memberFloorInfoVO.bestPrice ? '260rpx' : '372rpx'}}" -->
      <view class="vip_desc ellipsis">
        <text wx:for="{{memberFloorInfoVO.contentVos}}"
              wx:for-item="item"
              wx:key="index"
              style="color: {{item.color || '#881A0A'}}">{{item.desc}}</text>
      </view>
      <view class="vip_desc_price"  wx:if="{{memberFloorInfoVO.bestPrice && memberFloorInfoVO.bestPrice != 'null'}}">
        <image class="icon" src="https://storage.360buyimg.com/wximg/productDetail/productDetail_pricebg.png"/>
        <view class="price">Â¥{{memberFloorInfoVO.bestPrice}}</view>
      </view>
    </view>
    <view class="vip_btn flex-none flex-row" aria-hidden="true">
      <view class="vip_btn_text ellipsis" style="color:{{memberFloorInfoVO.buttonTipsColor || '#881A0A'}}">
        {{memberFloorInfoVO.buttonTips}}
      </view>
      <image class="vip_arrow" src="{{memberFloorInfoVO.buttonArrow ? memberFloorInfoVO.buttonArrow : 'https://storage.360buyimg.com/wximg/productDetail/productDetail_arrow.png'}}" />
    </view>
  </view>
</view>