.product_coupon_wrap {
  background: #fff;
    padding: 32rpx 24rpx;
    border-bottom: 1px solid rgb(245, 245, 245);
}
.product_coupon_title {
    align-items: baseline;
}
.product_coupon_left {
    color: #333333;
    font-size: 28rpx;
    font-weight: bold;
}
.product_coupon_right {
    margin-left: 8rpx;
    color: #999999;
    font-size: 24rpx;
}
.product_coupon_one {
    margin-right: 20rpx;
    position: relative;
    align-items: center;
    margin-top: 16rpx;
    height: 160rpx;
    border-radius: 14rpx;
    background: linear-gradient(129deg, #ff6640 0%, #ff3737 100%);
}
.product_coupon_one::before {
    content: "";
    position: absolute;
    left: -12rpx;
    top: 50%;
    transform: translateY(-50%);
    width: 24rpx;
    height: 24rpx;
    border-radius: 24rpx;
    background: #ffffff;
}
.product_coupon_one::after {
    content: "";
    position: absolute;
    right: -12rpx;
    top: 50%;
    transform: translateY(-50%);
    width: 24rpx;
    height: 24rpx;
    border-radius: 24rpx;
    background: #ffffff;
}
.product_coupon_one_left {
    align-items: baseline;
    width: 184rpx;
    justify-content: center;
}
.product_coupon_one_symbol {
    color: #ffffff;
    font-size: 28rpx;
    font-family: "regular";
}
.product_coupon_one_price {
    color: #ffffff;
    font-size: 56rpx;
    font-family: "regular";
}
.product_coupon_one_mid_title {
    color: #ffffff;
    font-size: 36rpx;
}
.product_coupon_one_mid_sub_title {
  height: 32rpx;
  align-items: center;
  margin-top: 16rpx;
  /* flex-wrap: wrap;
  overflow: hidden; */
}
.product_coupon_one_mid_sub_title_txt {
    color: #ffffff;
    font-size: 28rpx;
    height: 32rpx;
    line-height: 32rpx;
    max-width: 100%;
    overflow: hidden;
}
.product_coupon_one_mid_sub_title_tag {
    padding: 0 8rpx;
    margin-left: 8rpx;
    color: rgba(255, 255, 255, 0.69);
    font-size: 16rpx;
    border-radius: 4rpx;
    height: 24rpx;
    line-height: 26rpx;
    box-sizing: border-box;
}
.product_coupon_one_right {
    padding: 0 32rpx 0 8rpx;
}
.product_coupon_one_right_btn {
    width: 112rpx;
    height: 48rpx;
    line-height: 48rpx;
    background: #fff0df;
    box-shadow: 0 4rpx 8rpx 0 rgba(255, 88, 67, 0.21);
    border-radius: 30rpx;
    color: #fb3737;
    font-size: 24rpx;
    text-align: center;
    border: 1px solid transparent;
}
.product_coupon_one_right_btn_active {
    width: 112rpx;
    height: 48rpx;
    line-height: 48rpx;
    border-radius: 30rpx;
    color: #fff0df;
    font-size: 24rpx;
    text-align: center;
    border: 1px solid #fff0df;
}
.product_coupon_one_right_btn_disable {
    width: 112rpx;
    height: 48rpx;
    line-height: 48rpx;
    border-radius: 30rpx;
    background: rgba(255, 240, 223, 0.5);
    color: rgba(255, 30, 25, 0.5);
    font-size: 24rpx;
    text-align: center;
    border: 1px solid transparent;
}
.product_coupon_more {
    margin-top: 16rpx;
}
.product_scroll {
    height: 140rpx;
}
.product_coupon_list {
    height: 140rpx;
}
.product_coupon_item {
    padding: 0 16rpx;
    margin-right: 16rpx;
    width: 248rpx;
    overflow: hidden;
    height: 100%;
    border-radius: 14rpx;
    background: linear-gradient(129deg, #ff6640 0%, #ff3737 100%);
}
.product_coupon_item_price {
    padding-top: 10rpx;
    align-items: center;
}
.product_coupon_item_price_case {
    align-items: baseline;
}
.product_coupon_item_symbol {
    font-size: 28rpx;
    color: #ffffff;
    font-family: "regular";
}
.product_coupon_item_num {
    font-size: 40rpx;
    color: #ffffff;
    font-family: "regular";
}
.product_coupon_btn {
    text-align: center;
    width: 76rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: #fff0df;
    box-shadow: 0 4rpx 8rpx 0 rgba(255, 88, 67, 0.21);
    border-radius: 30rpx;
    color: #ff1e19;
    font-size: 20rpx;
    border: 1px solid transparent;
}
.product_coupon_btn_active {
    text-align: center;
    width: 76rpx;
    height: 40rpx;
    line-height: 40rpx;
    border-radius: 30rpx;
    color: #fff0df;
    font-size: 20rpx;
    border: 1px solid #fff0df;
}
.product_coupon_btn_disable {
    text-align: center;
    width: 76rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: rgba(255, 240, 223, 0.5);
    border-radius: 30rpx;
    color: rgba(255, 30, 25, 0.5);
    font-size: 20rpx;
    border: 1px solid transparent;
}
.product_coupon_item_info {
  flex-wrap: wrap;
    align-items: center;
    overflow: hidden;
    height: 24rpx;
}
.product_coupon_item_info_txt {
    font-size: 20rpx;
    color: #ffffff;
    height: 24rpx;
    line-height: 24rpx;
    max-width: 100%;
    overflow: hidden;
}
.product_coupon_item_info_tag {
    padding: 0rpx 6rpx;
    margin-left: 8rpx;
    border-radius: 4rpx;
    /* border: 1px solid rgba(255, 255, 255, 0.69); */
    color: rgba(255, 255, 255, 0.69);
    font-size: 16rpx;
    height: 24rpx;
    line-height: 24rpx;
    /* box-sizing: border-box; */
}
.product_coupon_item_line {
    position: relative;
    padding-top: 10rpx;
    border-bottom: 1px dashed rgba(255, 255, 255, 0.4);
}
.product_coupon_item_line::before {
    content: "";
    position: absolute;
    left: -28rpx;
    top: 50%;
    margin-top: 4rpx;
    transform: translateY(-50%);
    width: 24rpx;
    height: 24rpx;
    border-radius: 24rpx;
    background: #ffffff;
}
.product_coupon_item_line::after {
    content: "";
    position: absolute;
    right: -28rpx;
    top: 50%;
    margin-top: 4rpx;
    transform: translateY(-50%);
    width: 24rpx;
    height: 24rpx;
    border-radius: 24rpx;
    background: #ffffff;
}
.product_coupon_item_name {
    padding: 6rpx 0;
    font-size: 20rpx;
    color: rgba(255, 255, 255, 0.69);
}

.coupon-wrap {
  flex-wrap: wrap;
  flex: auto;
  height: 24rpx;
  display: flex;
  overflow: hidden;
}

/* 黑名单不可用券文案 */
.disabled-test {
  height: 32rpx;
  font-size: 24rpx;
  color: #999999;
  line-height: 32rpx;
  margin-top: 16rpx;
}