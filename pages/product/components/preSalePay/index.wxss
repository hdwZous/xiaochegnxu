.pre-sale-pay-wrap {
    width: 100%;
    height: 100rpx;
    background: #FFFFFF;
    border: 2rpx solid #F6F6F6;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 3;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}
.hand-price-desc {
    font-size: 24rpx;
    font-weight: 400;
    color: #666666;
}
.hand-price {
    font-size: 44rpx;
    font-weight: 400;
    color: #FF1E19;
    font-family: regular;
    margin-left: 16rpx;
}
.submit-desc {
    width: 220rpx;
    height: 100%;
    line-height: 300%;
    color: #fff;
    text-align: center;
    font-size: 32rpx;
    font-weight: 400;
    margin-left: 20rpx;
}
