
<view wx:if="{{showPop && memberFloorInfoVO.memberAgreementVO}}" class='content'>
    <view class="title">{{memberFloorInfoVO.memberAgreementVO.popUpHint}}</view>
    <view class='sub-title'>
        {{memberFloorInfoVO.memberAgreementVO.popUpMsg}}
        <text class='protocal'  bindtap="jumpHref" data-item="{{memberFloorInfoVO.memberAgreementVO}}">{{memberFloorInfoVO.memberAgreementVO.memberAgreementTitle}}</text>
    </view>
    <view class='button-group'>
        <view class="button borderR" 
              bindtap="cancel">
              {{memberFloorInfoVO.memberAgreementVO.buttonLeft}}
        </view>
        <view class="button confirm"
              style="color: {{memberFloorInfoVO.memberAgreementVO.buttonRightColor}}" 
              bindtap="openVip">
            {{memberFloorInfoVO.memberAgreementVO.buttonRight}}
        </view>
    </view>
</view>
<view wx:if="{{showPop}}" class='mask'></view>