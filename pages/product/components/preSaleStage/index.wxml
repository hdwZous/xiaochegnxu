<view class='pre-sale-wrap' wx:if="{{stage && stage.length > 0}}">
    <view class='stage-item'
         wx:for="{{stage}}"
         wx:for-item="item"
         wx:key="index"
         aria-role="text" 
         aria-label="预售{item.title}}.{{item.desc}}">{{item.title}}
        <view class='stage-title' aria-hidden="true"></view>
        <view class='stage-desc' aria-hidden="true">
            <text decode="true">{{item.desc}}</text>
        </view>
    </view>
</view>