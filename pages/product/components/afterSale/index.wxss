.aftersale_case {
  background: #f9f9f9;
  box-shadow: 0px 2rpx 8rpx 0px rgba(0, 0, 0, 0.02);
  border-radius: 0px 0px 24rpx 24rpx;
  width: 100%;
  padding: 24rpx 24rpx 8rpx 24rpx;
}
.aftersale_content {
  display: flex;
  flex-wrap: wrap;
  /* margin: 24rpx 0; */
}
.aftersale_item {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-right: 32rpx;
  margin-bottom: 16rpx;
  /* height: 28rpx; */
  /* background: aqua; */
}
.aftersale_item_title {
  /* height: 28rpx; */
  font-size: 22rpx;
  font-weight: 400;
  color: #666666;
  /* line-height: 28rpx; */
  display: inline-table;
}
.aftersale_item_icon {
  width: 28rpx;
  height: 28rpx;
  margin-left: 4rpx;
  box-sizing: border-box;
  /* vertical-align: middle; */
  /* display: flex;
  justify-content: center;
  align-items: center; */
}
.icon_url {
  display: block;
  width: 20rpx;
  height: 20rpx;
  /* vertical-align: top; */
  margin-left: 4rpx;
}