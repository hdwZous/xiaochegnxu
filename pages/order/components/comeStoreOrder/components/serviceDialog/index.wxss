.mask{
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 99;
}
.wrap_service{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate3d(-50%,-50%,0);
    overflow: scroll;
    width: 78%;
}
.container_service{
    max-height: 50%;
    border-radius: 16rpx;
    background: #fff;
    padding-top: 40rpx;
    padding-bottom: 40rpx;
    padding-left: 40rpx;
    padding-right: 40rpx;
}
.title{
    font-size: 36rpx;
    font-weight: bold;
    line-height: 44rpx;
    text-align: center;
    color: rgb(51,51,51);
    padding-bottom: 30rpx;
}
.tel_wrap{
    display: flex;
    font-size: 28rpx;
}
.name{
    color: #333;
}
.tel{
    color: #348BED;
    margin-left: 14rpx;
}
.desc_service{
    font-size: 28rpx;
    color: #666;
    margin-top: 20rpx;
}
.content_service{
    max-height: 400rpx;
    overflow: scroll;
    -webkit-overflow-scrolling: touch
}
.close_wrap{
    margin-top: 40rpx;
    width: 100%;
    display: flex;
    justify-content: center;
}
.close {
    width: 74rpx;
    height: 74rpx;
    background: url("https://storage.360buyimg.com/wximg/bargain/close.png") no-repeat center;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}
