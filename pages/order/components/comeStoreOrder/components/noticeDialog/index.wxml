<view class="mask" wx:if="{{noticeDialogVisible && initData && initData.title}}" catch:tap="close">
    <view class="wrap">
    <view class="container">
        <view class="title">{{initData.title}}</view>
        <view class="content01" wx:if="{{initData.childMoneyList}}">
            <view class="price_item header">
                <view class="name">总费用</view>
                <view class="price">{{initData.totalMoney || initData.receivableMoney || initData.value}}</view>
            </view>
            <view class="item_wrap">
                <view class="price_item" wx:for="{{initData.childMoneyList}}" wx:key="index">
                    <view class="name">{{item.name}}</view>
                    <view class="price">{{item.value}}</view>
                </view>
            </view>
            <view class="footer" wx:if="{{initData.titleTipValue}}">
                <view class="desc">{{initData.titleTipTitle}}</view>
                <rich-text class="desc_content" nodes="{{initData.titleTipValue}}"></rich-text>
            </view>
        </view>
        <view class="content02" wx:else>
            <rich-text class="desc_content" nodes="{{initData.titleTipValue}}" wx:if="{{initData.titleTipValue}}"></rich-text>
        </view>
    </view>
    <view class="close_wrap">
       <view class="close"></view>
    </view>
    </view>
</view>