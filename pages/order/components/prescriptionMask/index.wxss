@import '/common/css/light.wxss';
.mask_prescription {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 50;
  display: flex;
  align-items: center;
  background:rgba(0,0,0,0.8);
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.mask_prescription_img {
  position: fixed;
  left: 0;
  top:0;
  background-repeat: no-repeat;
  height: 100vh;
  background-size: contain;
  background-position: center;
  width: 100vw;
  z-index: 51;
}
.close_prescription{
  width: 100%;
  height: 74rpx;
  position: fixed;
  bottom: 104rpx;
  display: flex;
  justify-content: center;
  z-index: 52;
}
.close_prescription_img{
  width: 74rpx;
  height: 74rpx;
}