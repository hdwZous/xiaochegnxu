.feeds_title_case {
    align-items: center;
    justify-content: center;
}
.feeds_img {
    width: 292rpx;
    height: 44rpx;
    vertical-align: middle;
}
.feeds_txt {
    color: rgba(51, 51, 51, 1);
    font-size: 32rpx;
}
.list_wrap {
    padding-top: 16rpx;
}
.list_case {
    flex-wrap: wrap;
    padding: 0 16rpx;
}
.list_item_case {
    width: 50%;
    padding: 20rpx 8rpx 0;
    box-sizing: border-box;
}
.list_item {
    margin-bottom: 16rpx;
}
.ad_banner {
    width: 702rpx;
    height: 180rpx;
    margin: 0 auto 16rpx;
    border-radius: 16rpx;
}
.ad_banner_img {
    width: 100%;
    height: 100%;
    border-radius: 16rpx;
    vertical-align: middle;
}
.list_second_tab {
    margin: 0 24rpx;
    flex-wrap: wrap;
    background: #ffffff;
    border-radius: 24rpx;
    padding: 24rpx 12rpx 1rpx;
}
.list_second_case {
    width: 25%;
    justify-content: center;
}
.list_second_tab_item {
    width: 150rpx;
    height: 64rpx;
    line-height: 64rpx;
    background: #f6f6f6;
    border-radius: 32rpx;
    font-size: 24rpx;
    color: rgba(102, 102, 102, 1);
    text-align: center;
    margin-bottom: 24rpx;
}
.list_second_tab_item_active {
    background: rgba(0, 207, 55, 0.1);
    color: rgba(0, 207, 56, 1);
}
.reach_bottom {
    font-size: 24rpx;
    color: rgb(51, 51, 51);
    text-align: center;
    padding: 60rpx 0;
}