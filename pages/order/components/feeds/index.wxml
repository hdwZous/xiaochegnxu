<block wx:if="{{showFeed}}">
    <!-- feed标题 -->
    <view class="feeds_title_case flex-row" 
          aria-role="text"
          aria-label="{{titleText}}"
          wx:if="{{title}}">
        <image aria-hidden="true" class="feeds_img flex-none" src="{{title}}" />
    </view>
    <!-- feed流 -->
    <view class="list_wrap feeds-sku" id="feeds-sku">
        <view class="list_case flex-row">
            <view class="list_item_case flex-none">
                <block wx:for="{{leftList}}" wx:for-item="item" wx:for-index="index" wx:key="item">
                    <view class="list_item">
                        <!-- 商品 type:1 -->
                        <goods wx:if="{{item.type == '1'}}" fromPositon="order-feeds" item="{{item.data}}" traceId="{{traceId}}" pageId="{{pageId}}" currentPageName="{{currentPageName}}" prePageName="{{prePageName}}" buriedObj="{{recommendObj}}"/>
                        <!-- 热门店铺 type:2 -->
                        <hot-store wx:if="{{item.type == '2'}}" fromPositon="order-feed2" item="{{item.data}}" buriedObj="{{recommendObj}}"/>
                        <!-- 分类 type:3 属于推荐下发，单独调接口，前端自己拼，不做。 -->
                        <!-- type:4 8.5以下是这个，以上的改成type7。这个不做 -->
                        <!-- 特色商品 type:5 -->
                        <local-goods wx:if="{{item.type == '5'}}" 
                        fromPositon="order-feeds" item="{{item.data}}" buriedObj="{{recommendObj}}"/>
                        <!-- type:6 医药才会有，首页不会有，不做。 -->
                        <!-- banner type:7 -->
                        <feed-banner
                            wx:if="{{item.type == '7' && item.data && item.data.resources.length > 0}}"
                            item='{{item}}'
                            pageId="{{pageId}}"
                            currentPageName="{{currentPageName}}"
                            prePageName="{{prePageName}}" 
                            traceId="{{traceId}}"
                            buriedObj="{{recommendObj}}"
                        ></feed-banner>
                        <!-- 战略宫格 type:8 -->
                        <strategy-little wx:if="{{item.type === '8'}}" item="{{item.data.resource[0]}}" />
                        <!-- 图文宫格 type:9 -->
                        <grid-little wx:if="{{item.type == '9'}}" fromPositon="order-feeds2" list="{{item.data.resource}}" buriedObj="{{recommendObj}}"/>
                    </view>
                </block>
            </view>
            <view class="list_item_case flex-none">
                <block wx:for="{{rightList}}" wx:for-item="item" wx:for-index="index" wx:key="item">
                    <view class="list_item">
                        <!-- 商品 type:1 -->
                        <goods wx:if="{{item.type == '1'}}" fromPositon="order-feeds2" item="{{item.data}}" traceId="{{traceId}}" pageId="{{pageId}}" currentPageName="{{currentPageName}}" prePageName="{{prePageName}}" buriedObj="{{recommendObj}}"/>
                        <!-- 热门店铺 type:2 -->
                        <hot-store wx:if="{{item.type == '2'}}"
                            fromPositon="order-feed" item="{{item.data}}" buriedObj="{{recommendObj}}"/>
                        <!-- 特色商品 type:5 -->
                        <local-goods wx:if="{{item.type == '5'}}" fromPositon="order-feeds2" item="{{item.data}}" buriedObj="{{recommendObj}}"/>
                        <!-- banner type:7 -->
                        <feed-banner
                            wx:if="{{item.type == '7' && item.data && item.data.resources.length > 0}}"
                            item='{{item}}'
                            pageId="{{pageId}}"
                            currentPageName="{{currentPageName}}"
                            prePageName="{{prePageName}}" 
                            traceId="{{traceId}}"
                        ></feed-banner>
                        <!-- 战略宫格 type:8 -->
                        <strategy-little wx:if="{{item.type == '8'}}" item="{{item.data.resource[0]}}" buriedObj="{{recommendObj}}" />
                        <!-- 图文宫格 type:9 -->
                        <grid-little wx:if="{{item.type == '9'}}" fromPositon="order-feeds" list="{{item.data.resource}}" buriedObj="{{recommendObj}}"/>
                    </view>
                </block>
            </view>
        </view>
    </view>
    <!-- 已经到底啦提示 -->
    <view wx:if="{{isLastPage}}" class="reach_bottom">已经到底啦</view>
</block>