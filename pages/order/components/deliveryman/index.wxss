
.container{
    padding: 32rpx 26rpx 16rpx 24rpx;
    background: #fff;
    border-radius: 24rpx;
}
.delivery_top {
    display: flex;
    justify-content: space-between;
    margin-bottom: 24rpx;
}
.delivery_bottom {
    display: flex;
    margin-top: 16rpx;
}
.delivery_man_msg {
    flex: 1;
    display: flex;
}
.delivery_man_contanier {
    flex: 1;
}
.delivery_msg_title {
    color: #666;
    font-size: 20rpx;
    max-height: 56rpx;
    margin-bottom: 4rpx;
    overflow: hidden;
    font-family: PingFangSC-Regular;
}
.delivery_msg_content {
    font-size: 24rpx;
    color: #333;
    max-height: 70rpx;
    overflow: hidden;
    font-family: PingFangSC-Regular;
}
.delivery_man_explanation {
    width: 56rpx;
    display: flex;
    align-items: center;
}
.delivery_man_exp_text {
    font-size: 20rpx;
    color: #999;
    margin-right: 4rpx;
}
.delivery_exp_arrow {
    width: 8rpx;
    height: 16rpx;
}
.left{
    display: flex;
}
.avatar{
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}
.delivery_wrap{
    margin-left: 26rpx;
    display: flex;
    align-items: center;
}
.name{
    color:#333;
    font-size: 28rpx;
    /* margin-top: 4rpx; */
}
.type{
    height: 28rpx;
    line-height: 28rpx;
    color: #fff;
    border-radius:8rpx 4rpx 8rpx 2rpx;
    font-size: 18rpx;
    text-align: center;
    margin-top: 2rpx;
    padding: 0 8rpx;
    border: 2rpx solid;
}
.selfSend{
    background: #ccc;
    /* margin-top: 22rpx; */
}
.right{
    margin-top: 16rpx;
}
.right image{
    width: 32rpx;
    height: 32rpx;
    position: relative;
    top: 6rpx;
    margin-right: 8rpx;
}
.contact{
    font-size: 28rpx;
    color: #333333;
}
.line {
    width: 662rpx;
    height: 2rpx;
    margin: 0 auto;
    background: linear-gradient(360deg, #F5F5F5 0%, rgba(255, 255, 255, 0) 100%);
}
.exp_pop {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: rgba(0, 0, 0, .7);
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 15;
}
.exp_pop_contanier {
    width: 540rpx;
    /* height: 562rpx; */
    background: #FFFFFF;
    border-radius: 24rpx;
    padding-top: 36rpx;
    padding-right: 36rpx;
    padding-left: 36rpx;
    padding-bottom: 30rpx;
}
.exp_pop_title {
    color: #030303;
    font-size: 36rpx;
    text-align: center;
    margin-bottom: 24rpx;
}
.exp_pop_content {
    font-size: 28rpx;
    color: #666666;
    margin-bottom: 70rpx;
    white-space:pre-wrap;
}
.exp_pop_button {
    color: #00CF37;
    font-size: 36rpx;
    text-align: center;
}