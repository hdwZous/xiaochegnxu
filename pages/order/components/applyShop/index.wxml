<view class="applyShop">
    <view class="title">
        <view class="left">{{locStoreFloorInfo.title}}</view>
        <view class="right" catchtap="toAllStore">
            {{locStoreFloorInfo.buttonInfo.title}}
            <image src="https://storage.360buyimg.com/wximg/order/right.png" mode="widthFix" />
        </view>
    </view>
    <view class="shopInfo">
        <view class="shopInfoBox">
            <view class="shopName ellipsis">{{locStoreFloorInfo.locStoreInfo.storeName}}</view>
            <view class="address">
                <view class="distance">
                    <image class="distanceIcon" src="https://storage.360buyimg.com/wximg/order/location.png" mode="widthFix"/>
                    {{locStoreFloorInfo.locStoreInfo.distance}}
                </view>
                <view class="addressText">
                    {{locStoreFloorInfo.locStoreInfo.storeAddress}}
                </view>
            </view>
        </view>
        <view class="callUp" bind:tap="callUp">
               <image class="callUpIcon" src="https://storage.360buyimg.com/wximg/order/phone.png" mode="widthFix"/>
        </view>
    </view>
</view>