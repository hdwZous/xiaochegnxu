
@import '/common/css/global.wxss';
@import '/common/css/light.wxss';
@import '/common/css/regular.wxss';

page{
    background: #f8f8f8;
}
.container{
    padding:0 20rpx;
    height: 100%;
}
.order_status_wrap{
    text-align: center;
    background: #fff;
    margin-top: 30rpx;
    border-radius: 6px;
}
.order_status_title{
    padding-top: 40rpx;
    color: #333;
    font-size: 36rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
}
.order_status_title image{
    width: 20rpx;
    height: 20rpx;
    position: relative;
    top:-5rpx;
    left:-4rpx;
}
.order_status_content{
    color: #999;
    font-size: 26rpx;
    padding-top: 12rpx;
    padding-bottom: 30rpx;
}
.order_send_time{
    color: #666;
    padding-bottom: 30rpx;
    font-size: 26rpx;
    font-family: light;
}

/*配送员信息 订单列表信息 , 配送信息*/
.deliveryman_wrap,.orderInfoList_wrap,.delivery_info_wrap,.order_info_wrap{
    margin-top: 20rpx;
}
.order_info_wrap{
    margin-bottom: 154rpx;
}

.footer{
   position: fixed;
   left:0;
   right: 0;
   bottom: 0;
   width: 100%;
   z-index: 99;
   background: #fff;
}
.footer image{
   width:100%;
   height: 114rpx;
   /* margin-bottom: 6rpx; */
   margin-bottom: 54rpx;
}












