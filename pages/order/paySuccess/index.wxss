page {
    background: #f8f8f8;
}

.container {
    width: 100%;
    background: #f8f8f8;
    background: linear-gradient(360deg, #57D160 0%, #25C156 100%);
    margin-top: 34rpx;
}

.draw-container {
    width: 100%;
    padding-bottom: 36rpx;
    background: transparent;
}

.top {
    margin: 20rpx 0;
    position: relative;
}

.top .title {
    font-size: 28rpx;
    color: #fff;
    font-weight: bold;
    padding-left: 30rpx;
}

.dash-line {
    width: 690rpx;
    border-bottom: dashed 2rpx rgba(255, 255, 255, 0.6);
    margin: 0 auto;
}

.top .rule-text {
    position: absolute;
    right: 30rpx;
    top: 5rpx;
    line-height: 24rpx;
}

.top .rule {
    font-size: 24rpx;
    color: #fff;
}

.top .icon-next {
    font-size: 24rpx;
    color: #fff;
    margin-left: 10rpx;
}

.wrapper {
    width: 690rpx;
    height: 504rpx;
    border-radius: 17rpx;
    margin: 0 auto;
    background: #17A822;
}


/* 砍订单弹层 */
.dialog_content {
    width: 674rpx;
    background: url('https://storage.360buyimg.com/wximg/hackOrder/paySuccessPopBg.png') no-repeat center;
    background-size: 100%;
    height: 952rpx;
    text-align: center;
    transform: translateY(-20rpx);
}

.dialog_content .head {
    font-size: 60rpx;
    font-weight: 500;
    color: #fff;
    font-family: 'regular';
    padding-top: 240rpx;
    letter-spacing: 4rpx;
}

.dialog_content .money {
    color: #FFDC84;
    font-weight: bold;
    font-size: 72rpx;
}

.dialog_content .next_title {
    margin: 52rpx 84rpx 0;
    color: #fff;
    font-size: 36rpx;
    padding: 0 36rpx;
    height: 66rpx;
    line-height: 66rpx;
    background: rgba(188, 0, 32, 0.4);
    border-radius: 33px;
}

.dialog_content .hack_btn {
    margin: 18rpx 60rpx 0;
    background: linear-gradient(180deg, rgba(255, 229, 118, 1) 0%, rgba(255, 189, 68, 1) 100%);
    box-shadow: 0px 4px 16px 0px rgba(255, 81, 81, 0.6);
    border-radius: 44px;
    color: #672E2E;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 48rpx;
    font-weight: 500;
}

.dialog_content .process {
    width: 534rpx;
    margin-top: 40rpx;
}

.cutOrderBg {
    margin: -10rpx auto;
    display: block;
    width: 500rpx;
}

.dadaBg {
    margin: -28rpx auto;
    display: block;
    width: 500rpx;
}

.dadaBgAndCutOrder {
    margin: -10rpx auto;
    display: block;
    /* height: 200rpx; */
    width: 576rpx;
}

.discount {
    background: url('https://storage.360buyimg.com/wxmini/map/bg-discount.png') no-repeat center;
    background-size: 100%;
    width: 750rpx;
    height: 160rpx;
    text-align: center;
    margin: 0 50rpx;
    display: flex;
    justify-content: center;
    align-items: baseline;
    font-size: 40rpx;
    color: white;
    letter-spacing: 1px;
    padding-top: 34rpx;
    box-sizing: border-box;
}

.discount .price {
    font-size: 72rpx;
    color: #FFE178;
}

.discount-only-wrap {
    background: linear-gradient(180deg, #FFFFFF 0%, #FFEAE6 100%);
    border-radius: 24rpx;
    padding: 80rpx 0 0 40rpx;
    width: 550rpx;
    height: 344rpx;
    box-sizing: border-box;
    margin-top: -128rpx;
}

.bg-dog {
    width: 622rpx;
    height: 344rpx;
    position: absolute;
    z-index: 0;
    top: -128rpx;
    left: 0;
}

.discount-only {
    font-size: 40rpx;
    /* font-family: PingFangSC-Semibold, PingFang SC; */
    font-family: JDDJ-1-Regular, JDDJ-1;
    font-weight: 600;
    color: #640B09;
    position: relative;
    z-index: 99;
}

.discount-only .price {
    display: flex;
    align-items: baseline;
    font-size: 96rpx;
    color: #FF1E19;
}

.discount-only .price .cent {
    font-size: 40rpx;
}

.discount-only-wrap .btn-close {
    background: rgba(102, 102, 102, 0.9);
    border-radius: 64rpx;
    border: 1px solid #FFFFFF;
    font-size: 50rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #FFFFFF;
    padding: 30rpx 80rpx;
    width: 200rpx;
    position: absolute;
    left: 50%;
    bottom: -166rpx;
    transform: translateX(-50%) scale(0.5);
}

.dada-wrap {
    width: 576rpx;
    height: 20rpx;
    text-align: center;
    margin: 16rpx auto;
    position: relative;
}

.coupon-wrap {
    background: url('https://storage.360buyimg.com/wxmini/map/bg-pay-coupon.png') no-repeat center;
    background-size: 100%;
    width: 576rpx;
    height: 466rpx;
    text-align: center;
    margin: 0 auto;
    position: relative;
}

.coupon-wrap .title {
    font-size: 40rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #640B09;
    padding: 32rpx;
}

.coupon-wrap .icon-coupon-title {
    width: 32rpx;
    height: 18rpx;
}

.coupon-wrap .coupon {
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #640B09;
}

.coupon-wrap .coupon .price {
    font-size: 112rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #FF1E19;
}

.coupon-wrap .coupon .small {
    font-size: 28rpx;
}

.coupon-wrap .tip {
    font-size: 24rpx;
    color: #FFFFFF;
    text-shadow: 0px -1px 8px rgba(146, 4, 4, 0.25);
    position: relative;
    top: 50rpx;
}

.coupon-wrap .info {
    font-size: 16rpx;
    position: absolute;
    top: 24rpx;
    right: 80rpx;
    z-index: 3;
    color: #000000;
    opacity: 0.1;
}

.icon-foot {
    width: 12rpx;
    height: 44rpx;
    position: absolute;
    bottom: -2rpx;
}

.icon-foot.left {
    left: 100rpx;
}

.icon-foot.right {
    right: 100rpx;
}


.goStore {
    margin-top: 16rpx;
    padding-bottom: 64rpx;
}

.qrcode {
    position: absolute;
    left: 138rpx;
    top: 0;
    width: 240rpx;
    height: 240rpx;
}
.codeCenter{
    left: 0;
    right: 0;
    margin: auto;
}

.barcode {
    position: absolute;
    right: 138rpx;
    width: 80rpx;
    height: 240rpx;
}