page {
    background-color: #fafafa;
}

.load-tip {
    font-size: 30rpx;
    line-height: 30rpx;
    color: #666;
    margin: 10rpx 0;
    text-align: center;
}

.demo-item {
    margin: 16rpx 20rpx;
}

.store-wrap {
    background: #f4f4f4;
    padding: 32rpx 0 50rpx 20rpx;
}

.title {
    font-size: 36rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #333333;
    margin-left: 20rpx;
}

.seat {
    width: 100%;
    height: 72rpx;
}

.load_tip {
    font-size: 24rpx;
    color: #666;
    text-align: center;
    line-height: 100rpx;
}

.qrcode {
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    width: 240rpx;
    height: 240rpx;
}
.codeCenter{
    left: 0;
    right: 0;
    margin: auto;
}

.barcode {
    position: absolute;
    right: 0rpx;
    height: 240rpx;
    width: 80rpx;
  }

.tabSwitch {
    position: fixed;
    top: 0;
    z-index: 9;
    width: 750rpx;
    height: 72rpx;
    background: #FFFFFF;
    display: flex;
    align-items: center;
    padding-left: -12rpx;
}
.container{
    margin-top: 72rpx;
}