/*一行打点*/

.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.items-box {
  width: 701rpx;
  margin: 0 auto;
  margin-bottom: 16rpx;
  font-size: 0;
  position: relative;
  background: #fff;
  border-radius: 16rpx;
}

.items-box .content-box {
  width: 100%;
  height: 256rpx;
  box-sizing: border-box;
  padding: 16rpx;
}

.items-box.subsidies .content-box {
  height: 328rpx;
}

.detail .img-box {
  position: relative;
}

.detail .img-box .img {
  width: 180rpx;
  height: 180rpx;
  border-radius: 12rpx;
}

.detail .img-box .webp_img {
  width: 180rpx;
  height: 180rpx;
  border-radius: 12rpx;
}

.items-box.subsidies .detail .img-box .img {
  width: 250rpx;
  height: 250rpx;
}

.detail .img-box .cold-tag {
  position: absolute;
  top: 8rpx;
  left: 8rpx;
}

.descript-box {
  overflow: hidden;
  margin-left: 16rpx;
  flex: 1;
}

.head .name {
  max-width: 450rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  line-height: 40rpx;
  color: #333333;
}

.head .coucp {
  height: 36rpx;
  margin-top: 8rpx;
}

.coucp .coupon {
  height: 36rpx;
  box-sizing: border-box;
  position: relative;
  line-height: 32rpx;
  margin-right: 12rpx;
  border-width: 1px;
  border-style: solid;
  border-radius: 6rpx;
}

/* .coucp .coupon .one-border {
  position: absolute;
  width: 200%;
  height: 200%;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%) scale(0.5);
  transform-origin: center;
  border-radius: 6px;
  border: 1px solid rgba(255, 114, 67, 1);
} */

.coucp .coupon .money-icon {
  width: 34rpx;
  height: 32rpx;
}

.coucp .coupon .coupon-des {
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ff1e19;
  margin: 0 8rpx;
}

.process {
  width: 260rpx;
  margin-top: 18rpx;
  position: relative;
}

.process-content {
  background: rgba(255, 87, 87, 0.1);
  color: #FB4E44;
}

.process-contain {
  width: 260rpx;
  height: 32rpx;
  border-radius: 16rpx;
  position: absolute;
  top: 0;
  left: 0;
  overflow: hidden;
}

.process-basic-style {
  width: 260rpx;
  height: 32rpx;
  line-height: 32rpx;
  border-radius: 16rpx;
  font-size: 22rpx;
  font-weight: 400;
  text-align: center;
}

.process .process-line {
  border-radius: 16rpx;
  overflow: hidden;
  background: #FF8E46;
}

.process-text {
  color: #fff;
}

.store-info {
  margin-top: 10rpx;
}

.store-info .store-icon {
  width: 28rpx;
  height: 28rpx;
  flex-shrink: 0;
  flex-grow: 0;
  margin-right: 8rpx;
}

.store-info .store-name {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.3);
}

.price-group .main-price {
  margin-right: 8rpx;
}

.price-group .price-container {
  display: inline-block;
}

.footer .buy-button {
  width: 144rpx;
  padding: 8rpx 0;
  text-align: center;
  background: linear-gradient(90deg, #FF3C30 0%, #FF067A 100%);
  border-radius: 6rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #fff;
}

.footer .left-wrap {
  max-width: 65%;
}

.buy-button .bt-process-wrap .bt-process {
  flex: 1;
  height: 6rpx;
  border-radius: 4rpx;
  background: rgba(255, 255, 255, 0.2);
  margin-left: 14rpx;
  margin-right: 4rpx;
  position: relative;
}

.buy-button .bt-process-wrap .bt-process .bt-process-inner {
  height: 6rpx;
  border-radius: 4rpx;
  position: absolute;
  left: 0;
  top: 0;
  background: #fff;
}

.buy-button .bt-process-wrap .bt-precent {
  margin-right: 6rpx;
  font-size: 20rpx;
  font-family: 'regular';
}

.footer .discountText {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FF1E19;
  line-height: 34rpx;
}

/* 超级补贴新样式 */

.detail .subsidies-left {
  width: 250rpx;
  overflow: hidden;
}

.descript-box .middle {
  width: 356rpx;
  margin-top: 16rpx;
  text-align: center;
}

.middle .sub-price {
  display: inline-block;
  width: 108rpx;
  height: 80rpx;
  text-align: center;
  background-image: url('https://storage.360buyimg.com/wximg/seckill/sanjiao.png');
  background-repeat: no-repeat;
  background-size: 108rpx 30rpx;
  background-position: center bottom;
}

.middle .sub-price .sub-detail {
  width: 72rpx;
  display: inline-block;
  background: #FF1E19;
  border-radius: 8rpx 8rpx 0 0;
}

.sub-price .sub-detail .sub-detail-top {
  padding-top: 4rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 24rpx;
}

.sub-price .sub-detail .sub-detail-bottom {
  font-size: 24rpx;
  font-family: 'regular';
  font-weight: 400;
  color: #FFFFFF;
  line-height: 28rpx;
  margin-top: 4rpx;
}

.middle .sub-line {
  margin-top: -44rpx;
}

.sub-line .sub-line-middle {
  width: 136rpx;
  height: 68rpx;
  background-image: url('https://storage.360buyimg.com/wximg/seckill/line-middle.png');
  background-repeat: no-repeat;
  background-size: 100% 40rpx;
  background-position: center bottom;
}

.sub-line .sub-line-left, .sub-line .sub-line-right {
  flex: 1;
}

.sub-line .slf-top {
  font-size: 24rpx;
  font-family: 'light';
  font-weight: 300;
  color: #666666;
  line-height: 28rpx;
}

.sub-line .slf-middle {
  height: 12rpx;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: left;
}

.sub-line .sub-line-left {
  text-align: left;
}

.sub-line .sub-line-right {
  text-align: right;
}

.sub-line .sub-line-left .slf-middle {
  background-image: url('https://storage.360buyimg.com/wximg/seckill/line-left.png');
}

.sub-line .sub-line-right .slf-middle {
  background-image: url('https://storage.360buyimg.com/wximg/seckill/line-right.png');
}

.sub-line .slf-bottom {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 28rpx;
}

.footer-bt {
  width: 360rpx;
  height: 72rpx;
  margin-top: 16rpx;
  box-sizing: border-box;
  padding-left: 16rpx;
  background: linear-gradient(90deg, #FFEE8C 0%, #FEE988 100%, #FEE988 100%);
  border-radius: 8rpx;
  overflow: hidden;
}

.footer-bt .bt-left {
  overflow: hidden;
  width: 188rpx;
}

.footer-bt .bt-left .bt-left-price {
  font-size: 36rpx;
  font-family: 'regular';
  font-weight: 400;
  color: #FF1E19;
  line-height: 44rpx;
  max-width: 126rpx;
}

.footer-bt .bt-left .bt-left-des {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FF1E19;
  line-height: 24rpx;
  transform: translateY(-2rpx);
  -webkit-transform: translateY(-2rpx);
  margin-left: 2rpx;
}

.footer-bt .bt-middle {
  width: 16rpx;
  height: 72rpx;
}

.footer-bt .bt-right {
  flex-shrink: 0;
  width: 140rpx;
  height: 100%;
  background: linear-gradient(90deg, #FF3C30 0%, #FF067A 100%);
}
.footer-bt .bt-right.gray {
  background: #CCCCCC;
}

.footer-bt .bt-right .txt {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

.footer-bt .arrow {
  width: 12rpx;
  height: 20rpx;
  margin-left: 10rpx;
  margin-right: 16rpx;
}

.lazy-skeleton {
  width: 100%;
  height: 100%;
  border-radius: 16rpx;
  background: #fff;
}

/* 8.9 倒计时样式 */

.cutdown {
  width: 100%;
  height: 56rpx;
  box-sizing: border-box;
  padding: 0 16rpx;
  background: rgba(255, 66, 62, 0.05);
  overflow: hidden;
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  color: #FF1E19;
}

.cutdown .title {
  font-weight: 500;
  flex: 1;
}

.cutdown .txt {
  font-weight: 400;
  margin-right: 8rpx;
  flex-shrink: 0;
  flex-grow: 0;
}

.cutdown .times .times-item {
  width: 40rpx;
  height: 40rpx;
  background: #FF1E19;
  border-radius: 4rpx;
  line-height: 40rpx;
  text-align: center;
  font-family: 'regular';
  font-weight: 400;
  color: #FFFFFF;
}

.cutdown .times .symbol {
  margin: 0 8rpx;
}

.placehold_list {
  background: #f4f4f4;
}