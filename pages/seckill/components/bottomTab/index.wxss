.blank {
    height: 54rpx;
    width: 100%;
}
.fixed {
    position: fixed;
    right: 24rpx;
    bottom: 152rpx;
}
.bottom-tab-wrap {
    height: 116rpx;
    padding: 0 40rpx;
    background: #FFFFFF;
    box-shadow: 0rpx 0rpx 24rpx 0rpx rgba(0, 0, 0, 0.2);
    border-radius: 59rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    position: fixed;
    bottom: 80rpx;
    left: 50%;
    z-index: 2;
    transform: translateX(-50%);
}
.bottom-tab-item-wrap {
    width: 100rpx;
    height: 96rpx;
    /* background: #D8D8D8; */
    margin-right: 24rpx;
}
.bottom-tab-item-wrap:last-child {
    margin-right: 0;
}
.tab-img {
    width: 100%;
    height: 100%;
}