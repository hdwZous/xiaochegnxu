.classify {
  background: #fff;
  border-radius: 16rpx;
  box-sizing: border-box;
  position: relative;
}

.classify.tabFixd {
  background: #FF2626;
  border-radius: 0;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.scroll-wrap {
  flex: 1;
  overflow-x: hidden;
}

.tabs-items {
  flex-grow: 0;
  flex-shrink: 0;
  flex-basis: auto;
  height: 84rpx;
  padding: 0 20rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  position: relative;
  color: #666666;
}

.classify.tabFixd .tabs-items {
  color: #fff;
}

.tabs-items .tabs-item-txt {
  line-height: 28rpx;
}

.tabs-item-img {
  width: 136rpx;
  height: 60rpx;
}

.tabs-items .location {
  width: 24rpx;
  height: 32rpx;
  display: inline-block;
  position: relative;
  overflow: hidden;
  margin-right: 4rpx;
}

.tabs-items .location .location-img {
  width: 28rpx;
  height: 32rpx;
  position: absolute;
  top: 0;
  left: -1px;
}

.tabs-items .location-fixed {
  width: 28rpx;
  height: 28rpx;
  margin-right: 2rpx;
}

.tabs-items.actived {
  color: #FF2626;
}

.tabs-right {
  width: 96rpx;
  height: 84rpx;
  position: relative;
}

.ty-icon {
  position: absolute;
  top: 0;
  left: -24rpx;
  width: 96rpx;
  height: 84rpx;
}

.ty-icon-fix {
  width: 16rpx;
  height: 84rpx;
}

.flei-icon {
  position: absolute;
  right: 0;
  top: 0;
  width: 96rpx;
  height: 84rpx;
  background-image: url("https://storage.360buyimg.com/wximg/activity/arrow-down.png");
  background-repeat: no-repeat;
  background-size: 24rpx 12rpx;
  background-position: 42rpx center;
}

.flei-icon-fix {
  width: 24rpx;
  height: 84rpx;
  display: inline-block;
  padding-left: 32rpx;
  padding-right: 24rpx;
  background-image: url('https://storage.360buyimg.com/wximg/seckill/fenlei.png');
  background-repeat: no-repeat;
  background-size: 24rpx 24rpx;
  background-position: 32rpx;
}

.classify .bg {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 99;
  background: rgba(0, 0, 0, 0.7);
}

.popup {
  position: absolute;
  top: 0;
  width: 702rpx;
  box-sizing: border-box;
  padding: 24rpx;
  padding-bottom: 8rpx;
  box-shadow: 0px 6rpx 10rpx 0px rgba(0, 0, 0, 0.04);
  border-radius: 24rpx;
  background: #fff;
  z-index: 100;
}

.popup.yetTop {
  left: 0;
  width: 100%;
  border-radius: 0 0 24rpx 24rpx;
}

.popup .head .title {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 34rpx;
}

.popup .head .ts-up {
  width: 48rpx;
  height: 24rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 24rpx 12rpx;
  background-position: right top;
}

.popup .contain {
  margin-top: 38rpx;
  height: 320rpx;
  margin-right: -20rpx;
  overflow-y: scroll;
}

.popup .contain .category {
  width: 208rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  background: #F8F8F8;
  border-radius: 16px;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  margin-bottom: 16rpx;
  margin-right: 16rpx;
}

.popup.yetTop .contain .category {
  width: 224rpx;
}

.category_txt {
  display: inline-block;
  max-width: 170rpx;
}

.popup .contain .category.actived {
  background: #FFE8E8;
  color: #FF1E19;
  font-weight: bold;
}

.d-flex {
  display: -webkit-box !important;
  display: -webkit-flex !important;
  display: -ms-flexbox !important;
  display: flex !important;
}

.flex-row {
  -webkit-box-orient: horizontal !important;
  -webkit-box-direction: normal !important;
  -webkit-flex-direction: row !important;
  -ms-flex-direction: row !important;
  flex-direction: row !important;
}

.justify-content-around {
  -webkit-justify-content: space-around !important;
  -ms-flex-pack: distribute !important;
  justify-content: space-around !important;
}

.justify-content-between {
  -webkit-box-pack: justify !important;
  -webkit-justify-content: space-between !important;
  -ms-flex-pack: justify !important;
  justify-content: space-between !important;
}

.align-items-center {
  -webkit-box-align: center !important;
  -webkit-align-items: center !important;
  -ms-flex-align: center !important;
  align-items: center !important;
}

.flex-wrap {
  -webkit-flex-wrap: wrap !important;
  -ms-flex-wrap: wrap !important;
  flex-wrap: wrap !important;
}

/*一行打点*/
.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}