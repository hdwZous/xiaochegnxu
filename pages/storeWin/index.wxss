@import './common/css/reset.wxss';

button {
  padding: 0;
  border-radius: 0;
  background: transparent;
}

button:after {
  border: none;
}

button[plain] {
  border: 0
}

button.button-hover {
  background-color: transparent;
}

.storeWin {
  font-size: 0;
  min-height: 100vh;
  background-image: url("https://storage.360buyimg.com/wximg/storewin/bg.png");
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.storeWinErrorLayout{
  min-height: 100vh;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.storeWinErrorImg {
  width: 200px;
  height: 200px;
  margin-top: -30px;
}
.storeWinErrorText {
  font-size: 16px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #525B6A;
  line-height: 22px;
  margin-top: 24px;
}


.storeWin .headpic {
  width: 750rpx;
  height: 600rpx;
  margin-bottom: 50rpx;
}

.storeWin .yetrest {
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 143, 2, 1);
  line-height: 44rpx;
  margin-bottom: 40rpx;
}

.storeWin .store-btn {
  width: 702rpx;
  height: 110rpx;
  background-image: url("https://storage.360buyimg.com/wximg/storewin/button2x.png");
  background-size: contain;
  background-repeat: no-repeat;
  text-align: center;
  line-height: 92rpx;
  font-size: 44rpx;
  font-weight: 500;
  font-family: PingFangSC-Medium, PingFang SC;
  color: rgba(255, 255, 255, 1);
  text-shadow: 0px 4rpx 6rpx rgba(228, 30, 0, 0.72);
  margin-bottom: 32rpx;
}

.storeWin .loading-wrap {
  margin-top: 140rpx;
}

.storeWin .loading-wrap .loading {
  width: 34rpx;
  height: 34rpx;
  animation: spin 1s linear infinite;
  -webkit-animation: spin 1s linear infinite;
}

@keyframes spin {
  from {
    transform: rotate(0);
    -webkit-transform: rotate(0);
  }

  to {
    transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
  }
}

@-webkit-keyframes spin {
  from {
    transform: rotate(0);
    -webkit-transform: rotate(0);
  }

  to {
    transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
  }
}

.storeWin .loading-wrap .loading-txt {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 143, 2, 1);
  line-height: 40rpx;
  margin-left: 16rpx;
}

.storeWin .coupon {
  padding-bottom: 16rpx;
}

.storeWin .shop {
  text-align: center;
}

.shop .shop-img {
  width: 750rpx;
  height: 754rpx;
}

.shop .shop-tip {
  width: 560rpx;
  display: inline-block;
  margin-top: 50rpx;
  margin-bottom: 60rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Light, PingFang SC;
  font-weight: 300;
  color: rgba(153, 153, 153, 1);
  line-height: 32rpx;
}

.loadingStoreWin {
  position: fixed;
