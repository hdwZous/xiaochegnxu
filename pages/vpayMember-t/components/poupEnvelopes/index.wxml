<block wx:if="{{$taroCompReady}}"><view bindtap="closeBtn" class="{{style['alert-box']}}" data-e-tap-so="this"><view class="{{style.alert_box_content}}"><view class="{{style.alert_box_wrap}}"><view class="{{style.title}}">{{title}}</view><view class="{{style.couponBox}}"><view class="{{style.leftCouponBox}}"><view class="{{style.leftCouponMon}}">{{sourcePackage.amount}}<text class="{{style.leftCouponUnit}}">元</text></view><view class="{{style.leftCouponLimit}}">{{sourcePackage.limitRule || '满29元可用'}}</view></view><view class="{{style.exchangeIMg}}"></view><view class="{{style.rtCouponBox}}"><view class="{{style.rtCouponLogoBox}}"><image class="{{style.rtCouponLogo}}" src="{{storeLogo || 'https://storage.360buyimg.com/wximg/common/logo.jpg'}}"></image></view><view class="{{style.leftCouponMon}}">{{targetPackage.amount}}<text class="{{style.leftCouponUnit}}">元</text></view><view class="{{style.leftCouponLimit}}">{{targetPackage.limitRule || '满29元可用'}}</view></view></view><view class="{{style['alert-box-bottom']}}"><view class="{{style['alert-box-btn']}}" catchtap="confirmExchange" data-e-tap-so="this">兑换红包</view></view></view><view class="{{style.close}}" bindtap="closeBtn" data-e-tap-so="this"></view></view></view></block>
