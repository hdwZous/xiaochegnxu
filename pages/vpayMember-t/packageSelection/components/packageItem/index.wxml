<block wx:if="{{$taroCompReady}}"><view class="{{[style.scroll, userCardType == cardInfo.cardType ? style.current : '', 'skeleton-rect']}}" bindtap="selectPackage" data-e-tap-so="this" data-e-tap-a-a="{{cardInfo}}"><block wx:if="{{cardInfo.tipText}}"><view class="{{style.remark}}">{{cardInfo.tipText}}</view></block><view class="{{style.cardName}}">{{cardInfo.cardName}}</view><view class="{{style.money}}"><text class="{{style.unit}}">¥</text>{{cardInfo.cardType == 4 ? cardInfo.firstMonthPrice ? cardInfo.firstMonthPrice : cardInfo.priceWord : cardInfo.priceWord}}</view><view class="{{cardInfo.cardType == userCardType ? [style.price, style.currentPrice] : style.price}}">{{cardInfo.originPrice ? '¥' + cardInfo.originPrice : '¥' + cardInfo.priceWord}}</view><block wx:if="{{cardInfo.noticeTitle}}"><view class="{{style.tip}}">{{cardInfo.noticeTitle}}</view></block></view></block>
