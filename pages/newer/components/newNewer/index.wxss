@keyframes taglinear {
  0% {
    opacity: 0;
    transform: scale(4);
  }
  50%{
  opacity: 0;
    transform: scale(4);
  }
  100% {
    opacity: 1;
    transform: scale(1)
  }
}
@keyframes tag2linear {
  0% {
    opacity: 0;
    transform: scale(4);
  }
  70%{
  opacity: 0;
    transform: scale(4);
  }
  100% {
    opacity: 1;
    transform: scale(1)
  }
}
@keyframes tag3linear {
  0% {
    opacity: 0;
    transform: scale(4);
  }
  80%{
  opacity: 0;
    transform: scale(4);
  }
  100% {
    opacity: 1;
    transform: scale(1)
  }
}
@keyframes priceAnimation {
  0% {
    transform: translateY(0%);
    opacity: 1;
  }
  100% {
    transform: translateY(-100%);
    opacity: 1;
  }
}
