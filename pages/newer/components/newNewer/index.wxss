  position: absolute;
  left: 0;
  transform: translateY(0%);
  opacity: 0;
}
.save .price-animation:nth-child(odd)::after{
  animation: priceAnimation 2s ease-out;
}
.save .price-animation:nth-child(even)::after{
  animation: priceAnimation 2s ease;
}
