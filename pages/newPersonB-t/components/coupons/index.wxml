<block wx:if="{{$taroCompReady}}"><block wx:if="{{!showBtn}}"><view></view></block><view class="{{style.coupons}}" style="{{anonymousState__temp2}}" wx:else><block wx:if="{{showBtn}}"><template is="renderBtn" data="{{...anonymousState__temp}}"></template></block></view></block>
<template name="renderPlanHint"><block><view><view class="{{style.title}}">{{money}}元补贴已到账 ，快去抢购以下爆品吧~</view><view class="{{style.plan_hint}}"><view style="{{item.$loopState__temp6}}" wx:key="$loopState__temp8" wx:for="{{loopArray143}}" wx:for-item="item" wx:for-index="index"><view class="{{style.pecket}}"><text class="{{style.text}}" style="{{item.$loopState__temp10}}">{{taskInfo.currentStage > index ? '已完成' : '第' + (index + 1) + '单补贴' + item.$original.title + '元' + (!isLogin ? '起' : '')}}</text><image class="{{style.icon}}" src="{{'https://storage.360buyimg.com/wximg/newUser/' + item.styleType + '_packet.png'}}"></image><block wx:if="{{taskInfo.currentStage === index}}"><template is="renderBottomTriangle" data="{{...item.$loopState__temp4}}"></template></block></view><image class="{{style.schedule}}" src="{{'https://storage.360buyimg.com/wximg/newUser/' + item.showShedule + '_schedule.png'}}" style="{{item.$loopState__temp12}}"></image></view></view></view></block></template>
<template name="renderCoupon"><block><scroll-view scroll-x="{{true}}" class="{{[style.scrollView, couponsInfo.showType !== 3 ? style.clearMargin : null]}}"><view class="{{couponsList.length > 1 ? style.more_coupon : style.sheet_coupon}}" wx:key="$loopState__temp14" style="{{item.$loopState__temp16}}" wx:for="{{loopArray144}}" wx:for-item="item" wx:for-index="index"><view class="{{style.coupon_money}}"><text class="{{style.money_symbol}}">¥</text><text class="{{style.money_count}}">{{item.$original.quota}}</text><text class="{{style.money_hint}}" style="{{item.$loopState__temp18}}">起</text></view><view class="{{style.coupon_info}}"><view class="{{style.name}}">{{item.$original.showName || '优惠券'}}</view><view class="{{style.condition}}">{{item.$loopState__temp20}}{{!item.$original.minOrderAmount ? isLogin ? '新人券' : '首单可用' : '满' + item.$original.minOrderAmount + '元可用'}}</view></view><view class="{{style.coupon_used}}" style="{{item.$loopState__temp22}}"></view></view></scroll-view></block></template>
<template name="renderBottomTriangle"><block><image class="{{style.bottom_triangle}}" src="https://storage.360buyimg.com/wximg/newUser/bottom-triangle.png"></image></block></template>
<template name="renderBtn"><block><view class="{{style.login_btn}}" bindtap="_btnEvent">{{btnName}}</view></block></template>
