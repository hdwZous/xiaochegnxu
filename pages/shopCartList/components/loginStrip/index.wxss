.loginStrip {
    width: 100%;
    height: 80rpx;
    position: fixed;
    bottom: 30rpx;
    padding: 0 12px;
    box-sizing: border-box;
    z-index: 14;
    
}

.loginStrip .login {
    box-sizing: border-box;
    width: 100%;
    height: 80rpx;
    background: rgba(0, 0, 0, 0.6);
    border-radius: 16rpx;
    font-size: 28rpx;
    color: #ccc;
    padding: 0 24rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.login .login_btn {
    width: 136rpx;
    height: 48rpx;
    background: linear-gradient(270deg, #00CF37 0%, #45DC6D 100%);
    border-radius: 50rpx;
    text-align: center;
    line-height: 48rpx;
    color: #fff;
}