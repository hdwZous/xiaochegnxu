.cart-items {
  min-height: 290rpx;
  background: #fff;
  border-radius: 24rpx;
  margin-top: 16rpx;
}

.gray {
  height: 300rpx;
  background: #fff;
  border-radius: 24rpx;
  margin-top: 16rpx;
}

.cart_list .header {
  padding: 24rpx;
  padding-left: 20rpx;
  background-color: rgb(252 252 252);
  border-radius: 24rpx;
}

.cart_list .icon_select {
  margin-right: 12rpx;
  width: 44rpx;
  height: 44rpx;
  background: url("https://storage.360buyimg.com/wximg/store_2.0/not-choose.png");
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}

.cart_list .icon_select.icon_select_active {
  background-image: url("https://storage.360buyimg.com/wximg/store_2.0/checked-icon.png");
}

.cart_list .icon_select.select_all_disabled {
  background-image: url("https://storage.360buyimg.com/wximg/store_2.0/none-choose.png");
}

.cart_list .select {
  color: #333;
  font-size: 24rpx;
}

.cart_list .cancel_case .icon_coupon {
  width: 80rpx;
  height: 36rpx;
  background-image: url("https://storage.360buyimg.com/wximg/store_2.0/youhuiquan.png");
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

.cart_list .cancel_case .space-line {
  width: 1px;
  height: 18rpx;
  background: #979797;
  transform: scaleX(0.5);
  -webkit-transform: scaleX(0.5);
  transform-origin: center;
  -webkit-transform-origin: center;
  margin: 0 12rpx;
}

.cart_list .cancel_case .cancel-delete {
  width: 24rpx;
  height: 24rpx;
  margin-right: 6rpx;
}

.cart_list .cancel {
  color: #999999;
  font-size: 24rpx;
  line-height: 30rpx;
}

.h_right {
  flex: 1;
  overflow: hidden;
}

.store_name {
  flex: 1;
  overflow: hidden;
  box-sizing: border-box;
  padding-right: 50rpx;
}

.store_name .store_txt {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}

.store_name .store_icon {
  color: #333;
  font-size: 24rpx;
  margin-left: 8rpx;
  flex-shrink: 0;
  flex-grow: 0;
}

.h_right_b .right_b_txt {
  color: rgb(102, 102, 102);
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  margin-top: 8rpx;
}

.h_right_b .right_b_txt.line {
  color: rgb(153, 153, 153);
  margin-left: 10rpx;
  text-decoration: line-through;
}

.h_right_b .right_b_line {
  width: 1px;
  height: 20rpx;
  background-color: rgb(204, 204, 204);
  margin: 0 12rpx;
  margin-top: 8rpx;
}

.add_on_tips {
  padding: 20rpx;
  box-sizing: border-box;
  width: 100%;
  background-color: rgba(255, 246, 205, 1);
  color: #713F00;
  font-size: 24rpx;
  text-align: center;
  position: relative;
  z-index: 20;
}

.add_on_tips_top {
  position: relative;
  z-index: 20;
  height: 24rpx;
  background: #fff;
}

.add_on_tips .tips_box {
  text-align: left;
  line-height: 28rpx;
}

.add_on_tips .go_add_on_btn_container {
  flex: none;
  color: #FF5757;
  margin-left: 10rpx;
}

.add_on_tips .arrow {
  background: url("https://storage.360buyimg.com/wximg/cart/arrow.png") no-repeat;
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  background-size: cover;
  vertical-align: -5rpx;
}

.price_wrap, .member_wrap {
  margin: 0 24rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  color: rgb(51, 51, 51);
  height: 72rpx;
  box-sizing: border-box;
  border-bottom: 1px solid rgb(245, 245, 245);
  overflow: hidden;
}

.price_wrap .price_delete {
  margin-right: 8rpx;
  color: rgb(102, 102, 102);
  text-decoration-line: line-through;
}

.price_wrap .price_normal {
  font-size: 32rpx;
  font-weight: bold;
}

.members {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
}

.member_title {
  flex: none;
}

.members .members-price {
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  margin-left: 4rpx;
}

.members .members-space-line {
  width: 1px;
  height: 20rpx;
  transform: scaleX(0.5);
  -webkit-transform: scaleX(0.5);
  background: rgb(255, 179, 179);
  margin: 0 10rpx;
}

.members .member-arrow {
  width: 10rpx;
  height: 10rpx;
  border: 0 solid black;
  border-right-width: 2rpx;
  border-bottom-width: 2rpx;
  transform: rotate(-45deg);
  transform-origin: center center;
  margin-left: 4rpx;
  margin-right: 2rpx;
}

.footer_wrap {
  padding: 24rpx;
  text-align: center;
}

.footer_wrap .right {
  /* display: table; */
  width: 176rpx;
  height: 80rpx;
  color: #fff;
  font-size: 28rpx;
  border-radius: 40rpx;
  margin-left: 16rpx;
}

.footer_wrap .right .button-txt {
  box-sizing: border-box;
  max-width: 176rpx;
  height: 80rpx;
  padding: 0 8rpx;
  line-height: 32rpx;
  /* display: table-cell;
  vertical-align: middle; */
  font-weight: bold;
}

.footer_wrap .right .button-txt .small_btname {
  font-size: 24rpx;
}

.footer_wrap .right .button-txt .button-txt-money {
  font-family: 'regular';
  font-size: 40rpx;
  font-weight: 400;
  max-width: 100%;
}

.cart_good_wrap {
  position: relative;
}

.cart_good_wrap .layer_wrap {
  position: absolute;
  bottom: 0;
  z-index: 13;
}

.cart_good_wrap .layer_wrap.open {
  position: relative;
  margin-top: 24rpx;
}

.cart_good_wrap .layer_wrap.open .layer_btn {
  position: relative;
}

.cart_good_wrap .layer {
  width: 702rpx;
  height: 184rpx;
}

.layer_btn {
  position: absolute;
  bottom: 0;
  width: 654rpx;
  height: 56rpx;
  background: #F6F6F6;
  border-radius: 28rpx;  
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
}

.layer_btn .layer_arrow {
  width: 26rpx;
  height: 26rpx;
  margin-left: 10rpx;
  transition: all 0.3s;
}

.layer_btn .layer_arrow.open {
  transform: rotate(180deg);
  -webkit-transform: rotate(180deg);
}