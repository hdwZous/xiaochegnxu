#globalshopCart {
  background-color: rgb(246, 246, 246);
  overflow-x: hidden;
  min-height: 100vh;
}

.shopCart {
  padding: 8rpx 24rpx;
  padding-bottom: 50rpx;
}

.allShopCart {
  width: 100%;
  height: 84rpx;
  border-radius: 24rpx;
  background: #fff;
  margin-top: 16rpx;
  line-height: 84rpx;
  display: flex;
  color: rgb(51, 51, 51);
  align-items: center;
  justify-content: center;
}

.triangle {
  color: rgb(102, 102, 102);
  font-size: 18rpx;
  margin-left: 8rpx;
}

.restPos {
  font-size: 32rpx;
  font-weight: bold;
  color: rgb(51, 51, 51);
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 64rpx 0 16rpx 0;
}

.restPos .restPosIcon {
  width: 20rpx;
  height: 24rpx;
  margin-right: 8rpx;
}

.locationEmpty {
  width: 100%;
  height: 116rpx;
  background: rgb(255, 255, 255);
  margin-top: 30rpx;
  border-radius: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: rgb(102, 102, 102);
  font-size: 28rpx;
}

.locationEmpty .locationIcon {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}

.v-floor {
  background: linear-gradient(180deg, #FFFAF0 0%, #FFFFFF 100%);
  border-radius: 24rpx;
  padding: 24rpx 0;
  margin-top: 16rpx;
}

.v-floor .vf-logo {
  margin-left: 24rpx;
  margin-right: 16rpx;
  width: 88rpx;
  height: 32rpx;
}

.vf-bt {
  flex: 1;
}

.vfb {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 32rpx;
}

.vf-arrow {
  color: #474747;
  font-size: 22rpx;
  margin-left: 4rpx;
  margin-right: 16rpx;
}

.vfd_num {
  transform: translateY(2rpx);
  -webkit-transform: translateY(2rpx);
}

