
@import "/common/css/global";
page{
    -webkit-overflow-scrolling: touch;
}
.pt12{
    padding-top: 24rpx;
}
.plr12{
    padding-left: 24rpx;
    padding-right: 24rpx;
}
.scroll-view {
    background: #F6F6F6;
}
.header_wrap{
    position: fixed;
    top:0;
    left: 0;
    right: 0;
    z-index: 99;
}
/* .hori_ball{
    position: relative;
    z-index: 101;
} */
.component-padding {
    margin: 24rpx 23rpx 0;
    font-size: 0;
}
.cover_image{
    position: fixed;
    z-index: -1;
    height: 440rpx;
}
.no_data_store_img {
    text-align: center;
    padding-top: 200rpx;
}
.no_data_store_img image {
    width: 160px;
    height: 160px;
}
.no_data_store {
    color: #999;
    font-size: 24rpx;
    line-height: 32rpx;
    width: 100%;
    margin: 12rpx auto 0;
    text-align: center;
}

.content_area{
    height: 100vh;
    overflow: hidden;
    display: flex;
    flex-direction: column;
}

.content_wrap{
    background: #F6F6F6;
    /* border-radius: 12px; */
    flex: 1;
    flex-shrink: 0;
    height: 1px;
    /* position: relative; */
    /* z-index: 100; */
}
.content_wrap02{
    /* height: calc(100vh - 155px); */
    -webkit-overflow-scrolling: touch;
    height: 100%;
    /* overflow: auto; */
}
.minHeight{
    min-height: calc(100vh + 50px);
}
.content_wrap02 ::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
    display:none
}

.hide{
    display: none;
}
.hide02{
    position: absolute;
    top:-150px;
    /* z-index: -1; */
    /* opacity: 0; */
}
.z101{
    z-index: 101;
}
.z99{
    z-index: 99;
}

/* 横向滚动 */
.wrap01{
    display: flex;
    width: 100%;
    background: linear-gradient(180deg, #FFFFFF 0%, #F6F6F6 100%);
    padding: 24rpx 24rpx 24rpx 24rpx;
    box-sizing: border-box;
    border-radius: 12px;
}
.item01{
    flex:1;
    text-align: center;
}
.img_wrap01{
    display: flex;
    align-items: center;
    justify-content: center;
    width: 104rpx;
    height: 104rpx;
    margin: 0 auto;
    position: relative;
}
.tooltips{
    position: absolute;
    height: 38rpx;
    line-height: 38rpx;
    width: 80rpx;
    background: url('https://storage.360buyimg.com/wximg/channel/tooltips.png') no-repeat center center;
    top:-12rpx;
    left: 58rpx;
    background-size:contain;
    font-size: 20rpx;
    color: #fff;
    text-align: center;
}
.tooltips_word{
    position: relative;
    top:-4rpx;
}
.img01{
    width: 96rpx;
    height: 96rpx;
    border: 4rpx solid transparent; 
    border-radius: 50%;  
    transition: all ease 0.3s;
}
.title{
    width: 124rpx;
    height: 32rpx;
    line-height: 32rpx;
    border-radius: 26rpx;
    font-size: 24rpx;
    margin: 0 auto;
    margin-top: 16rpx;
    text-align: center;
    white-space: nowrap;
}
.nav_wrap{
    width: 100%;
    transform: translateY(0px);
    white-space: nowrap;
    background:#fff;
    height: 176rpx;
}
.nav_item{
    text-align: center;
    display: inline-block;
    margin-right: 24rpx;
}
.nav_item:first-child{
    padding-left: 24rpx;
}
.move_arrow{
    width: 7px;
    height: 12px;
    margin: 0 auto;
}
.arrow{
    border:1px solid rgb(51, 51, 51);
    border-color: rgb(51, 51, 51);
    border-top-width: 0px;
    border-left-width: 0px;
    height: 12rpx;
    transform: translateY(-0rpx) rotate(45deg);
    transform-origin: center center;
    width: 12rpx;
}

.secondNav1{
    border-radius: 24rpx;
    padding:24rpx 24rpx 0rpx 24rpx;
    background: #fff;
}
.secondNav2{
    background: #fff;
    padding:12rpx 24rpx 0rpx 24rpx;
    border-radius: 0 0 24rpx 24rpx;
    max-height: calc(100vh - 320rpx);
    overflow: scroll;
    -webkit-overflow-scrolling: touch;
    box-sizing: border-box;
}
.mask{
    position: fixed;
    left:0;
    right: 0;
    top:0;
    bottom:0;
    z-index: -1;
    background: rgba(0,0,0,0.5);
}


/* 二级导航 */
.item{
    display: inline-block;
    width: 151rpx;
    height: 64rpx;
    line-height: 64rpx;
    background: #F6F6F6;
    border-radius: 32rpx;
    font-size: 24rpx;
    text-align: center;
    margin-right: 16rpx;
    margin-bottom: 16rpx;
    color: #666666;
    position: relative;
    overflow: hidden;
}
.item2{
    width: 162rpx;
}
.bg{
    opacity: 0.2;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
}
.name{
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    line-height: 64rpx;
}
.ts_next{
    font-size: 18rpx;
    display: inline-block;
}
.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/* 没有数据 */
.no_data{
    text-align: center;
    margin-top: 150rpx;
    margin-bottom: 300rpx;
    padding-bottom: 200rpx;
}
.no_data_img{
    width: 160px;
    height: 160px;
}
.no_data_text{
    font-size: 28rpx;
    color: #666;
    text-align:center;
}

.cateBall{
    /* border-radius: 12px;
    overflow: hidden; */
    position: relative;
}
.to_bottom{
    text-align: center;
    font-size: 24rpx;
    padding-bottom: 24rpx;
    color: #333
}

.hint_msg {
    text-align: center;
    padding: 16rpx 0;
}

.hint_msg_text {
    font-size: 28rpx;
    color: #666;
    text-align:center;
    padding: 28rpx 0 56rpx 0;
}

.hint_msg_img {
    height: 40rpx;
}
.ball_bgcolor {
    width: 100%;
    position: absolute;
    background-size: 100% auto;
}