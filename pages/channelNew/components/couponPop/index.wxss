.wrap {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.5);
    z-index: 101;
}
.content {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}
.case {
    padding-bottom: 20rpx;
    width: 594rpx;
    background: linear-gradient(-7deg, #ff1743, #ff414a);
    border-radius: 32rpx 32rpx 30rpx 30rpx;
}
.img {
    margin-top: -84rpx;
    width: 594rpx;
    height: 248rpx;
}
.list {
    height: 432rpx;
}
.item {
    padding: 0 20rpx 20rpx;
}
.check_case {
    padding: 40rpx 0 20rpx;
}
.check_btn {
    margin: 0 auto;
    width: 390rpx;
    color: rgb(255, 219, 186);
    border: 1px solid rgb(255, 219, 186);
    border-radius: 36rpx;
    text-align: center;
    height: 72rpx;
    line-height: 72rpx;
    font-size: 30rpx;
}
.close {
    margin: 60rpx auto 0;
    width: 74rpx;
    height: 74rpx;
    background: url("https://storage.360buyimg.com/wximg/common/vip_close.png")
        no-repeat center;
    background-size: 100% 100%;
}
