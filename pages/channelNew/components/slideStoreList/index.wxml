<view class="store-list">
  <view class="store-list-header">
    <view
      aria-label='附近商家'
      aria-role='text'
      class="title">{{floorName}}</view>
    <view
      wx:if="{{data.moreBtnDesc}}"
      class="look-more"
      aria-label='查看更多商家，点击跳转到附近商家列表'
      aria-role='link'
      data-to="{{data.to}}"
      data-params="{{data.params}}"
      data-user-action="{{data.userAction}}"
      data-trace-id="{{traceId}}"
      data-page-id="{{pageId}}"
      data-current-page-name="{{currentPageName}}"
      data-pre-page-name='{{prePageName}}'
      bindtap="jumper">
      {{data.moreBtnDesc}}
      <view class="more-arrow"></view>
    </view>
  </view>
  <scroll-view
    class="store-wrapper"
    scroll-x
    scroll-anchoring="true"
    scroll-with-animation>
    <view
      wx:for="{{storeList}}"
      wx:for-index="index"
      wx:for-item="item"
      wx:key="item"
      class="store-item channel_comp_ep"
      data-type="{{item.storeName}}"
      data-to="{{item.to}}"
      data-params="{{item.params}}"
      data-user-action="{{item.userAction}}"
      data-trace-id="{{traceId}}"
      data-page-id="{{pageId}}"
      data-current-page-name="{{currentPageName}}"
      data-pre-page-name='{{prePageName}}'
      bindtap="jumper"
      aria-label='{{item.storeName}}{{item.coupons[0].words}}，点击跳转门店使用优惠'
      aria-role='link'>
      <view class="store-img">
        <view style="background-image: url({{item.storeBrandLogo}})" class="store-img_logo"></view>
        <webp-img src="{{item.backgroundImage}}" webp-img='store-img_image round-top'></webp-img>
        <view class="dj_tag" wx:if="{{item.businessTag}}">
          <biz-tag bizType="horizontal" businessTag="{{item.businessTag}}"/>
        </view>
      </view>
      <view class="store-logo">
        <!-- <image src="{{item.imgUrl}}" class="image round" lazy-load></image> -->
        <webp-img src="{{item.imgUrl}}" webp-img='store-logo_image round'></webp-img>
        <!-- <view class="store-tags">常逛</view> -->
      </view>
      <view class="store-name ellipsis">{{item.storeName}}</view>
      <!-- 膨胀劵 -->
      <view
        class="expand channel_comp_ep"
        bind:tap="expandCouponJump"
        data-item='{{item}}'
        data-user-action="{{item.userAction}}"
        data-trace-id="{{traceId}}"
        data-page-id="{{pageId}}"
        data-current-page-name="{{currentPageName}}"
        data-pre-page-name='{{prePageName}}'
        wx:if="{{item.expansionCoupon.iconText}}">
        <view style="background-image: url({{item.expansionCoupon.iconUrl}});" class="expand-image"></view>
        <text class="expand-text">{{item.expansionCoupon.iconText}}</text>
        <text class="expand-price">{{item.expansionCoupon.price}}</text>
      </view>
      <block wx:else>
        <!-- 优惠券 -->
        <view
          class="store-coupon"
          wx:if="{{item.coupons && item.coupons.length > 0}}"
          style="background-color:{{item.coupons[0].backgroundColor || ''}} ;color: {{item.coupons[0].frontColor}};border-color:{{item.coupons[0].outLineColor}}">
          {{item.coupons[0].words}}
        </view>
        <!-- 星级 -->
        <view class="store-start" wx:elif="{{item.storeStar > 0}}">
          <rating rating="{{item.storeStar}}"/>
          <view class="store-score">{{item.scoreAvg}}</view>
        </view>
        <view wx:else class="store-default">暂无评价</view>
      </block>
    </view>
    <view
      wx:if="{{data.moreBtnDesc}}"
      class="more-store log_slide_store_more"
      data-to="{{data.to}}"
      data-params="{{data.params}}"
      data-user-action="{{data.userAction}}"
      bindtap="jumper"
      aria-label='链接：{{data.moreBtnDesc}}'
      aria-role='link'>
      <view class="more-icon">
        <image src="data:image/png;base64," class="image" lazy-load />
      </view>
      <view class="more-text">{{data.moreBtnDesc}}</view>
    </view>
  </scroll-view>
</view>