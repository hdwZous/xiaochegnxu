.meeting-container {
  width: 100%;
}
.top-bg {
}
.title {
  font-size: 40rpx;
  color: #FF1E19;
  text-align: center;
}
.meeting-swiper-wrapper {
  width: 100vw;
  height: 224rpx;
  padding-bottom: 4rpx;
}
.img-wrapper {
  width: 212rpx;
  height: 164rpx;
  margin: 0 auto;
  margin-top: 56rpx;
  -webkit-transition: transform 200ms;
  transition: transform 200ms;
  transform: scale(1);
  -webkit-transform: scale(1);
  transform-origin: center bottom;
}
.meeting-swiper-wrapper .img {
  display: block;
  width: 100%;
  height: 100%;
}
.bigger {
  /* margin-top: 41rpx; */
  /* transform-origin: 50% 100%; */
  transform: scale(1.2);
  -webkit-transform: scale(1.2);
}