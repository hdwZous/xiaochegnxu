.wrap{
    border-radius: 12px;
    height: 110px;
    overflow: hidden;
    position: relative;
}
.swiper-item{
    border-radius: 24rpx;
    width: 700rpx;
    margin: 0 auto;
}
.swiper-item-img{
    width: 100%;
    height: 110px;
    border-radius: 24rpx;
}
.dots{
    position: absolute;
    left:50%;
    bottom: 0px;
    transform: translateX(-50%);
    z-index: 10;
}
.dots_item{
    display: inline-block;
    width: 6px;
    height: 3px;
    background: #FFFFFF;
    border-radius: 2px;
    opacity: 0.4;
    margin-right: 4px;
}
.active{
    width: 8px;
    color: #FFFFFF;
    opacity: 1;
}