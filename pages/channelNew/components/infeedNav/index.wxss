
.nav_wrap{
  width: 100%;
  transform: translateY(0px);
  white-space: nowrap;
  background:#fff;
  height: 176rpx;
}
.hide02{
  position: absolute;
  top:-150px;
  /* z-index: -1; */
  /* opacity: 0; */
}
.nav_item{
  text-align: center;
  display: inline-block;
  margin-right: 24rpx;
}
.nav_item:first-child{
  padding-left: 24rpx;
}
.img_wrap01{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 104rpx;
  height: 104rpx;
  margin: 0 auto;
  position: relative;
}
.img01{
  width: 96rpx;
  height: 96rpx;
  border: 4rpx solid transparent; 
  border-radius: 50%;  
  transition: all ease 0.3s;
}
.title{
  width: 124rpx;
  height: 32rpx;
  line-height: 32rpx;
  border-radius: 26rpx;
  font-size: 24rpx;
  margin: 0 auto;
  margin-top: 16rpx;
  text-align: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.move_arrow{
  width: 7px;
  height: 12px;
  margin: 0 auto;
  transform: "translateY(0rpx) rotate(45deg)"
}
.arrow{
  border:1px solid rgb(51, 51, 51);
  border-color: rgb(51, 51, 51);
  border-top-width: 0px;
  border-left-width: 0px;
  height: 12rpx;
  transform: translateY(-0rpx) rotate(45deg);
  transform-origin: center center;
  width: 12rpx;
}
.hide{
  display: none;
}
.directionTop {
  transform: translateY(0rpx) rotate(180deg)
}
.bg{
  opacity: 0.1;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}