<view class="warp channel_store_list">
  <view class="title_wrap">
    <view class="channel_store_title" aria-label="{{floorName}}" aria-role="text">{{floorName}}</view>
    <view class="title_more " 
      data-to="{{data.to}}"
      data-params="{{data.params}}"
      data-user-action="{{data.topUserAction}}"
      bindtap="jumper"
      aria-label="跳转{{data.moreBtnDesc}}"
      aria-role="link"
      wx:if="{{data.moreBtnDesc}}">
      <text>{{data.moreBtnDesc}}</text>  
      <text class="more-icon icon dj_icon ts_next"></text>
    </view>
  </view>
  <view
    wx:for="{{storeList}}"
    wx:for-index="index"
    wx:for-item="item"
    wx:key="item"
    data-trace-id="{{traceId}}"
    data-user-action="{{item.userAction}}"
    data-page-id="{{pageId}}"
    data-current-page-name="{{currentPageName}}"
    data-pre-page-name='{{prePageName}}'
    class="store-item channel_comp_ep">
    <store-item
      item="{{item}}" 
      buriedObj="{{recommendObj}}"
      traceId="{{traceId}}"
      noNeedExposure="{{true}}"
    ></store-item>
  </view>
  <view
    wx:if="{{data.moreBtnDesc}}"
    class="more-store">
    <view
      class="flex align-center log_list_store_more more-store-button "
      data-to="{{data.to}}"
      data-type="more"
      data-params="{{data.params}}"
      data-user-action="{{data.userAction}}"
      bindtap="jumper">
      <view class="more-text">{{data.moreBtnDesc}}</view>
      <view
        class="more-icon icon dj_icon ts_next"
        data-icon-down="{{item.tagArrowDown}}"
        data-index="{{index}}"
      ></view>
    </view>
  </view>
</view>