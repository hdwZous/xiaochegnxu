@import "/common/css/iconfont.wxss";
.title_wrap{
    display: flex;
    justify-content: space-between;
}
.channel_store_title {
    font-size: 36rpx;
    font-weight: 500;
    color: #333333;
}
.title_more{
    font-size: 24rpx;
    color: #333333;
}

.store-item {
    margin-top: 24rpx;
    background: #FFFFFF;
    border-radius: 24rpx;
    overflow: hidden;
    /* padding: 24rpx; */
    /* display: flex; */
}

.store-img {
    height: 100rpx;
    width: 100rpx;
    /* overflow: hidden; */
    position: relative;
}

.image {
    height: 100rpx;
    width: 100rpx;
    border-radius: 8rpx;
    border:1rpx solid rgba(0, 0, 0, 0.08);
}

.store-info {
    max-width: 532rpx;
    padding-left: 16rpx;
    flex: 1;
    position: relative;
}

.flex {
    display: flex;
}

.between {
    justify-content: space-between;
}

.align-center {
    align-items: center;
}

.store-name {
    font-size: 32rpx;
    font-weight: 500;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 392rpx;
    flex: 1;
}

.padding-top__6 {
    padding-top: 12rpx;
}

.padding-top__7 {
    padding-top: 14rpx;
}

.store-start {
    display: flex;
    justify-content: center;
    align-items: center;
}

.store-score {
    font-size: 22rpx;
    font-weight: 400;
    color: #FF9415;
    padding-left: 2px;
}

.month-sale {
    padding-left: 10rpx;
    font-size: 22rpx;
    font-weight: 300;
    color: #333333;
}

.distance {
    font-size: 22rpx;
    font-weight: 300;
    color: #333333;
}

.store-default {
    font-size: 24rpx;
    color: #999;
}

.freight-words {
    font-size: 22rpx;
    font-weight: 400;
    color: #333;
}

.carrier-tag {
    border-radius: 2px;
    border: 1px solid;
    font-size: 20rpx;
    padding: 0 4rpx;
}

.store-coupon {
    display: flex;
    flex: 1;
    align-items: flex-start;
    justify-content: flex-start;
    padding-top: 10rpx ;
    flex-wrap: wrap;
    overflow: hidden;
}

.coupon-btn {
    padding-top: 20rpx;
    height: 28rpx;
    display: flex;
    align-items: center;
}

.coupon-item {
    overflow: hidden;
    margin: 10rpx 10rpx 0 0;
}

.coupon-tag {
    padding: 0 10rpx;
    position: relative;
    border-radius: 4rpx;
    font-size: 20rpx;
    white-space: nowrap;
}

.left-radio {
    position: absolute;
    width: 10rpx;
    height: 10rpx;
    left: -8rpx;
    top: 50%;
    transform: translateY(-50%);
    border-radius: 50%;
    background: #ffffff;
}

.right-radio {
    position: absolute;
    width: 10rpx;
    height: 10rpx;
    right: -8rpx;
    top: 50%;
    transform: translateY(-50%);
    border-radius: 50%;
    background: #ffffff;
}

.tags {
    padding-top: 12rpx;
    width: 100%;
}

.tag-list {
    width: 100%;
}

.tag-item {
    display: flex;
    align-items: flex-start;
    justify-content: flex-start;
    padding-top: 8rpx;
}

.tag-icon {
    border-radius: 4rpx;
    padding: 0 8rpx;
    font-size: 20rpx;
    font-weight: 400;
    color: #FFF;
}

.tag-words {
    flex: 1;
    font-size: 22rpx;
    padding-left: 18rpx;
    font-weight: 300;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 400rpx;
}

.tag-btn {
    padding-top: 15rpx;
}

.bros-store {
    padding-top: 40rpx;
}

.bros-store-title {
    margin-left: 10rpx;
    color: rgb(51, 51, 51);
    font-size: 22rpx;
}

.bros-store-icon {
    width: 26rpx;
    height: 26rpx;
    background-image: url("https://storage.360buyimg.com/wximg/channel/store-icon.png");
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.arrow-icon {
    padding-left: 30rpx;
    font-size: 14rpx;
    color: #666666;
}

.near-store {
    padding: 16rpx 0;
    border-top: 1px solid rgb(245, 245, 245);
}

.near-store:last-child {
    padding-bottom: 0;
}

.near-store-one {
    border: none;
    padding: 16rpx 0;
    padding-bottom: 0;
}

.near-store-name {
    font-size: 28rpx;
    color: rgba(51, 51, 51);
}

.near-store-desc {
    font-size: 24rpx;
    color: rgba(102, 102, 102);
}

.more-store {
    display: flex;
    justify-content: center;
    align-items: center;
}

.more-store-button {
    padding: 32rpx 40rpx;
}

.more-text {
    font-size: 24rpx;
    color: #333;
}

.more-icon {
    font-size: 20rpx;
}

.recommend{
    white-space: nowrap;
    margin-top: 20rpx;
}
.recommend_list{
    display: inline-block;
    margin-right: 6rpx;
    position: relative;
}
.recommend_label{
    position: absolute;
    left: 0;
    top: 0;
    height: 32rpx;
    font-size: 20rpx;
    background: linear-gradient(90deg, #FF5B57 0%, #FF1E19 100%);
    border-radius: 8rpx 0px 8px 0px;
    padding:0 8rpx;
    color:#fff;
}
.recommend_list:last-child{
    margin-right: 0;
}
.recommend_img{
    width: 126rpx;
    height: 126rpx;
    border-radius: 8rpx;
}
.real_price{
    font-family: regular;
    font-size: 24rpx;
    text-align: center;
    color:#FF1E19;
}
.store_num {
    position: absolute;
    right: -15rpx;
    top: -15rpx;
    width: 36rpx;
    height: 36rpx;
    line-height: 36rpx;
    text-align: center;
    color: #ffffff;
    padding: 0 6rpx;
    box-sizing: border-box;
    font-size: 22rpx;
    font-weight: 500;
    border-radius: 50%;
    background: rgba(255, 87, 87, 1);
}
.store_num_more {
    padding: 0 4rpx;
    min-width: 38rpx;
    border-radius: 18rpx;
    position: absolute;
    right: -15rpx;
    top: -15rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    color: #ffffff;
    font-size: 20rpx;
    font-weight: 500;
    background: rgba(255, 87, 87, 1);
}

.closeTip{
    display: flex;
    font-size: 24rpx;
    margin-top: 8rpx;
}
.reserveText{
    height: 32rpx;
    line-height: 32rpx;
    color:#fff;
    padding:0 8rpx;
    background: linear-gradient(135deg, rgb(76, 156, 255) 0%, rgb(36, 134, 255) 100%);
    border-radius: 4rpx 0 0 4rpx ;
}
.timeText{
    height: 29rpx;
    line-height: 29rpx;
    color: rgb(36, 134, 255);
    padding:0 8rpx;
    border: 1px solid rgb(36, 134, 255);
    border-radius: 0rpx 4rpx 4rpx 0rpx ;
}

.fresh_good_tag{
    width: 176rpx;
    height: 32rpx;
    background-repeat: no-repeat;
    background-position: right;
    background-size: contain;
}
.store-name-logo {
    display: flex;
    align-items: center;
}
.expand-image {
    min-width: 32rpx;
    height: 32rpx;
    background-image: url(https://storage.360buyimg.com/wxmini/newuser/pengzhangicon.png);
    background-repeat: no-repeat;
    background-size: 100% auto;
    margin-right: 8rpx;
}
.expand {
    display: inline-flex;
    height: 32rpx;
    background: linear-gradient(142deg, #FF6963 0%, #FF4141 100%);
    border-radius: 4rpx;
    align-items: center;
    margin-top: 12rpx;
    padding-right: 2rpx;
}
.expand-text {
    font-size: 24rpx;
    color: #fff;
    margin-right: 2rpx;
}
.expand-price {
    font-size: 32rpx;
    color: #fff;
    font-family: regular;
    line-height: 29rpx;
}
.store-mark {
    width: 56rpx;
    height: 28rpx;
}