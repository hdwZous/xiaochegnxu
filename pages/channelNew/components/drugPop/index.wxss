.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.ellipsis3 {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}
  
/* 价格 */
.pirce_item{
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
    margin-top: 8rpx;
}
.price-group{
    display: flex;
    align-items: baseline;
}
.price-group .main-price {
    font-size: 36rpx;
    font-family: regular;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    margin-right: 8rpx;
}

.price-group .mino-price {
    font-size: 24rpx;
    font-family: light;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.price-group .vip-icon {
    /* flex: 1; */
    width: 76rpx;
    height: 21rpx;
}

.price-group .gray-icon {
    font-size: 16rpx;
    line-height: 21rpx;
    padding-left: 10rpx;
    width: 76rpx;
    /* flex: 1; */
    height: 21rpx;
    color: #fff;
}


/* 弹层 */
.mask02{
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    background: rgba(0,0,0,0.5);
    z-index: 102;
}
.pop_container{
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    height: 80vh;
    transition: all ease 0.3s;
    z-index: 103;
}

.drug{
    display: flex;
    background: #fff;
    padding:16rpx;
    border-radius: 12rpx;
    margin: 0 24rpx;
}
.drug_content{
    flex: 1;
    max-width: 490rpx;
}
.drug_img{
    width: 160rpx;
    height: 160rpx;
    margin-right: 16rpx;
}
.drug_name{
    font-size: 28rpx;
    color: #333;
    margin-bottom: 16rpx;
}
.drug_desc{
    font-size: 24rpx;
    color:#999;
    line-height: 1.5;
}


/* 门店列表 */
.store_list{
  margin-top: 24rpx;
  background: #F6F6F6;
  border-radius: 20rpx 20rpx 0px 0px;  
  padding: 0 24rpx 24rpx 24rpx;
}
.store_list_title{
    font-size: 24rpx;
    color: #999;
    text-align: center;
    padding:16rpx 0;
    height: 34rpx;
    line-height: 34rpx;
}
.scroll{
    height: calc(80vh - 292rpx);
}
scroll-view {
    -webkit-overflow-scrolling: touch
}
  
/* 门店item */
.store-item {
    margin-bottom: 24rpx;
    background: #FFFFFF;
    border-radius: 24rpx;
    padding: 24rpx;
    display: flex;
}
.store-item:last-child{
    margin-bottom: 48rpx;
}


.store-img {
    height: 120rpx;
    width: 120rpx;
    border-radius: 8rpx;
    overflow: hidden;
}

.image {
    height: 120rpx;
    width: 120rpx;
}

.store-info {
    max-width: 532rpx;
    padding-left: 16rpx;
    flex: 1;
}

.flex {
    display: flex;
}

.between {
    justify-content: space-between;
}

.align-center {
    align-items: center;
}
.store-name-wrap{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.store-name {
    font-size: 32rpx;
    font-weight: 500;
    color: #333;
    flex: 1;
}
.store-icon{
    margin-right: 8rpx;
}
.store-icon:last-child{
    margin-right: 0rpx;
}
.store-icon-image{
    width: 82rpx;
    height: 32rpx; 
    position: relative;
    top:4rpx;
}
.padding-top__6 {
    padding-top: 12rpx;
}

.padding-top__7 {
    padding-top: 14rpx;
}

.store-start {
    display: flex;
    justify-content: center;
    align-items: center;
}

.store-score {
    font-size: 22rpx;
    font-weight: 400;
    color: #FF9415;
    padding-left: 2px;
}

.month-sale {
    padding-left: 10rpx;
    font-size: 22rpx;
    font-weight: 300;
    color: #333333;
}

.distance {
    font-size: 22rpx;
    font-weight: 300;
    color: #333333;
}

.store-default {
    font-size: 24rpx;
    color: #999;
}

.freight-words {
    font-size: 22rpx;
    font-weight: 400;
    color: #333;
}
.freight-line{
    font-size: 20rpx;
    color: #999;
    text-decoration: line-through;
}

.carrier-tag {
    border-radius: 2px;
    border: 1px solid;
    font-size: 20rpx;
    padding: 0 4rpx;
}

.store-coupon {
    display: flex;
    flex: 1;
    align-items: flex-start;
    justify-content: flex-start;
    padding-top: 10rpx ;
    flex-wrap: wrap;
    overflow: hidden;
}

.coupon-btn {
    padding-top: 20rpx;
    height: 28rpx;
    display: flex;
    align-items: center;
}

.coupon-item {
    overflow: hidden;
    margin: 10rpx 10rpx 0 0;
}

.coupon-tag {
    padding: 0 10rpx;
    position: relative;
    border-radius: 4rpx;
    font-size: 20rpx;
    white-space: nowrap;
}

.left-radio {
    position: absolute;
    width: 10rpx;
    height: 10rpx;
    left: -8rpx;
    top: 50%;
    transform: translateY(-50%);
    border-radius: 50%;
    background: #ffffff;
}

.right-radio {
    position: absolute;
    width: 10rpx;
    height: 10rpx;
    right: -8rpx;
    top: 50%;
    transform: translateY(-50%);
    border-radius: 50%;
    background: #ffffff;
}


.arrow-icon {
    padding-left: 30rpx;
    font-size: 14rpx;
    color: #666666;
}

.near-store {
    padding: 16rpx 0;
    border-top: 1px solid rgb(245, 245, 245);
}

.near-store-name {
    font-size: 28rpx;
    color: rgba(51, 51, 51);
}

.near-store-desc {
    font-size: 24rpx;
    color: rgba(102, 102, 102);
}

.more-store {
    display: flex;
    justify-content: center;
    align-items: center;
}

.more-store-button {
    padding: 32rpx 40rpx;
}

.more-text {
    font-size: 24rpx;
    color: #333;
}

.more-icon {
    font-size: 20rpx;
}

.pop-footer{
    display: flex;
    justify-content: space-between;
    margin-top: 20rpx;
    align-items: baseline;
}
.go-to-store{
    color: #333;
    font-size: 24rpx;
    display: flex;
    align-items: center;
}
.right-arrow{
    border-style: solid;
    border-color: rgb(102, 102, 102);
    border-left-width: 0px;
    border-bottom-width: 0px;
    border-top-width: 1px;
    border-right-width: 1px;
    width: 12rpx;
    height: 12rpx;
    transform: translateX(0px) rotate(45deg);
    /* transform-origin: center center; */
}



.skelton_left{
    width: 160rpx;
    height: 160rpx;
    border-radius: 8rpx;
    background: #F4F4F4;
    margin-right: 16rpx;
}
.skelton_right_content{
    width: 480rpx;
    background: #F4F4F4;
    height: 42rpx;
    border-radius: 12rpx;
    margin-bottom: 16rpx;
}
.wd260{
    width: 260rpx;
}
.wd146{
    width: 146rpx;
    margin-bottom: 0;
}

.closeTip{
    display: flex;
    font-size: 24rpx;
    margin-top: 8rpx;
}
.reserveText{
    height: 32rpx;
    line-height: 32rpx;
    color:#fff;
    padding:0 8rpx;
    background: linear-gradient(135deg, rgb(76, 156, 255) 0%, rgb(36, 134, 255) 100%);
    border-radius: 4rpx 0 0 4rpx ;
}
.timeText{
    height: 29rpx;
    line-height: 29rpx;
    color: rgb(36, 134, 255);
    padding:0 8rpx;
    border: 1px solid rgb(36, 134, 255);
    border-radius: 0rpx 4rpx 4rpx 0rpx ;
}
.white_blank {
    width: 100%;
    height: 48rpx;
    /* background-color: white; */
}
  