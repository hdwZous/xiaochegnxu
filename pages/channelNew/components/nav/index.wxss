.item{
    display: inline-block;
    width: 151rpx;
    height: 64rpx;
    line-height: 64rpx;
    background: #F6F6F6;
    border-radius: 32rpx;
    font-size: 24rpx;
    text-align: center;
    margin-right: 16rpx;
    margin-bottom: 16rpx;
    color: #666666;
    position: relative;
    overflow: hidden;
}
.item2{
    width: 162rpx;
}
.bg{
    opacity: 0.1;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
}
.name{
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
}
.ts_next{
    font-size: 18rpx;
    display: inline-block;
}
.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}