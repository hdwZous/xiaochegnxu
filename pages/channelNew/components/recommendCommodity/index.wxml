<view class="commodity-list {{data.styleTpl == 'tpl19' ? 'background' : ''}}">
  <view wx:if="{{data.styleTpl == 'tpl19' && data.floorTitle && data.floorTitle.floorName}}" class="brand-title">
    {{data.floorTitle.floorName}}
  </view>
  <view wx:if="{{data.floorStyle == 'act4' && data.styleTpl == 'tpl12'}}" class="plr12 pt12">
    <meeting-four
      meetingFourObj="{{data}}"
      floor-id="meetFour_{{floorIndex}}"
      img-lazy-load="{{imgLazyLoad}}"
      traceId="{{traceId}}"
      data-page-id="{{pageId}}"
      data-current-page-name="{{currentPageName}}"
      data-pre-page-name='{{prePageName}}'
      recommendObj="{{recommendObj}}"
    />
  </view>
  <view wx:elif="{{data.floorStyle == 'act3' && data.styleTpl == 'tpl13'}}" class="plr12 pt12">
    <meeting-three
      meetingThreeObj="{{data}}"
      floor-id="meetThree_{{floorIndex}}"
      img-lazy-load="{{imgLazyLoad}}"
      traceId="{{traceId}}"
      data-page-id="{{pageId}}"
      data-current-page-name="{{currentPageName}}"
      data-pre-page-name='{{prePageName}}'
      recommendObj="{{recommendObj}}"
    />
  </view>
  <!-- 手机频道四宫格楼层 -->
  <view class="wrap" wx:elif="{{data.styleTpl=='tpl15'}}">
    <view class="container" wx:if="{{data.data.length>2}}">
      <view
        class="commodity-list-item channel_comp_ep"
        wx:for="{{data.data}}"
        wx:key="index"
        data-to="{{item.to}}"
        data-trace-id="{{traceId}}"
        data-user-action="{{item.userAction}}"
        data-page-id="{{pageId}}"
        data-current-page-name="{{currentPageName}}"
        data-pre-page-name='{{prePageName}}'
        data-params="{{item.params}}"
        bind:tap="goToDetail"
        aria-label='促销广告图: {{item.title}}{{item.words}}'
        aria-role='link'
      >
        <image wx:if="{{item.itemBgImg}}" class="item-bg-image" src="{{item.itemBgImg}}" />
        <view class="item_img">
          <image class="item_img_src" src="{{item.imgUrl}}" style="border:{{item.imgStrokeColor ? '1px solid ' + item.imgStrokeColor : ''}}" />
        </view>
        <view class="img_title">{{item.title}}</view>
        <view class="img_subtitle" style="color:{{item.wordsColor}}">{{item.words}}</view>
      </view>
    </view>
    <view class="container02" wx:if="{{data.data.length==2}}">
      <view
        class="item02 channel_comp_ep"
        wx:for="{{data.data}}"
        wx:key="index"
        data-to="{{item.to}}"
        data-trace-id="{{traceId}}"
        data-user-action="{{item.userAction}}"
        data-page-id="{{pageId}}"
        data-current-page-name="{{currentPageName}}"
        data-pre-page-name='{{prePageName}}'
        data-params="{{item.params}}"
        bind:tap="goToDetail"
        aria-label='促销广告图: {{item.title}}{{item.words}}'
        aria-role='link'>
        <image wx:if="{{item.itemBgImg}}" class="item-bg-image02" src="{{item.itemBgImg}}" />
        <view class="item02_left">
          <view>
            <view class="img_title" style="margin-top:0">{{item.title}}</view>
            <view class="img_subtitle" style="color:{{item.wordsColor}}">{{item.words}}</view>
          </view>
        </view>
        <view class="item_right">
          <image class="item_img_src" src="{{item.imgUrl}}" style="border:{{item.imgStrokeColor ? '1px solid ' + item.imgStrokeColor : ''}}" />
        </view>
      </view>
    </view>
  </view>
  <block wx:elif="{{data.styleTpl == 'tpl19'}}">
    <view wx:for="{{commodityGroup}}" wx:for-index="index" wx:for-item="groupItem" wx:key="item" class="brand-group ">
      <view
        wx:for="{{groupItem}}"
        wx:key="imgUrl"
        wx:for-item="item"
        class="brand-item channel_comp_ep"
        data-to="{{item.to}}"
        data-params="{{item.params}}"
        data-type='detail'
        data-trace-id="{{traceId}}"
        data-user-action="{{item.userAction}}"
        data-page-id="{{pageId}}"
        data-current-page-name="{{currentPageName}}"
        data-pre-page-name='{{prePageName}}'
        bindtap="goToDetail"
        aria-label='跳转至{{item.title}}活动页'
        aria-role='link'>
        <image src="{{item.imgUrl}}" class="brand-img" lazy-load widthFix />
        <view class="brand-name">{{item.title}}</view>
      </view>
    </view>
  </block>
  <block wx:else>
    <view wx:for="{{commodityGroup}}" wx:for-index="index" wx:for-item="groupItem" wx:key="item" class="commodity-group ">
      <view
        wx:for="{{groupItem}}"
        wx:key="imgUrl"
        wx:for-item="item"
        class="commodity-item channel_comp_ep"
        data-to="{{item.to}}"
        data-params="{{item.params}}"
        data-type='detail'
        aria-label='促销广告图'
        aria-role='link'
        data-trace-id="{{traceId}}"
        data-user-action="{{item.userAction}}"
        data-page-id="{{pageId}}"
        data-current-page-name="{{currentPageName}}"
        data-pre-page-name='{{prePageName}}'
        bindtap="goToDetail"
      ><image src="{{item.imgUrl}}" class="commodity-img" lazy-load widthFix />
      </view>
    </view>
  </block>
</view>