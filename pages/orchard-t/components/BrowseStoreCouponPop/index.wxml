<template name="renderstoreCouponLayer"><block><view class="{{style.toStoreCouponBox}}"><view class="{{style.store_coupon_box}}"><text class="{{style.store_coupon_top_title}}">先领券再逛更爽哦</text><view class="{{style.store_coupon_item}}"><view class="{{style.store_coupon_item_left}}"><text class="{{style.store_coupon_left_num}}">{{couponResult.amount}}</text><text class="{{style.store_coupon_left_money}}">{{couponResult.amountUnit}}</text></view><view class="{{style.store_coupon_item_right}}"><text class="{{[style.store_coupon_right_title, 'ellipsis']}}">{{couponResult.couponTitle}}</text><text class="{{style.store_coupon_right_limit}}">{{couponResult.limitRule}}</text></view></view><view bindtap="clickStrCoupBtn" class="{{style.store_coupon_bottom_btn}}" data-e-tap-so="this"><block><block wx:if="{{couponResult.markState == 2}}"><view class="{{style.store_coupon_bottom_btn_text}}">去领取</view></block><block wx:else><view class="{{style.store_coupon_bottom_btn_text}}">去使用</view></block></block></view></view><view class="{{style.store_coupon_close}}" bindtap="closeCouponLayer" data-e-tap-so="this"></view></view></block></template>
<block wx:if="{{$taroCompReady}}"><view><block wx:if="{{isShow}}"><template is="renderstoreCouponLayer" data="{{...anonymousState__temp}}"></template></block></view></block>
