<template name="renderItemNotClick"><block><block><block wx:if="{{item.status == 0 && item.finishType == 300 && item.extraFinishMQType == 602}}"><button class="taskBtn taskBtnNotHandle shareBtn" open-type="share" data-params="{{anonymousState__temp}}" bindtap="isShareEvent" data-e-tap-so="this" data-e-tap-a-a="{{item.taskId}}" data-e-tap-a-b="{{item.taskType}}">{{item.buttonText}}</button></block><block wx:else><block><block wx:if="{{item.status == 0 && true && item.taskType == 306}}"><view class="taskBtn taskBtnNotHandle" bindtap="clickTaskEvent" data-e-tap-so="this" data-e-tap-a-a="{{item}}"><button class="taskBtn buttonTextCont btnTransparent" open-type="contact" send-message-title="{{shareInfo.title}}" send-message-img="{{shareInfo.shareUrl}}" show-message-card="true" session-from="WorkWechatMission_Farm">{{item.buttonText}}</button></view></block><block wx:else><block wx:if="{{item.status == 0}}"><view class="taskBtn taskBtnNotHandle" bindtap="clickTaskEvent" data-e-tap-so="this" data-e-tap-a-a="{{item}}"><view class="buttonTextCont">{{item.buttonText}}</view></view></block></block></block></block></block></block></template>
<block wx:if="{{$taroCompReady}}"><view><view class="{{style.container}}"><view class="{{style.box}}"><view class="{{style.textTitle}}">{{sceneTaskInfo.taskName ? sceneTaskInfo.taskName : ''}}</view><view class="{{style.imgbg}}"><image src="https://storage.360buyimg.com/wximg/orchard/icon_double_drop.png" class="{{[style.img, style.initimg]}}"></image></view><view class="{{style.text}}">{{sceneTaskInfo.taskSubTitle ? sceneTaskInfo.taskSubTitle : ''}}</view><view class="{{style.btn}}"><view class="{{style.sure}}" style="background-size: 100% 100%;" bindtap="handleGetWater" data-e-tap-so="this"><view class="{{style.getWaterBtn}}"></view></view></view></view></view></view></block>
