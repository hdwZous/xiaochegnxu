.city-wrap{
    padding-bottom: 20px;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
    background: #FFFFFF;
    width:100%;
  }
  
  .city-index-wrap{
    position: fixed;
    right: 5px;
    top: 50%;
    transform: translate(0,-50%);
    display: flex;
    flex-direction: column;
    font-size: 12px;
    color: #999999;
    width:60rpx;
    justify-content: flex-end;
  }
  .city-index-wrap-view {
    width: 16px;
    height: 16px;
    text-align: center;
    font-size: 11px;
    display: flex;
    align-items: center;
    justify-content: center;
  } 
  .city-index-wrap-view-text{
    color: #FFFFFF;
    font-size: 11px;
  }
  .city-index-wrap-view-text1{
    color: #999999;
    font-size: 11px;
  }
  .selectedCity{
    background: #47B34F;
    border-radius: 8px;
    
  }
  .sub-city-wrap{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    width: 100%;
    padding-top: 20px;
  }
  .sub-city-text{
    font-size: 12px;
    color: #999999;
  }
  .sub-city-content{
    display: flex;
    align-self: stretch;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    margin-top: 10px;
    flex-wrap: wrap;
   
  }

  .fre-city-wrap{
    padding: 20px 0px 0 10px;
    background: #FFFFFF;
  }
  .fre-list-wrap{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    margin-top: 10px;
    flex-wrap: wrap;
  }
  .fre-city-head{
    display: flex;
    justify-content: space-between;
    align-items: center;
    
  }
  .fre-city-head view{
    font-size: 12px;
    color: #666666;
    padding-right: 10px;
  }
  .fre-city-head text{
    font-size: 16px;
    font-weight: bold;
  }
  .confirm_dialog_content{
    color:#666666;
    font-size: 34rpx;
    text-align: center;
    margin-top: 80rpx
}