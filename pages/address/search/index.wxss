@import "/common/css/iconfont.wxss";
.search {
    height: 100vh;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
}
.search .case {
    padding: 20rpx 30rpx;
    -webkit-flex: none;
    flex: none;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    background: #47B34F;
}
.case .position{
    -webkit-flex: none;
    flex: none;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
    overflow: hidden;
}
.case .name {
    -webkit-flex: auto;
    flex: auto;
    color: #FFFFFF;
    font-size: 30rpx;
    max-width: 200rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.case .icon_arrow {
    padding: 0 10rpx;
    -webkit-flex: none;
    flex: none;
    width: 14rpx;
    height: 8rpx;
