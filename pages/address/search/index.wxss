.line {
    width: 100%;
    height: 1px;
    background: #E8E8E8;
}
.section .icon_location {
    padding: 5rpx 10rpx 0 0;
}
.section .info {
    -webkit-flex: auto;
    flex: auto;
}
.section .name {
    color: #333333;
    font-size: 30rpx;
    -webkit-flex-direction: row;
    flex-direction: row;
    overflow: hidden;
}
.section .address {
    margin-top: 15rpx;
    color: #999999;
    font-size: 26rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}
.section .clear_btn {
    padding: 50rpx 0;
    text-align: center;
}
.section .clear_history {
    display: inline-block;
    position: relative;
    font-size: 28rpx;
    padding: 10rpx 30rpx;
    color: #47B34F;
}
.section .clear_history:before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    border: 1px solid #47B34F;
    border-radius: 16rpx;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}
.section .no_history {
    color: #999999;
    font-size: 28rpx;
    text-align: center;
}
.section .no_history_img {
    width: 320rpx;
    height: 320rpx;
    display: block;
    margin: 0 auto;
}
.cities {
    padding-bottom: 20rpx;
}
.cities .case {
    position: relative;
    margin: 20rpx 20rpx 10rpx;
    height: 60rpx;
}
.cities .title {
    padding: 0 20rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    color: #999999;
    font-size: 24rpx;
    background: #FFFFFF;
}
.cities .case:before {
    content: "";
    position: absolute;
    left: 0;
    top: 50%;
    width: 200%;
    height: 1px;
    background: #979797;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5) translateY(-50%);
    transform: scale(0.5) translateY(-50%);
}
.cities .wrap {
    padding: 0 10rpx;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
}
.cities .item {
    width: 25%;
    margin-top: 20rpx;
    padding: 0 10rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.cities .text {
    position: relative;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    color: #333333;
    font-size: 30rpx;
}
.cities .text:before {
    position: absolute;
    left: 0;
    top: 0;
    content: "";
    width: 200%;
    height: 200%;
    border: 1px solid #E8E8E8;
    border-radius: 4rpx;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}
.map {
    position: relative;
    -webkit-flex: none;
    flex: none;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
    height: 104rpx;
    padding: 0 30rpx;
}

.map:before {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    width: 200%;
    height: 1px;
    background: #E8E8E8;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}

.map .icon_location {
    -webkit-flex: none;
    flex: none;
    margin-right: 20rpx;
    width: 40rpx;
    height: 40rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}

.map .txt {
    -webkit-flex: auto;
    flex: auto;
    color: #47B34F;
    font-size: 32rpx;
}

.map .icon_arrow {
    -webkit-flex: none;
    flex: none;
    width: 32rpx;
    height: 32rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}
