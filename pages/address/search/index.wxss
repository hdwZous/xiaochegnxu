    -webkit-background-size: 14rpx 8rpx;
    background-size: 14rpx 8rpx;
}
.case .input_case {
    padding-left: 20rpx;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
    -webkit-flex: auto;
    flex: auto;
    height: 60rpx;
    line-height: 60rpx;
    background: #f4f4f4;
    border-radius: 60rpx;
}
.case .icon_search {
    margin-right: 10rpx;
    -webkit-flex: none;
    flex: none;
    width: 28rpx;
    height: 28rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}
.case .input {
    -webkit-flex: auto;
    flex: auto;
}
.case .input>input {
    width: 100%;
    height: 100%;
    color: #999999;
    font-size: 24rpx;
}
.case .clear {
    width: 80rpx;
    height: 100%;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 28rpx 28rpx;
    background-size: 28rpx 28rpx;
}
.search .section {
    -webkit-flex: auto;
    flex: auto;
    position: relative;
}
.section .scroll {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}
.section .up {
    background: #FFFFFF;
}
.section .title {
    color: #333333;
    font-size: 34rpx;
    padding: 30rpx 20rpx 0;
    font-weight: 500;
}
.section .item {
    position: relative;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    overflow: hidden;
    padding: 30rpx 20rpx;
}
