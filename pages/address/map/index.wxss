.wrap {
    height: 100vh;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
}
.wrap .search{
    padding: 20rpx 30rpx;
    -webkit-flex: none;
    flex: none;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    background: #47B34F;
}
.search .position{
    -webkit-flex: none;
    flex: none;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
    overflow: hidden;
}
.search .name {
    -webkit-flex: auto;
    flex: auto;
    color: #FFFFFF;
    font-size: 30rpx;
    max-width: 200rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.search .icon_arrow {
    padding: 0 10rpx;
    -webkit-flex: none;
    flex: none;
    width: 14rpx;
    height: 8rpx;
    background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABUAAAAMCAYAAACNzvbFAAAAAXNSR0IArs4c6QAAAGFJREFUKBVjYKABYPz//38O0FwRKpr9hgFoqD4QvwFiagCQOfpgB4IYQEypwQgDYd6m0GBMAyk0GLeBZBpM2EASDSbeQCINJt1AAgaTbyAOgyk3EMlgcWCSSwRicZgYPhoAonTQNkDV9SYAAAAASUVORK5CYII=') center no-repeat;
    -webkit-background-size: 14rpx 8rpx;
    background-size: 14rpx 8rpx;
}
.search .input_case {
    padding-left: 20rpx;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
    -webkit-flex: auto;
    flex: auto;
    height: 60rpx;
    line-height: 60rpx;
    background: #f4f4f4;
    border-radius: 60rpx;
}
.search .icon_search {
    margin-right: 10rpx;
    -webkit-flex: none;
    flex: none;
    width: 28rpx;
    height: 28rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}
.search .input {
    -webkit-flex: auto;
    flex: auto;
}
.search .input>input {
    width: 100%;
    height: 100%;
    color: #999999;
    font-size: 24rpx;
}
.search .clear {
    width: 80rpx;
    height: 100%;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 28rpx 28rpx;
    background-size: 28rpx 28rpx;
}
.wrap .section {
    position: relative;
    -webkit-flex: auto;
    flex: auto;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
}
.wrap .hide {
    display: none;
}
.section .map_case {
    position: relative;
    -webkit-flex: auto;
    flex: auto;
}
.section .map {
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: 1;
}
.section .controls {
    width:71rpx;
    height:71rpx;
    position:absolute;
    bottom:40rpx;
    left:20rpx;
    z-index: 1;
}
.section .marker {
    width: 52rpx;
    height: 72rpx;
    position: absolute;
    z-index: 1;
    left: 50%;
    top: 50%;
    margin-top: -36rpx;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.section .address {
    position: relative;
    -webkit-flex: auto;
    flex: auto;
}
.section .scroll,
.section .default {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}
.result {
    overflow-x: hidden;
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 100;
    background: #ffffff;
}
.section .item {
    position: relative;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    overflow: hidden;
    padding: 30rpx 20rpx;
}
.result .cover_view_case{
    padding-left: 20rpx;
}
.result .line:before {
    z-index: 3;
    content: "";
    position: absolute;
    bottom: 0;
    left: 20rpx;
    width: 200%;
    height: 1px;
    background: #E8E8E8;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);

}
.section .item:before {
    /* content: "";
    position: absolute;
    bottom: 0;
    left: 20rpx;
    width: 200%;
    height: 1px;
    background: #E8E8E8;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5); */
}
.line1 {
    width: 100%;
    height: 1px;
    background: #E8E8E8;
}
.section .icon_location {
    -webkit-flex: none;
    flex: none;
    width: 25rpx;
    margin: 0 20rpx 0 20rpx;
    position: relative;
    top: 6rpx;
}
.section .icon_location_sub {
    -webkit-flex: none;
    flex: none;
    width: 40rpx;
    background: url("data:image/png;base64,") left 8rpx no-repeat;
    -webkit-background-size: 22rpx 26rpx;
    background-size: 22rpx 26rpx;
}
.section .info {
    -webkit-flex: auto;
    flex: auto;
    overflow: hidden;
}
.section .name {
    display: -webkit-flex;
    display: flex;
    overflow: hidden;
}
.section .txt {
    max-width:90%;
    -webkit-flex: none;
    flex: none;
    color: #333333;
    font-size: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.section .tag {
    margin-left: 10rpx;
    -webkit-flex: none;
    flex: none;
    width: 52rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    color: #FFFFFF;
    font-size: 20rpx;
    border-radius: 4rpx;
    background: #5FBC65;
    position: relative;
    top: 4rpx;
    margin-right: 20rpx;
}
.section .address {
    margin-top: 15rpx;
    color: #999999;
    font-size: 26rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}
.section .search_list {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: red;
}
