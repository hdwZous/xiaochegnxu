page {
    background: #f4f4f4;
}
.deliver {
    height: 68rpx;
    line-height: 68rpx;
    text-align: center;
    background: #FFF6CD;
    font-size: 24rpx;
    color: #D97900;
}
.wrap {
    padding: 0 20rpx;
    background: #FFFFFF;
}
.wrap .item {
    height: 104rpx;
    color: #333333;
    font-size: 30rpx;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
}
.wrap .line {
    position: relative;
    height: 1px;
}
.wrap .line:before {
    content: "";
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    width: 200%;
    height: 1px;
    background: #E8E8E8;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}
.wrap .left {
    -webkit-flex: none;
    flex: none;
}
.wrap .middle {
    -webkit-flex: auto;
    flex: auto;
    display: -webkit-flex;
    display: flex;
}
.wrap .input {
    width: 100%;
}
.wrap .default_value {
    color: #999999;
}
.wrap .tag {
    margin-right: 30rpx;
    -webkit-flex: none;
    flex: none;
    position: relative;
    width: 96rpx;
    height: 44rpx;
    line-height: 44rpx;
    text-align: center;
    color: #666666;
    font-size: 28rpx;
    padding: 1rpx 0 0 0;
}
.wrap .tag.active {
    color: #FFFFFF;
    background: #47B34F;
    border-radius: 4rpx;
    padding: 1rpx 0;
}
.wrap .tag:before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    border: 1px solid #E8E8E8;
    border-radius: 8rpx;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}
.wrap .tag.active:before {
    display: none;
}
.wrap .right {
    -webkit-flex: none;
    flex: none;
    width: 32rpx;
    height: 32rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}
.save{
    color: #FFFFFF;
    font-size: 32rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border-radius: 4rpx;
    background: #47B34F;
    margin: 80rpx 30rpx;
}

.delete {
    margin: 0 auto;
    width: 96rpx;
    height: 96rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}

.mask{
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background:rgba(0,0,0,0.7);
}
.content{
    z-index: 9999;
    position: absolute;
    left:50%;
    top:50%;
    width: 80%;
    background: #fff;
    transform: translate3d(-50%,-50%,0);
}










