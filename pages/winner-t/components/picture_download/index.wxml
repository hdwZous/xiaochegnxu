<block wx:if="{{$taroCompReady}}">
    <block>
        <block wx:if="{{visible}}">
            <view class="{{styles.download_picture}}">
                <view class="{{styles.picBox}}">
                    <image class="{{styles.close}}" src="https://image3.imdada.cn/7022704994a14838ad249213df333e3c.png" bindtap="anonymousFunc0"></image>
                    <image mode="aspectFit" class="{{styles.pic}}" src="{{imgUrl}}"></image>
                </view>
                <view class="{{styles.buttonBox}}" bindtap="anonymousFunc1">
                    <image class="{{styles.button}}" src="https://image3.imdada.cn/219fa0d144ef47e08b97a6b80e42baff.png"></image>
                    <view class="{{styles.text}}">保存图片</view>
                </view>
            </view>
        </block>
        <block wx:else>
            <view></view>
        </block>
    </block>
</block>