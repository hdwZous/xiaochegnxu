<view wx:if="{{showEmpty}}" class="loading_case">
    <view class="loading"></view>
</view>
<block wx:else>
    <view class="wrap">
        <!-- 按钮 -->
        <view class="btn">
            <image src="https://storage.360buyimg.com/wximg/webank4.0/btn{{btnStatus}}.png" class="img" bindtap="clickBtn"></image>
            <block wx:if="{{btnStatus === 20}}">
                 <checkbox-group class="protocol" bindchange="checkboxChange">
                    <label class="checkbox">
                        <view class="proto_icon {{checked ? 'proto_icon_checked' : ''}}"></view>
                        <checkbox style="display:none" checked="{{checked}}" />
                        阅读并同意
                    </label>
                </checkbox-group>
                <view class="protocol_wrap">《<text class="protocol_text" bindtap="goToProtocol">京东到家隐私政策</text>》、《<text class="protocol_text" bindtap="goToProtocol">微众银行微常准服务协议</text>》</view>
            </block>
        </view>
        <!-- 底部说明 -->
        <!-- <view class="bottom">
            <image mode="widthFix" class="img" src="https://storage.360buyimg.com/wximg/webank3.0/logo.png"></image>
        </view> -->
    </view>
</block>