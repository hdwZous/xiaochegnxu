<view class="share_content">
    <view class="share_bg">
        <view class="share_title">投放活动名称</view>
        <text class="share_msg">{{codeInfo.shareTitle}}</text>
    </view>
    <view class="share_bg">
        <view class="share_title">投放活动ID</view>
        <text class="share_msg">{{codeInfo.dj_par_key}}</text>
    </view>
    <view class="share_bg">
        <view class="share_title">跳转页面</view>
        <text class="share_msg">{{codeInfo.jumpTypeName}}</text>
    </view>
    <view class="share_bg">
        <view class="share_title">分享卡片标题</view>
        <text class="share_msg">{{shareInfo.title}}</text>
    </view>
    <view class="share_bg">
        <view class="share_title">分享卡片图</view>
        <image src="{{shareInfo.imageUrl}}" class="share_image"></image>
    </view>
    <view class="share_bg">
        <view class="share_title">分享卡路径</view>
        <text class="share_msg">{{shareInfo.path}}</text>
    </view>
</view>
<view class="share_btn">
    <button bindtap="toShare" type="primary" open-type="share" >生成分享卡片</button>
</view>