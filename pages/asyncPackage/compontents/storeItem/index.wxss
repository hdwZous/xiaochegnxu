.marL24 {
  margin-left: 24rpx;
}
.wrap{
  background: #FFFFFF;
  border-radius: 24rpx;
}
.store_case {
  padding-top: 20rpx;
}
.store_left {
  padding: 0 16rpx 0 20rpx;
}
.store_logo {
  position: relative;
  width: 116rpx;
  height: 116rpx;
  border-radius: 4px;
  border: 1px solid rgba(0, 0, 0, 0.08);
}
.store_img {
  width: 100%;
  height: 100%;
  border-radius: 4px;
}
.store_num {
  position: absolute;
  right: -15rpx;
  top: -15rpx;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  color: #ffffff;
  font-size: 20rpx;
  font-weight: 500;
  border-radius: 50%;
  background: rgba(255, 87, 87, 1);
}
.store_num_more {
  padding: 0 4rpx;
  min-width: 38rpx;
  border-radius: 18rpx;
  position: absolute;
  right: -15rpx;
  top: -15rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  color: #ffffff;
  font-size: 20rpx;
  font-weight: 500;
  background: rgba(255, 87, 87, 1);
}
.rest_tag {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  text-align: center;
  color: #ffffff;
  font-size: 20rpx;
  background: rgba(204, 204, 204, 1);
  border-radius: 0 0 8rpx 8rpx;
}
.store_right {
  padding-bottom: 20rpx;
  overflow: hidden;
  position: relative;
}
.go_store_btn{
  position: absolute;
  right: 0rpx;
  top: 24rpx;
  width: 120rpx;
  height: 52rpx;
  background: linear-gradient(270deg, #00CF37 0%, #0FDF46 100%);
  border-radius: 28rpx;
  font-size: 28rpx;
  font-weight: 600;
  color: #FFFFFF;
  line-height: 52rpx;
  box-sizing: border-box;
  padding-left: 24rpx;
}
.go_store_btn::before{
  content: '';
  width: 12rpx;
  height: 20rpx;
  position: absolute;
  top: 16rpx;
  right: 24rpx;
  background: url(https://storage.360buyimg.com/wximg/store/store_overhalf_gotostoreicon.png) no-repeat center;
  background-size: 100% 100%;
}
.store_name_case {
  align-items: center;
  max-width: 100%;
  /* max-width: 392rpx; */
}
.store_name {
  color: #333333ff;
  font-size: 32rpx;
  font-weight: bold;
  overflow: hidden;
}
.close-tip-wrap {
  margin-top: 6rpx;
}
.line_one {
  margin-top: 8rpx;
  align-items: center;
}
.line_one_left {
  align-items: center;
}
.line_one_right {
  text-align: right;
  color: #666666ff;
  font-size: 22rpx;
}
.line_two_right_tag {
  width: 88rpx;
  height: 28rpx;
  border-radius: 8rpx 2rpx 8rpx 2rpx;
  border: 1px solid #20acff;
  color: #20acffff;
  font-size: 20rpx;
  text-align: center;
}
.line_three {
  height: 34rpx;
  margin-top: 16rpx;
  /* padding-right: 60rpx; */
  position: relative;
  overflow: hidden;
}
.line_three_icon {
  position: absolute;
  right: 0;
  top: 0;
  width: 60rpx;
  font-size: 14rpx;
  color: #666666;
  text-align: right;
  height: 32rpx;
  line-height: 32rpx;
}

.coupon_tag_case {
  overflow: hidden;
  margin-right: 10rpx;
  margin-bottom: 5px;
  /* margin-bottom: 10rpx; */
}
.coupon_tag {
  height: 30rpx;
  line-height: 30rpx;
  padding: 0 10rpx;
  position: relative;
  border-radius: 4rpx;
  font-size: 20rpx;
}
.left_radio {
  position: absolute;
  width: 10rpx;
  height: 10rpx;
  left: -8rpx;
  top: 50%;
  transform: translateY(-50%);
  border-radius: 50%;
  background: #ffffff;
}
.right_radio {
  position: absolute;
  width: 10rpx;
  height: 10rpx;
  right: -8rpx;
  top: 50%;
  transform: translateY(-50%);
  border-radius: 50%;
  background: #ffffff;
}
.line_four {
  /* padding-right: 60rpx; */
  position: relative;
  margin-top: 20rpx;
}

.line_five {
  /* padding-right: 60rpx; */
  position: relative;
}
.line_five_head {
  align-items: center;
}
.line_five_icon {
  color: #666666;
  font-size: 24rpx;
}
.line_five_title {
  margin-left: 10rpx;
  color: rgb(51, 51, 51);
  font-size: 22rpx;
}
.nearby_store {
  padding: 16rpx 0;
  border-top: 1px solid rgb(245, 245, 245);
}
.nearby_store_name {
  color: #333;
  font-size: 22rpx;
}
.nearby_store_distance {
  color: #333;
  font-size: 22rpx;
}

/* 膨胀 */
.expasion {
  height: 32rpx;
  border-radius: 4rpx;
  position: relative;
  box-sizing: border-box;
  padding-left: 40rpx;
  padding-right: 12rpx;
  display: inline-flex;
}

.expasion.down {
  margin-right: 10rpx;
}

.expasion .titile_img {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  left: 0;
}

.expasion_text {
  font-size: 24rpx;
  color: #fff;
  height: 32rpx;
  line-height: 32rpx;
  /* max-width: 160rpx; */
  box-sizing: border-box;
}

.expasion_price {
  font-size: 32rpx;
  color: #fff;
  height: 32rpx;
  line-height: 32rpx;
  font-family: regular;
}

/* 膨胀 */
.coupon-and-tags-wrapper {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}
.coupon-and-tags {
  flex: 1;
  min-width: 0;
}
.coupon-wrap {
  flex-wrap: wrap;
  align-items: flex-start;
}

.coupon-and-tags_icon {
  flex-shrink: 0;
  /* width: 60rpx; */
  width: 32rpx;
  font-size: 14rpx;
  color: #666666;
  text-align: right;
  height: 32rpx;
  line-height: 32rpx;
  margin-top: 14rpx;
}

.marL16 {
  margin-left: 16rpx;
}

.marR20 {
  margin-right: 20rpx;
}

.store_item_space_line {
  height: 1px;
  background: #EDEDED;
  margin: 20rpx 0;
}

.right_top_left_wrap {
  flex: 1;
  overflow: hidden;
}