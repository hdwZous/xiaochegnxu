@import "/common/css/global.wxss";
.bold {
    font-weight: bold;
}
.font28 {
    font-size: 28rpx;
}
.spu_bg {
    z-index: 100;
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.6);
}
.spu_content {
    z-index: 100;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #ffffff;
    border-radius: 20rpx 20rpx 0 0;
}
.spu_case {
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
}
.spu_info {
    position: relative;
}
.scroll {
    position: absolute;
    top: 20rpx;
    left: 0;
    width: 100%;
    height: 100%;
    padding: 0 20rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.goods_info {
    padding: 30rpx 20rpx 20rpx 20rpx;
    background-color: #fff;
    position: relative;
    z-index: 1;
}
.image_case {
    position: relative;
    width: 210rpx;
    height: 210rpx;
    background: #f4f4f4;
    margin-right: 20rpx;
    border-radius: 8rpx;
}
.image {
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
}
.tags {
    padding: 0 5rpx;
    position: absolute;
    left: 0;
    top: 0;
    max-width: 100%;
    color: #ffffff;
    font-size: 20rpx;
    border-radius: 8rpx 0 8rpx 0;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.info {
    overflow: hidden;
    padding-top: 74rpx;
}
.name {
    color: #333333;
    font-size: 32rpx;
    font-weight: bold;
    /* display: flex;
    align-items: center */
}
.sku-name-tag {
  font-size: 20rpx;
  padding: 2rpx 8rpx;
  color: #fff;
  border-radius: 4rpx;
  margin-right: 6rpx;
  display: inline-table;
  margin-top: 6rpx;
  line-height: 30rpx;
  vertical-align: top;
}
.price_major_case {
    display: flex;
    /* align-items: baseline; */
    align-items: center;
}
.promotion {
    color: #ff5757;
    font-size: 36rpx;
    font-weight: 500;
    margin-right: 12rpx;
    font-family: "regular";
}
.qi {
    font-size: 20rpx;
    margin-right: 10rpx;
}
.base {
    font-size: 28rpx;
    margin-left: 4rpx;
}
.vip_icon {
    height: 20rpx;
    width: 70rpx;
}
.vip {
    margin-left: 10rpx;
    font-size: 28rpx;
    font-weight: 500;
}
.tips {
    color: #333333;
    font-size: 28rpx;
}
.select {
    margin-top: 30rpx;
}
.title {
    margin-bottom: 30rpx;
    font-size: 28rpx;
    color: #333333;
}
.item {
    display: inline-flex;
    justify-content: flex-start;
    box-sizing: border-box;
    align-items: center;
    max-width: 702rpx;
    min-width: 60rpx;
    text-align: center;
    font-size: 28rpx;
    color: #333333;
    background: rgba(248, 248, 248, 1);
    border-radius: 30rpx;
    padding: 4rpx 20rpx;
    margin-right: 20rpx;
    margin-bottom: 18rpx;
}
.attr-value-name {
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
}
.attr-value-price {
    margin-left: 8rpx;
    font-family: regular;
    flex-shrink: 0;
}
.disabled {
    color: #cccccc;
}
.selected {
    background: rgba(71, 179, 79, 0.1);
    color: #47b34f;
}
.number_case {
    -webkit-align-items: center;
    align-items: center;
    margin-top: 50rpx;
}
.number {
    color: #333333;
    font-size: 28rpx;
}
.controller {
    padding-right: 30rpx;
}
.case {
    -webkit-justify-content: flex-end;
    justify-content: flex-end;
    height: 52rpx;
}
.middle {
    width: 96rpx;
    line-height: 52rpx;
    text-align: center;
    background: #f8f8f8;
    color: #333333;
    font-size: 28rpx;
    border-radius: 4rpx;
}
.left {
    margin-right: 20rpx;
    width: 32rpx;
    background: url("https://storage.360buyimg.com/wximg/spu/spu_disabled.png")
        no-repeat center;
    -webkit-background-size: 32rpx 32rpx;
    background-size: 32rpx 32rpx;
    border-radius: 4rpx;
}
.left_active {
    width: 32rpx;
    background: url("https://storage.360buyimg.com/wximg/spu/spu_min.png")
        no-repeat center;
    -webkit-background-size: 32rpx 32rpx;
    background-size: 32rpx 32rpx;
}
.right {
    margin-left: 20rpx;
    width: 32rpx;
    background: url("https://storage.360buyimg.com/wximg/spu/spu_add.png")
        no-repeat center;
    -webkit-background-size: 32rpx 32rpx;
    background-size: 32rpx 32rpx;
}
.btn_case {
    padding: 20rpx;
}
.btn {
    text-align: center;
    font-size: 32rpx;
    color: #ffffff;
    background: rgba(255, 255, 255, 1);
    height: 80rpx;
    line-height: 80rpx;
    background: -webkit-linear-gradient(
        270deg,
        rgba(0, 190, 52, 1) 0%,
        rgba(0, 203, 30, 1) 100%
    );
    background: linear-gradient(
        270deg,
        rgba(0, 190, 52, 1) 0%,
        rgba(0, 203, 30, 1) 100%
    );
    box-shadow: 0 8rpx 16rpx 0 rgba(0, 190, 50, 0.14);
    border-radius: 40rpx;
}
.close {
    position: absolute;
    right: 8rpx;
    top: 20rpx;
    z-index: 2;
    width: 60rpx;
    height: 60rpx;
    background: red;
    text-align: center;
    line-height: 60rpx;
    background: url("https://storage.360buyimg.com/wximg/store/close.png")
        no-repeat center;
    -webkit-background-size: 36rpx 36rpx;
    background-size: 36rpx 36rpx;
}
.notice_price {
    border-bottom: 1px solid rgb(244, 244, 244);
    font-weight: bold;
    color: #333333;
    font-size: 32rpx;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
}
.notice_wrap {
    margin-top: 20rpx;
    height: 100rpx;
    align-items: center;
    border-top: 1px solid rgb(244, 244, 244);
}
.notice_img_case {
    margin-right: 20rpx;
}
.notice_img {
    width: 28rpx;
    height: 28rpx;
    vertical-align: middle;
}
.notice_txt {
    padding: 20rpx;
    align-items: center;
    font-size: 24rpx;
    color: rgb(51, 51, 51);
    border-bottom: 1px solid rgb(244, 244, 244);
    background: #fff;
    position: relative;
    z-index: 1;
}
.notice_title {
    font-size: 28rpx;
    color: rgb(51, 51, 51);
}
.notice_phone {
    font-weight: bold;
}
.price_btn {
    position: relative;
    width: 72rpx;
    height: 44rpx;
    background: #00CF37;
    border-radius: 22rpx;
}
.price_btn .price_btn_radio {
    position: absolute;
    right: 4rpx;
    top: 50%;
    transform: translateY(-50%);
    width: 36rpx;
    height: 36rpx;
    background: #fefffe;
    box-shadow: 0px 0px 3px 0px rgba(0, 0, 0, 0.1);
    border-radius: 36rpx;
}
.price_gray_btn {
    position: relative;
    width: 72rpx;
    height: 44rpx;
    background: #ccc;
    border-radius: 22rpx;
}
.price_gray_btn .price_btn_radio {
    position: absolute;
    left: 4rpx;
    top: 50%;
    transform: translateY(-50%);
    width: 36rpx;
    height: 36rpx;
    background: #fefffe;
    box-shadow: 0px 0px 3px 0px rgba(0, 0, 0, 0.1);
    border-radius: 36rpx;
}

/* 手机增值服务 */
.added_value_wrap {
    margin-top: 42rpx;
}
.added_value_line {
    width: 710rpx;
    height: 2rpx;
    margin: 0 auto;
    margin-bottom: 50rpx;
    background: linear-gradient(180deg, rgba(238, 238, 238, 0) 0%, #F5F5F5 100%);
}
.added_value_title {
    color: #333;
    font-weight: bold;
    font-size: 28rpx;
    margin-bottom: 30rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.added_value_list .group_item {
    margin-bottom: 60rpx;
}
.added_value_list .group_item:last-child {
    margin-bottom: 0rpx !important;
}
.added_value_list .title_wrap {
    display: flex;
    position: relative;
}
/* 服务标题 */
.added_value_list .group_title {
    font-size: 28rpx;
    color: #333;
    font-weight: 400;
    margin-left: 10rpx;
}
.added_value_list .ground_icon {
    width: 36rpx;
    height: 36rpx;
}
.ground_icon .img  {
    width: 100%;
    height: 100%;
}
/* 服务说明 */
.service_exp {
    position: absolute;
    right: 0;
    top: 2rpx;
    display: flex;
    align-items: center;
} 
.service_exp  .service_exp_text {
    font-size: 24rpx;
    font-weight: 400;
    color: #348BED;
}
.service_exp .service_exp_icon {
    width: 24rpx;
    height: 24rpx;
    margin-left: 10rpx;
}
.service_exp_icon  .img {
    width: 100%;
    height: 100%;
    display: block;
}
/* 服务项 */
.add_values_wrap {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
}
.add_values_wrap .sub_item_wrap {
    height: 60rpx;
    border-radius: 30rpx;
    display: flex;
    align-items: center;
    width: 345rpx;
    margin-top: 40rpx;
    position: relative;
}
/* 不可点击状态 */
.add_values_wrap .add_value_disabled {
    background: rgba(248, 248, 248, 1);
    color: #ccc;
}
/* 未选中状态 */
.add_values_wrap .sub_item_unselect {
    background: #F8F8F8;
    color: #333;
}
/* 已选中状态 */
.add_values_wrap .sub_item_select {
    background: #ECF7ED;
    color: #47B34F
}
.sub_item_wrap .sku_name {
    font-weight: 400;
    max-width: 208rpx;
    text-align: center;
    margin-left: 20rpx;
    flex: auto;
}
.sub_item_wrap .center {
    width: 1rpx;
    height: 20rpx;
    background: #CCCCCC;
    margin: 0 16rpx;
}
.price {
    /* width: 68rpx; */
    text-align: center;
    padding-right: 20rpx;
}
.bubble {
    position: absolute;
    top: -26rpx;
    right: 0rpx;
    font-size: 20rpx;
    font-weight: 400;
    height: 26rpx;
    padding: 0 8rpx;
    line-height: 26rpx;
    border-radius: 13rpx;
}
.bubble_unselect {
    background: #FBDEDA;
    color: #FF1D19;
}
.bubble_unselect::after {
    content: '';
    background-image: url('https://storage.360buyimg.com/wximg/common/bubble_unselect.png');
    background-size: cover;
    width: 16rpx;
    height: 6rpx;
    position: absolute;
    bottom: -6rpx;
    left: 16rpx
}
.bubble_select {
    color: #FFFFFF;
    background: #FF1D19;
}
.bubble_select::after {
    content: '';
    background-image: url('https://storage.360buyimg.com/wximg/common/bubble_select.png');
    background-size: cover;
    width: 16rpx;
    height: 6rpx;
    position: absolute;
    bottom: -6rpx;
    left: 16rpx
}
.base-price {
    display: inline-block;
    text-decoration: line-through;
    margin-right: 4rpx;
    max-width: 80rpx;
}
.zhe {
    display: inline-block;
    max-width: 80rpx;
}


/* 优惠套装 */
.suit_list_wrap {
    margin-top: 48rpx;
}
.suit_list_wrap >  .suit_list_title {
    color: #333;
    font-weight: bold;
    font-size: 28rpx;
    height: 30rpx;
    line-height: 30rpx;
    /* margin-bottom: 30rpx; */
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.suit_list_wrap > .suit_list {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
}
.suit_list > .suit_item {
    height: 60rpx;
    border-radius: 30rpx;
    width: 312rpx;
    margin-top: 24rpx;
    text-align: center;
    line-height: 60rpx;
    font-size: 28rpx;
    padding: 0 16rpx;
}
.suit_item_unselect {
    background: #F8F8F8;
    color: #333;
}
.suit_item_disabled {
    background: #F6F6F6;
    color: #999;
}
.suit_item_select {
    background: #EBFCEF;
    color: #00CF37;
}
.no_support_tip {
    font-size: 24rpx;
    font-weight: 500;
    color: #FF1E1A;
    margin-left: 16rpx;
}

.blank {
    width: 100%;
    height: 120rpx;
}
.suit_list_line {
    width: 710rpx;
    height: 2rpx;
    margin: 0 auto;
    margin-bottom: 48rpx;
    background: linear-gradient(180deg, rgba(238, 238, 238, 0) 0%, #F5F5F5 100%);
}


.pre-sale-wrap {
    margin-top: 58rpx;
}
.pre-sale-title {
    font-size: 28rpx;
    font-weight: 400;
    color: #333333;
}
.pre-sale-content {
    width: 702rpx;
    padding: 12rpx 24rpx 24rpx 24rpx;
    background: #F6F6F6;
    border-radius: 14rpx;
    margin-top: 30rpx;
}
.pre-sale-item {
    margin-top: 12rpx;
    display: flex;
    justify-content: flex-start;
    font-size: 24rpx;
    font-weight: 300;
    color: #333333;
}
.sale-title {
    width: 86rpx;
}
.sale-desc {
    flex: 1;
}
.pre-sale-limit {
    width: 394rpx;
    height: 34rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #666666;
    line-height: 34rpx;
    margin-left: 20rpx;
}
.btn-disabled {
    background: rgb(204, 204, 204) !important;
}
.btn-gray{
    background: #CCCCCC !important;
}
.good-process-title {
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}
/* 鲜活加工服务 */
.good-process-content {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-top: 28rpx;
}
.good-process-item {
  padding: 0 24rpx;
  height: 56rpx;
  line-height: 56rpx;
  background: #F8F8F7;
  border-radius: 28rpx;
  font-size: 28rpx;
  color: #333;
  margin-right: 16rpx;
  margin-bottom: 18rpx;
}
.process-selected {
  background: rgba(0, 207, 55, 0.08);
  color: #00CF37;
}

.store-sku-remain {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 28rpx;
  font-size: 20rpx;
  color: #fff;
  text-align: center;
  line-height: 28rpx;
  border-radius: 0 0 8rpx 8rpx;
  background: linear-gradient(to right, #ff5c37 0%, #ff9248 100%);
}
.base-price {
  font-size: 24rpx;
  color: rgb(153, 153, 153);
  text-decoration-line: line-through;
}
.suit-price-text {
  font-size: 28rpx;
  color: #333;
}
/* 订阅成功弹层 */
.subscribePop{
	margin: 0 106rpx;
	margin-top: 50%;
	transform: translateY(-50%);
	background: #fff;
	border-radius: 24rpx;
	text-align: center;
	padding: 40rpx 40rpx 0;
}
.subscribePop .title{
	font-size: 36rpx;
	font-weight: 500;
	color: #333333;
}
.subscribePop .content{
	font-size: 28rpx;
	font-weight: 300;
	color: #666666;	
	margin-top: 10rpx;
}
.subscribePop .sub_btn{
	height: 104rpx;
	line-height: 104rpx;
	font-size: 36rpx;
	font-weight: 500;
	color: #47B34F;
}