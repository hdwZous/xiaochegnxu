}
.vplus-icon {
  width: 168rpx;
  height: 40rpx;
  margin: 0 auto;
  margin-bottom: 32rpx;
}
.vplus-icon image {
  width: 100%;
  height: 100%;
}

.vplus-close {
  background-image: url(https://storage.360buyimg.com/wxmini/minicart/vmember_close.png);
  background-repeat: no-repeat;
  background-size: 50% 50%;
  background-position: center;
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 0;
  right: 12rpx;
}
.oneShot {
  display: flex;
  padding: 0 24rpx;
