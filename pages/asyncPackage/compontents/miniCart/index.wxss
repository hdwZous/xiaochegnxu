@-webkit-keyframes scale {
  0% {
    transform: scale(0.75);
    -webkit-transform: scale(0.75);
  }
  10% {
    transform: scale(0.8);
    -webkit-transform: scale(0.8);
  }
  20% {
    transform: scale(0.9);
    -webkit-transform: scale(0.9);
  }
  40% {
    transform: scale(1.1);
    -webkit-transform: scale(1.1);
  }
  60% {
    transform: scale(1.2);
    -webkit-transform: scale(1.2);
  }
  80% {
    transform: scale(1.1);
    -webkit-transform: scale(1.1);
  }
  100% {
    transform: scale(1);
    -webkit-transform: scale(1);
  }
}

@keyframes scale {
  0% {
    transform: scale(0.75);
    -webkit-transform: scale(0.75);
  }
  10% {
    transform: scale(0.8);
    -webkit-transform: scale(0.8);
  }
  20% {
    transform: scale(0.9);
    -webkit-transform: scale(0.9);
  }
  40% {
    transform: scale(1.1);
    -webkit-transform: scale(1.1);
  }
  60% {
    transform: scale(1.2);
    -webkit-transform: scale(1.2);
  }
  80% {
    transform: scale(1.1);
    -webkit-transform: scale(1.1);
  }
  100% {
    transform: scale(1);
    -webkit-transform: scale(1);
  }
}

