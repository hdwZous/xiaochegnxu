.floor-container {
  padding-top: 48rpx;
  padding-bottom: 24rpx;
  border-radius: 24rpx;
}
.floor-title-wrap {
  margin: 0rpx 24rpx 24rpx;
}
.floot-title-url {
  width: 264rpx;
  height: 36rpx;
}
.floot-title-url image {
  width: 100%;
  height: 100%;
}
.floor-title {
  height: 36rpx;
  font-size: 28rpx;
  font-weight: 600;
  color: #333333;
  line-height: 36rpx;
}
.floor-coupon-wrap {
  margin-top: 16rpx;
}
.show-more {
  margin-top: 24rpx;
  text-align: center;
}
.show-more-title {
  height: 32rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
  line-height: 32rpx;
  display: inline-block;
}
.show-more-arrow {
  display: inline-block;
  transform: rotateZ(90deg);
  font-size: 22rpx;
  transition: all 0.5s;
  margin-left: 4rpx;
}
.rotate {
  transform: rotateZ(90deg);
  font-size: 22rpx;
  color: #999;
}
.rotate1 {
  transform: rotateZ(-90deg);
  font-size: 22rpx;
  color: #999;
}