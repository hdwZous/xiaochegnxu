.floor-title-wrap {
  margin: 48rpx 24rpx 0;
}
.floor-title {
  height: 36rpx;
  font-size: 28rpx;
  font-weight: 600;
  color: #333333;
  line-height: 36rpx;
}
.floor-coupon-container {
  width: 702rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 0 auto;
}
.floor-coupon-wrap {
  margin-top: 16rpx;
}
.show-more {
  margin-top: 24rpx;
  text-align: center;
}
.show-more-title {
  height: 32rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
  line-height: 32rpx;
  display: inline-block;
}
.show-more-arrow {
  display: inline-block;
  transform: rotateZ(90deg);
  font-size: 22rpx;
  transition: all 0.5s;
  margin-left: 4rpx;
}
.rotate {
  transform: rotateZ(90deg);
  font-size: 22rpx;
  color: #999;
}
.rotate1 {
  transform: rotateZ(-90deg);
  font-size: 22rpx;
  color: #999;
}