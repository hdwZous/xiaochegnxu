.cp-yhe-one-wrap {
  width: 344rpx;
  height: 156rpx;
  border-radius: 16rpx;
  margin: 0 auto;
  display: flex;
  justify-content: flex-start;
  overflow: hidden;
  position: relative;
}
.cp-yhe-left-one {
  width: 120rpx;
  height: 101%;
  box-sizing: border-box;
  flex-direction: column;
  justify-content: center;
  display: flex;
  position: relative;
}

.cp-yhe-center-one {
  flex: auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.cp-yhe-line {
  width: 16rpx;
  height: 100%;
  background: url('https://storage.360buyimg.com/wximg/common/coupon/cp-yhe-one-line.png') no-repeat;
  background-size: 100% 100%;
}
.cp-yhe-right-one {
  width: 46rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.cp-yhe-price-one {
  display: flex;
  justify-content: center;
  align-items: flex-end;
}
.yhe-price-unit-one {
  font-size: 24rpx;
  font-family: regular;
  font-weight: 400;
  line-height: 32rpx;
}
.yhe-price-value-one {
  font-size: 56rpx;
  font-family: regular;
  font-weight: 400;
  line-height: 1;
}
.cp-yhe-limit-one {
  height: 28rpx;
  font-size: 20rpx;
  font-weight: 400;
  margin-top: 14rpx;
  line-height: 28rpx;
  text-align: center;
  padding: 0 8rpx;
}
.cp-yhe-title-one {
  max-width: 160rpx;
  height: 32rpx;
  font-size: 24rpx;
  font-weight: 600;
  color: #333333;
  line-height: 32rpx;
}
.cp-yhe-tags-one {
  width: 160rpx;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
  height: 36rpx;
  margin-top: 4rpx;
}
.cp-yhe-tag-one {
  max-width: 144rpx;
  padding: 0 8rpx;
  margin-bottom: 6rpx;
  margin-right: 8rpx;
  font-size: 24rpx;
  height: 32rpx;
  font-weight: 400;
  position: relative;
  top: 2rpx;
  left: 2rpx;
  overflow: initial;
  display: flex;
  align-items: center;
}
.cp-yhy-tag-text-one{
  line-height: 24rpx;
  max-width: 100%;
  overflow: hidden;
}
.cp-yhy-tag-border-one{
  position: absolute;
  width: 200%;
  height: 200%;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%) scale(0.5);
  transform-origin: center;
  border-radius: 16rpx;
}
.cp-yhe-time-one {
  width: 160rpx;
  min-height: 28rpx;
  max-height: 56rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: #999999;
  line-height: 28rpx;
  margin-top: 4rpx;
}
.cp-yhe-button-one {
  width: 24rpx;
  font-size: 24rpx;
  font-weight: 400;
  white-space: normal;
  line-height: 32rpx;
  margin-left: 6rpx;
}
.item-content-gray-one {
  display: flex;
  width:344rpx;
  height: 156rpx;
  border-radius: 8rpx;
  background: #fff;
  margin: 0 auto;
}