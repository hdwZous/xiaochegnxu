.pop-wrapper {
  width: 590rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 17;
}
.pop-container {
  width: 590rpx;
  height: 780rpx;
  position: relative;
}
.vip-button {
  position: absolute;
  bottom: 56rpx;
  left: 58rpx;
  width: 475rpx;
  height: 104rpx;
  border-radius: 56rpx;
  line-height: 104rpx;
  text-align: center;
  font-size: 38rpx;
  font-weight: 500;
}
.close {
  display: block;
  width: 76rpx;
  height: 76rpx;
  margin: 60rpx auto 0;
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 16;
  background: rgba(0, 0, 0, 0.6);
}