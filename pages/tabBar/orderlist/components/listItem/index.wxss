@import "/common/css/global.wxss";
@import "../../public.wxss";
page {
    background-color: #fafafa;
}
.map {
    width: calc(100% - 10px);
    z-index: 2;
    overflow: hidden;
}
.line {
    height: 1rpx;
    border-top: 1rpx solid #f5f5f5;
    margin-left: 20rpx;
    margin-right: 20rpx;
}
.order-list-item {
    border-radius: 12rpx;
    position: relative;
    /* margin-bottom: 20rpx; */
    color: #484848;
    border-top: 1rpx solid #f5f5f5;
    border-bottom: 1rpx solid #f5f5f5;
    font-size: 28rpx;
    background: #fff;
    max-width: 100%;
}


/*第一项订单, 去 顶部边框 */

.order-list-item:first-child {
    border-top: none;
}


/*门店信息*/
.order-item-header {
    display: flex;
    font-size: 32rpx;
    padding-left: 20rpx;
    padding-top: 30rpx;
    padding-bottom: 4rpx;
    white-space: nowrap;
    line-height: normal;
    margin-bottom: 12rpx;
}
.order-item-imgs {
    width: 100rpx;
    height: 100rpx;
    border-radius: 8rpx;
}
.store-logo {
    min-width: 84rpx;
    max-width: 84rpx;
    height: 84rpx;
    border-radius: 8rpx;
    border: 1px solid rgba(0, 0, 0, 0.08);
}
.header-right {
    margin-left: 20rpx;
    padding-right: 24rpx;
    overflow: hidden;
}

/*门店标题*/
.name-wrap {
   
}
.order-item-header .order-name {
    font-weight: bold;
}

.order-item-header .icon_arrow {
    display: inline-block;
    width: 22rpx;
    height: 22rpx;
    background: url("https://storage.360buyimg.com/wximg/order/order_right_arrow.png") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}
.close-tip-wrap {
    display: flex;
    align-items: center;
    margin-top: 6rpx;
}
.close-tip {
    display: inline-block;
    border-radius: 4rpx;
    color: #fff;
    font-size: 24rpx;
    padding: 2rpx 8rpx;
}
.time-tip {
    background-color: #fff;
    padding: 2rpx 8rpx;
}

/* 优惠券 */
.list-coupon {
    width: 75vw;
    overflow-x: scroll;
    /* text-overflow: ellipsis;
    white-space: nowrap;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical; */
}
.item-coupon {
    display: inline-block;
    white-space: nowrap;
    word-break: keep-all;
    font-size: 20rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #FF1E19;
    border-radius: 4rpx;
    border: 2rpx solid rgba(255, 30, 25, 0.5);
    padding: 2rpx 12rpx;
    margin-right: 8rpx;
}
.item-coupon.bg-img {
    /* background: url('https://storage.360buyimg.com/wxmini/map/bg-coupon-middle.png') no-repeat center;
    background-size: 100%; */
    /* border: 0; */
    position: relative;
}
.bg-coupon-left {
    position: absolute;
    left: 0;
    top: 0;
    height: 32rpx;
    width: 22rpx;
}
.bg-coupon-middle {
    position: absolute;
    left: 22rpx;
    right: 22rpx;
    top: 0;
    height: 32rpx;
    z-index: 999;
}
.bg-coupon-right {
    position: absolute;
    right: 0;
    top: 0;
    height: 32rpx;
    width: 22rpx;
}

/*订单头部状态类型*/

.order-item-header .order-status {
    /* position: absolute; */
    /* top: 32rpx;
    right: 20rpx; */
    /* width: 184rpx; */
    text-align: right;
    font-size: 28rpx;
    color: #333;
}

/* 下单时间 */
.order-item-time{
    font-size: 24rpx;
    color:#999;
    padding-left: 20rpx;
    padding-bottom: 20rpx;
}
.order-item-time02{
    font-size: 24rpx;
    color:#999;
    padding-bottom: 12rpx;
}

/*订单列表项容器*/

.order-list-item-content {
    display: flex;
    width: 100%;
    position: relative;
    padding-left: 20rpx;
    box-sizing: border-box;
}
.order-item-product-list {
    align-items: center;
}
.order-item-number {
    color: #999;
    margin-top: 0;
    font-size: 28rpx;
}
.goodInfo {
    display: flex;
    margin: 20rpx 20rpx 0 0;
    justify-content: space-between;
    align-items: center;
}
.tag-time-wrap {
    display: flex;
}
.order-item-money {
    color: #333;
    font-size: 32rpx;
    font-family: JDDJ-1-Regular, JDDJ-1;
    font-weight: bold;
    margin-top: 0;
}

/*订单状态面板, 订单信息面板*/
.order-item-info {
    flex: 1;
}

.order-item-status-info {
    border-radius: 8rpx;
    margin-right: 10px;
    line-height: 20px;
    font-size: 13px;
    color: #999;
    background: #fafafa;
    display: flex;
    justify-content: space-between;
    position: relative;
}

.order-status-header {
    padding: 15px 5px 15px 15px;
}
.order-status-header-first{
    font-size: 28rpx;
    color:#333;
}
.order-status-header-second{
    font-size: 24rpx;
    color:#999;
}
.order-status-right{
    display: flex;
    align-items: center;
    padding-right: 22rpx;
    padding-left: 22rpx;
}
.order-status-right image{
    width: 26rpx;
    height: 26rpx;
}
.after_sale image{
    width: 20rpx;
    height: 20rpx;
}
/* 订单状态信息, -- 气泡框箭头 */
.order-item-status-info::after{
    position: absolute;
    width: 24rpx;
    height: 24rpx;
    border-radius: 8rpx;
    background: #fafafa;
    content: " ";
    right: 30rpx;
    top: -10rpx;
    transform: rotate(45deg);
}


.order-item-footer {
    display: flex;
    padding-right: 10px;
    /* padding-top: 8rpx; */
    padding-bottom: 30rpx;
    line-height: 45px;
    justify-content: space-between;
    position: relative;
    /* border-top: 1rpx solid #f5f5f5; */
}


/*去支付按钮*/

.order-item-footer .txt {
    display: inline-block;
    margin-left: 10px;
    padding: 4rpx 0rpx;
    color: #333;
    border: 1px solid #d9d9d9;
    border-radius: 2px;
    text-align: center;
    font-size: 14px;
    line-height: 26px;
    width: 210rpx;
}


/*第一个按钮边距*/

.order-item-footer text:first-child {
    margin-left: 0;
}

.order-item-footer .order-list-to-pay {
    background: #FF5757;
    color: #fff;
    /* border: none; */
    border-radius: 32rpx;
    font-size: 28rpx;
    margin-top: 16rpx;
    padding: 0 32rpx;
    height: 64rpx;
    line-height: 64rpx;
    width: auto;
}

.order-list-to-code {
    background-color: #eaeaea;
    color: 666666;
    border: 1px solid #d9d9d9;
    border-radius: 2px;
}


.order-list-delivery {
    color: #ee7a12;
    font-size: 20rpx;
    border: 1px solid #ffaa0d;
    border-radius: 4rpx;
    line-height: 1;
}

.self-tag {
    color: #47b34f;
    font-size: 20rpx;
    border: 1px solid #47b34f;
    border-radius: 4rpx;
    line-height: 28rpx;
}

.store-tag {
    background: #999;
    font-size: 20rpx;
    border: 1px solid #999;
    border-radius: 6rpx;
    line-height: 28rpx;
    padding: 0 6rpx;
}


/*拼团*/

.group_tips {
    background: -webkit-linear-gradient(135deg, rgba(254, 106, 72, 1), rgba(255, 63, 63, 1));
    background: linear-gradient(135deg, rgba(254, 106, 72, 1), rgba(255, 63, 63, 1));
    border-radius: 16rpx 2rpx 16rpx 2rpx;
    font-size: 20rpx;
    color: #fff;
    height: 35rpx;
    line-height: 35rpx;
    width: 100rpx;
    display: inline-block;
    text-align: center;
    position: absolute;
    bottom: 20rpx;
    left: 50%;
    margin-left: -50rpx;
}

.group_buy {
    padding: 20rpx 20rpx 30rpx 0;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
}

.group_buy .left {
    -webkit-flex: auto;
    flex: auto;
    color: #333;
    font-size: 24rpx;
}

.group_buy .item {
    line-height: 1.8em;
}

.group_buy .right {
    -webkit-flex: none;
    flex: none;
    display: -webkit-flex;
    display: flex;
    -webkit-align-items: center;
    align-items: center;
    -webkit-justify-content: center;
    justify-content: center;
}

.group_buy .btn {
    width: 160rpx;
    height: 60rpx;
    line-height: 60rpx;
    -webkit-align-items: center;
    align-items: center;
    text-align: center;
    border-radius: 4rpx;
    background: #ff5757;
    color: #fff;
    font-size: 28rpx;
    border-radius:30rpx;
}

.group_buy button::after {
    border: 0;
}

.group_buy .btn_share {
    width: 160rpx;
    height: 60rpx;
    line-height: 60rpx;
    -webkit-align-items: center;
    align-items: center;
    text-align: center;
    border-radius: 4rpx;
    background: #ff5757;
    color: #fff;
    font-size: 28rpx;
    border-radius: 30rpx;
}
.groupAgainSwitch{
    border:1rpx solid #DADADA;
    background: #fff !important;
    color: #333 !important;
}

.group_buy .split {
    color: rgba(0, 0, 0, 0.1);
}


/* y预售团标识 */

.group-goodinfo {
    flex: auto;
}

.flags {
    align-items: center;
    justify-content: flex-end;
    margin-top: 2rpx;
}

.pre-flag {
    color: #FF5757;
    padding: 0 6rpx;
    line-height: 28rpx;
    font-size: 20rpx;
    border: 1rpx solid #ff5757;
    margin-right: 10rpx;
    border-radius: 6rpx;
}

.flags .dada-tag {
    margin: 0;
    position: relative;
    padding: 0;
    line-height: 34rpx;
    height: 34rpx;
    transform: scale(0.8);
}
.tag-right-wrap {
  display: flex;
  align-items: center;
}
.total-discount-tag {
  font-size: 28rpx;
  color: #20acff;
  border: 1rpx solid #9E9E9E;
  border-radius: 4rpx;
  padding-left: 6rpx;
  padding-right: 6rpx;
  height: 36rpx;
  line-height: 36rpx;
  transform: scale(0.8);
  position: relative;
  text-align: center;
  display: inline-block;
}

.pay-status {
    padding-bottom: 20rpx;
}
.flags .collection {
    padding:0 6rpx;
    line-height:28rpx;
    font-size:20rpx;
    color:#47B34F;
    border:1rpx solid #47B34F;
    border-radius:6rpx;
    margin-right: 10rpx;
}

.order-pay-link {
    align-items: center
}
.load-tip {
    font-size: 30rpx;
    line-height: 30rpx;
    color: #666;
    margin: 10rpx 0;
    text-align: center;
}
.self-tag {
    color: #47b34f;
    border-color: #47b34f;
    border-radius: 8rpx 0 8rpx 0;
}
.store-tag {
    color: #ffffff;
    background: #999999;
    border-color: #999999;
    border-radius: 8rpx 0 8rpx 0;
}

.transparent-tag {
    color: transparent;
    border-color: transparent;
}
.dada-tag {
  font-size: 24rpx;
  color: #20acff;
  border: 1rpx solid #20acff;
  border-radius: 8rpx 0 8rpx 0;
  padding-left: 4rpx;
  padding-right: 4rpx;
  height: 34rpx;
  line-height: 34rpx;
  transform: scale(0.8);
  position: relative;
  /* right: -8rpx; */
  width: 106rpx;
  text-align: center;
  display: inline-block;
}

.hack-wrap{
    margin: 20rpx 20rpx 0;
  }
.line-gray{
    background: #f6f6f6;
    height: 10rpx;
}

.refill {
    display: flex;
    justify-content: flex-end;
    padding: 0 20rpx 40rpx 0;
}

.refill-button {
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    line-height: 28rpx;
    padding: 14rpx 40rpx;
    border: 1px solid;
    border-radius: 32rpx;
}

.again_buy {
  width: 192rpx;
  height: 64rpx;
  line-height: 64rpx;
  border-radius: 16px;
  border: 1px solid #DADADA;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  margin-top: 16rpx;
  margin-left: 20rpx;
}

.name_wrap_box {
  flex: 1;
  overflow: hidden;
}
