<block wx:if="{{showFeed}}">
  <!-- feed标题 -->
  <view class="feeds_title_case flex-row" 
        aria-role="text"
        aria-label="{{titleText}}"
        wx:if="{{title}}">
      <image aria-hidden="true" class="feeds_img flex-none" src="{{title}}" />
  </view>

  <!-- feed流 -->
  <view class="list_wrap feeds-sku" id="feeds-sku">
      <!-- 广告图 -->
      <view class="ad_banner" wx:if="{{banner.length}}">
          <image class="ad_banner_img" src="{{banner[0].imgUrl}}" data-item="{{banner[0]}}" bind:tap="clickBanner" />
      </view>
      <!-- 二级分类 -->
      <view class="list_second_tab flex-row" wx:if="{{subTabs.length}}">
          <view class="list_second_case flex-none flex-row" wx:for="{{subTabs}}" wx:for-item="subItem" wx:for-index="subIndex" wx:key="item">
              <view class="list_second_tab_item {{secondActiveTab == subItem.tagId ? 'list_second_tab_item_active':''}}" data-item="{{subItem}}" bind:tap="clickSecondTab">
                  {{subItem.name}}
              </view>
          </view>
      </view>
      <view class="list_case flex-row">
          <view class="list_item_case  left flex-none">
              <block wx:for="{{leftList}}" wx:for-item="item" wx:for-index="index" wx:key="item">
                  <view class="list_item">
                      <!-- 商品 type:1 -->
                      <goods wx:if="{{item.type == '1'}}" fromPositon="tabbar-orderlist-feed" item="{{item.data}}" traceId="{{traceId}}" pageId="{{pageId}}" currentPageName="{{currentPageName}}" prePageName="{{prePageName}}" buriedObj="{{recommendObj}}"/>
                      <!-- 热门店铺 type:2 -->
                      <hot-store wx:if="{{item.type == '2'}}" fromPositon="tabbar-orderlist-feed" item="{{item.data}}" buriedObj="{{recommendObj}}"/>
                      <!-- 分类 type:3 属于推荐下发，单独调接口，前端自己拼，不做。 -->
                      <!-- type:4 8.5以下是这个，以上的改成type7。这个不做 -->
                      <!-- 特色商品 type:5 -->
                      <local-goods wx:if="{{item.type == '5'}}" fromPositon="tabbar-orderlist-feed" item="{{item.data}}" buriedObj="{{recommendObj}}"/>
                      <!-- type:6 医药才会有，首页不会有，不做。 -->
                      <!-- 轮播banner type:7 -->
                      <pic-swiper wx:if="{{item.type == '7'}}" list="{{item.data.resources}}" buriedObj="{{recommendObj}}"/>
                      <!-- 战略宫格 type:8 -->
                      <strategy-little wx:if="{{item.type == '8'}}" item="{{item.data.resources[0]}}" buriedObj="{{recommendObj}}"/>
                      <!-- 图文宫格 type:9 、4宫格 type:10-->
                      <grid-little wx:if="{{item.type == '9' ||  item.type == '10'}}" fromPositon="tabbar-orderlist-feeds" type="{{item.type}}" data="{{item.data}}"  buriedObj="{{recommendObj}}"/>
                  </view>
              </block>
          </view>
          <view class="list_item_case right flex-none">
              <block wx:for="{{rightList}}" wx:for-item="item" wx:for-index="index" wx:key="item">
                  <view class="list_item">
                      <!-- 商品 type:1 -->
                      <goods wx:if="{{item.type == '1'}}" item="{{item.data}}"  traceId="{{traceId}}" pageId="{{pageId}}" currentPageName="{{currentPageName}}" prePageName="{{prePageName}}" buriedObj="{{recommendObj}}"/>
                      <!-- 热门店铺 type:2 -->
                      <hot-store wx:if="{{item.type == '2'}}" fromPositon="tabbar-orderlist-feed" item="{{item.data}}" buriedObj="{{recommendObj}}"/>
                      <!-- 特色商品 type:5 -->
                      <local-goods wx:if="{{item.type == '5'}}" fromPositon="tabbar-orderlist-feed" item="{{item.data}}" buriedObj="{{recommendObj}}"/>
                      <!-- 轮播banner type:7 -->
                      <pic-swiper wx:if="{{item.type == '7'}}" list="{{item.data.resources}}" />
                      <!-- 战略宫格 type:8 -->
                      <strategy-little wx:if="{{item.type == '8'}}" item="{{item.data.resources[0]}}" />
                      <!-- 图文宫格 type:9 、4宫格 type:10-->
                      <grid-little wx:if="{{item.type == '9' ||  item.type == '10'}}" fromPositon="tabbar-orderlist-feed" type="{{item.type}}" data="{{item.data}}" buriedObj="{{recommendObj}}"/>
                  </view>
              </block>
          </view>
      </view>
  </view>
  <!-- 已经到底啦提示 -->
  <view wx:if="{{isLastPage}}" class="reach_bottom">已经到底啦</view>
</block>