.top_tips {
  width: 100%;
  height: 72rpx;
  display: flex;
  line-height: 72rpx;
  background: #FFE7BC;
  align-items: center;
  position: fixed;
  z-index: 2;
}
.tips_image {
  width: 88rpx;
  height: 72rpx;
}
.tips_text {
  margin: 0 16rpx 0 8rpx;
  font-size: 24rpx;
  font-weight: 400;
  flex: 1;
  color: #333333;
}

.tips_go_icon {
  width: 12rpx;
  height: 20rpx;
  text-align: right;
  margin-right: 24rpx;
}
