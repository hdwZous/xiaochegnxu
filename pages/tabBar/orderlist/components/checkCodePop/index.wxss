.checkCodePop {
    position: fixed;
    z-index: 11;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background-color: rgba(0, 0, 0, 0.5);
    display: flex;
    align-items: center;
    justify-content: center;
}

.checkCodePopContainer {
    position: relative;
    width: 580rpx;
    min-height: 200rpx;
    text-align: center;
}

.bgTopBox {
    position: absolute;
    z-index: -1;
    width: 100%;
    height: 200rpx;
}

.bgTopBox .bgTop {
    width: 100%;
    height: 146rpx;
    background-color: #fff;
    border-top-left-radius: 32rpx;
    border-top-right-radius: 32rpx;
}

.bgTopBox .popBgCentre {
    position: absolute;
    top: 144rpx;
    left: 0;
    z-index: 1;
    width: 100%;
}


.titleContainer {
    min-height: 200rpx;
}

.titleContainer .title {
    text-align: center;
    padding-top: 40rpx;
    font-size: 40rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #333333;
}

.titleContainer .titleDate {
    text-align: center;
    margin-top: 8rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    color: #999999;
}



.container {
    background-color: #FFFFFF;
    border-bottom-left-radius: 32rpx;
    border-bottom-right-radius: 32rpx;
    /* margin-top: -6rpx; */
    position: relative;
    top: -4px;
    padding-top: 42rpx;
    padding-bottom: 32rpx;
}




.container .code {
    /* display: flex; */
    position: relative;
    margin: 0 100rpx;
    height: 240rpx;
}


.container .reloadBtn {
    width: 144rpx;
    height: 48rpx;
    background: #FFFFFF;
    border-radius: 24rpx;
    /* border: 1rpx solid #DADADA; */
    font-size: 24rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    color: #333333;
    margin: 32rpx auto 0;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}
.container .reloadBtn::after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 200%;
    height: 200%;
    box-sizing: border-box;
    border-radius: 48rpx;
    border: 2rpx solid #DADADA;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
}



.container .reloadBtn .reloadIcon {
    width: 24rpx;
    height: 24rpx;
    margin-right: 8rpx;
}

.container .codeId {
    box-sizing: border-box;
    text-align: center;
    width: 100%;
    margin-top: 32rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    color: #333333;
    position: relative;
    z-index: 9;
    overflow-y: auto;
    padding: 0 100rpx;
    display: flex;
    flex-wrap: wrap;
}

.codeId .idItem {
    padding: 8rpx 0;
    border-bottom: 1rpx solid #F5F5F5;
    width: 100%;
}
.codeId .idItem:last-child {
  border-bottom: none;
}


.codeId .idItem view:nth-last-child(2) {
    display: inline;
}

.codeId .idItem>view {
    text-align: left;
    word-break:break-all;
    word-wrap:break-word;
    width: 100%;
    /* display: flex; */
}

.codeId .idItem:first-child {
    margin-top: 0;
}

.codeId .idItem .openBtn {
    /* display: flex;
    align-items: center; */
    /* display: inline-block; */
    /* display: flex;
    align-items: center; */
    display: inline;
    font-size: 24rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    color: #999999;
    width: 80rpx;
    margin-left: 8rpx;
    /* float: right; */
}

.codeId .idItem .openBtn image {
    width: 16rpx;
    height: 16rpx;
    margin-left: 4rpx;
}


.closePop {
    width: 76rpx;
    height: 76rpx;
    margin: 64rpx auto 0;
}

.barcode {
    position: absolute;
    top: 50%;
    right: -80rpx;
    width: 240rpx;
    height: 80rpx;
    transform: translateY(-50%) rotate(270deg);
    -webkit-transform: translateY(-50%) rotate(270deg);
}

.strikeThrough {
    text-decoration: line-through;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    color: #999999;
}