@import "/common/css/light.wxss";
page {
    background: #f6f6f6;
    height: 100vh;
}
.gap {
    height: 98rpx;
}
::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
    display: none;
}
.forbid {
    overflow: hidden;
    height: 100vh;
}
.container {
    background: #f6f6f6;
    position: relative;
    overflow: hidden;
    /* z-index: 12; */
}
/* AB测试改动2 */
.sticky_case {
    max-height: 152rpx;
    background: #f6f6f6;
}

.sticky_case_b {
  z-index: 10;
  position: sticky;
  /* top: 96rpx; */
  top: 0rpx
}
.sticky_case_b_top150{
  /* top: 150rpx */
  top: 0rpx
}
.content-bg {
    width: 100%;
    background: #f6f6f6;
}
.content-container {
    background: #fff;
    border-radius: 32rpx 32rpx 0 0;
}
.store-nav-wrapper {
    background: #f6f6f6;
}
/* AB测试改动1 */
.sort-fixed {
    background: #f6f6f6;
}

.sort-fixed-b{
  position: sticky;
  position: -webkit-sticky;
  z-index: 10;
}

.category_case {
  width: 100%;
  position: fixed;
  top: -2rpx;
  z-index: 110;
  background: #f6f6f6;
}


.tab_switch_fix {
  width: 100%;
  position: fixed;
  top: 106rpx;
  z-index: 1;
  background: #f6f6f6;
  height: 60rpx;
}

.seat{
  width: 100%;
  height: 76rpx;
}

.seat_all_90{
  width: 100%;
  height: 90rpx;
}

.seat_all_0{
  width: 100%;
  height: 0rpx;
}

.footer {
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    background: #fff;
    z-index: 11;
}
.wran_case {
    height: 64rpx;
    align-items: center;
    justify-content: center;
    background: rgb(255, 246, 205);
}
.warn_txt {
    font-size: 24rpx;
    position: sticky;
    position: -webkit-sticky;
    z-index: 12;
}
.none-medicine {
    height: 100vh;
    padding-top: 100rpx;
    background-color: #fff;
    text-align: center;
}

.medicine_img {
    width: 100%;
}

.medicine_btn {
    margin: 60rpx 20rpx 0;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    background: linear-gradient(270deg, #00cf37 0%, #0fdf46 100%);
    border-radius: 40rpx;
    font-size: 32rpx;
    font-weight: 500;
    color: #fff;
}

.search-tag-floor-wrapper {
    background: #fff;
}

.search-tag-floor {
    height: 136rpx;
    background: #F8F8F8;
    border-radius: 6rpx;
    display: flex;
    overflow: hidden;
    padding-bottom: 24rpx;
    font-size: 0;
}

.tag-floor-item {
    height: 136rpx;
    width: 100rpx;
    margin-right: 16rpx;
}

.tag-floor-item:first-child {
    margin-left: 24rpx;
}

.tag-floor-item:last-child {
    padding-right: 16rpx;
}
.item-image-wrapper {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100rpx;
    width: 100rpx;
    background: #fff;
    padding: 8rpx;
    border-radius: 3px;
    box-sizing: border-box;
}

.tag-floor-item-img {
    height: 88rpx;
    width: 92rpx;
    background: #fff;
    border-radius: 2px;
    overflow: hidden;
}

.tag-floor-item-text {
    padding-top: 12rpx;
    font-size: 20rpx;
    font-weight: 400;
    color: #333;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
    text-align: center;
}
