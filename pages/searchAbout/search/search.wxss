.wrapper {
  width: 100%;
  min-height: 100vh;
  overflow: hidden;
  background: #f6f6f6;
}
.marTop56 {
  /* margin-top: 56rpx; */
  background-color: #fff;
  border-radius: 24rpx 24rpx 0 0;
  min-height: 100vh;
}
.type {
  font-size: 30rpx;
  color: #333;
  padding-bottom: 12rpx;
}
.type .clear {
  font-size: 24rpx;
  color: #666;
  float: right;
}
.elips {
  width: 279rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.history-search {
  padding: 40rpx 32rpx 0;
  /* margin-top: 32rpx; */
}

.history-search > .history-words {
  /* margin-top: 12rpx; */
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.ellipsis-two-line {
  max-height: 150rpx;
  overflow: hidden;
}

.word {
  font-size: 26rpx;
  color: #333;
  padding: 0rpx 24rpx;
  background: #f4f4f4;
  border-radius: 60rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
  max-width: 279rpx;
  height: 52rpx;
  line-height: 52rpx;
  /* overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; */
  display: flex;
  align-items: center;
}
.search_text{
  display: inline-block;
  max-width: 230rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.rank__wrapper {
  background-color: #fff;
  padding: 40rpx 0;
}