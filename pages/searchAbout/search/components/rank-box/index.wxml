<view class="rank-box">
  <block wx:if="{{searchFloorList.length === 1}}">
    <block wx:for="{{searchFloorList}}" wx:for-item="item" wx:key="rankType">
      <view class="rank-box__single">
        <rank searchFloor="{{item}}" traceId="{{traceId}}" buriedObj="{{buriedObj}}" />
      </view>
    </block>
  </block>
  <block wx:else>
    <view class="rank-box__multiple">
      <scroll-view class="d-flex align-items-start nowrap" scroll-x>
        <block wx:for="{{searchFloorList}}" wx:for-item="item" wx:key="rankType">
          <view class="rank-box__item">
            <rank searchFloor="{{item}}" traceId="{{traceId}}" buriedObj="{{buriedObj}}" />
          </view>
        </block>
      </scroll-view>
    </view>
  </block>
</view>