.guess-like-wrapper {
    /* margin-top: 40rpx; */
    background-color: #fff;
    padding-top: 32rpx;
    margin-bottom: 16rpx;
    border-radius: 20rpx;
    padding-left: 20rpx;
}
.title {
    display: flex;
    justify-content: center;
    font-size: 32rpx;
    color: #333;
    font-weight: 500;
    margin-bottom: 34rpx;
}
.title image {
    width: 40rpx;
    height: 40rpx;
}
.title .name {
    margin: 0 10rpx;
}
.guess-list-wrapper {
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
    padding-bottom: 16rpx;
}
.guess-item {
    width: 160rpx;
    height: 64rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    background: #f8f8f8;
    border-radius: 32rpx;
    font-size: 24rpx;
    color: #333;
    text-align: center;
    line-height: 64rpx;
    margin-bottom: 16rpx;
    margin-right: 10rpx;
    padding: 0 21rpx;
    box-sizing: border-box;
}
.guess-item:nth-last-of-type(4n-3) {
    margin-right: 0;
}
.guess-item-2 {
    width: 324rpx;
    height: 80rpx;
    overflow: hidden;
    background: #f8f8f8;
    border-radius: 64rpx;
    font-size: 28rpx;
    color: rgb(51, 51, 51);
    margin-bottom: 16rpx;
    padding: 0 8rpx;
    box-sizing: border-box;
    align-items: center;
}
.guess-item-2:nth-last-child(2n) {
    margin-right: 16rpx;
}
.logo_img_case {
    width: 64rpx;
    height: 64rpx;
    margin-right: 8rpx;
    border-radius: 50%;
    border: 1rpx solid rgba(0, 0, 0, 0.08);
}
.log_img {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.guess-selected {
    background: #ecf7ed;
    color: #47b34f;
}
