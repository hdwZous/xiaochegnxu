.commonSearch{
  flex-direction: row;
  -webkit-flex-direction: row;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  -webkit-align-items: center;
  padding: 24rpx;
  background-color: #f6f6f6;
}
.editbox {
  height: 60rpx;
  display: flex;
  align-items: center;
  border-radius: 60rpx;
  padding-left: 26rpx;
  padding-right:16rpx;
  background: #fff;
  flex:1;
}
.editboxDrawableLeft {
  width: 28rpx;
  height: 28rpx;
}

.editboxDrawableRight {
  width: 28rpx;
  height: 28rpx;
}

.editboxInput {
  line-height: 50rpx;
  display: flex;
  flex:1;
  align-items: center;
  font-size: 28rpx;
  margin-left: 12rpx;
  color: #333333
}
.editboxInputArea{
  width: 90%;
}
.editboxButton {
  font-size: 28rpx;
  margin-left: 28rpx;
  color: #333;
}
.section_up{
  background-color: #fff;
  border-radius: 24rpx 24rpx 0 0;
  height: 100%;
}