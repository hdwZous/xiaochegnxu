<view 
  class="banner_wrap"
  wx:if="{{data.prefixImgUrl}}"
  aria-role='link'
  aria-label="广告促销图" 
  data-user-action="{{data.userAction}}"
  data-page-id='{{pageId}}'
  data-current-page-name='{{currentPageName}}'
  data-pre-page-name='{{prePageName}}'
  data-trace-id="{{traceId}}">
    <image aria-hidden="true" class="banner_img" style="height: {{imgHeight}}" src="{{data.prefixImgUrl}}" bind:tap="clickImg" />
</view>