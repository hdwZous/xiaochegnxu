.tag-floor-item-con{
    height: 136rpx;
    width: 100rpx;
    background: #fff;
    border-radius: 12rpx;
  }
.item-image-wrapper {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100rpx;
    width: 100rpx;
    background: #fff;
    padding: 8rpx;
    border-radius: 8rpx;
    box-sizing: border-box;
}

.tag-floor-item-img {
    height: 88rpx;
    width: 92rpx;
    background: #fff;
    border-radius: 2px;
    overflow: hidden;
}

.tag-floor-item-text {
    font-size: 20rpx;
    font-weight: 400;
    color: #333;
	  overflow: hidden;
	  text-overflow: ellipsis;
	  white-space: nowrap;
    text-align: center;
}