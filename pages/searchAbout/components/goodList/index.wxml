<view class='list-wrapper' wx:if="{{goodList && goodList.length > 0}}">
  <block wx:for="{{goodList}}" wx:key="index" wx:for-item="item">
    <!-- 猜你喜欢 -->
    <guess-like wx:if="{{item.isShowGuess && showAllGuess}}" guessLike="{{item}}" traceId="{{traceId}}" currentPageName="{{currentPageName}}"
        prePageName="{{prePageName}}"
        pageId="{{pageId}}"
        recommendObj="{{recommendObj}}"/>
    <view class="good-contanier" wx:if="{{!item.isShowGuess}}">
      <block wx:if="{{item.storeBusinessType != 2}}">
        <!-- 单个商品 -->
        <view
          data-to="{{item.to}}"
          data-params="{{item.params}}"
          data-user-action="{{item.userActionSku}}"
          class="log-good good-item-wrapper {{item.guessNext ? 'good-item-wrapper-none':''}} {{!searchAll ? 'good-item-wrapper-none-bottom' : ''}}"
          data-item="{{item}}"
          data-sku-id="{{item.skuId}}"
          data-state="{{item.isJianyun ? 'Reduce_freight_commodity': ''}}"
          bindtap="jumpStore">
          <good-list-item
            traceId="{{traceId}}"
            item="{{item}}"
            currentPageName="{{currentPageName}}"
            prePageName="{{prePageName}}"
            recommendObj="{{recommendObj}}"
            pageId="{{pageId}}"/>
        </view>
        <!-- 门店信息 -->
        <view class="store-name-freight flex-row" 
              wx:if="{{item.storeName}}"
              aria-role='link'
              aria-label="{{item.storeName}}{{item.freightWords}}.配送时长{{item.deliveryTime}}{{item.distance ? '.配送距离' + item.distance : ''}}">
          <!-- 品牌logo -->
          <image aria-hidden="true" wx:if="{{item.storeLogo}}" class="store-brand" src="{{item.storeLogo}}" alt=""/>
          <!-- 无品牌logo -->
          <view aria-hidden="true" wx:if="{{!item.storeLogo}}" class="store-brand-none"></view>
          <view aria-hidden='true' style="display: flex;flex-direction: column;flex: 1">
            <!-- 门店名称 -->
            <view class='store-name-wrap' data-item="{{item}}" data-user-action="{{item.userActionStore}}" catchtap="goStore">
              <view class=" store-name">{{item.storeName}}</view>
              <view class="strore-arrow">
                <image src="https://storage.360buyimg.com/wxmini/cash/arrow.png"></image>
              </view>
            </view>
            
            <view class='freight' wx:if="{{item.closeTip.timeText}}">
              <view class="freight-time">
                <view class="freight-time-left" style="background: linear-gradient(90deg, {{item.closeTip.startColor}} 0%, {{item.closeTip.endColor}} 100%)">{{item.closeTip.reserveText}}</view>
                <view class="freight-time-right" style="color: {{item.closeTip.endColor}}">{{item.closeTip.timeText}}</view>
              </view>
              <!-- <view class="freight-text ellipsis good-time-text">{{item.closeTip.timeText}}</view> -->
            </view>

            <view class='freight' wx:if="{{item.freightWords && !item.closeTip.timeText}}">
              <view class="freight-text ellipsis {{item.freightDescForLine || item.freightWords ==='免运费' ? 'text-red': ''}}">{{item.freightWords}}</view>
              <view wx:if="{{item.freightDescForLine}}" class='line-price'>
                {{item.freightDescForLine}}
              </view>
            </view>
          </view>
          <!-- 时间、距离 -->
          <view aria-hidden="true" class='distance-deliveryTime'>
            <view wx:if="{{item.storeBrandLogo}}" style="height: 26rpx; text-align:right">
              <image mode="heightFix" class="fresh-tag" src="{{item.storeBrandLogo}}" />
            </view>
            <view wx:if="{{!item.storeBrandLogo && (item.closeTip.timeText || item.freightWords)}}" style="height: 26rpx; text-align:right">
            </view>
            <view class="distance-deliveryTime-flex">
              <view wx:if="{{item.deliveryTime}}">{{item.deliveryTime}}</view>
              <view class='mg10' wx:if="{{item.deliveryTime && item.distance}}">|</view>
              <view wx:if="{{item.distance}}">{{item.distance}}</view>
            </view>
          </view>
        </view>
      </block>
      <block wx:else>
        <DdSkuYhySearch item="{{item}}" keyword="{{keyword}}"/>
      </block>
    </view>
  </block>
  <view class='text-bottom' wx:if="{{!hasNextPage}}" style="height: 300rpx">已经到底啦</view>
  <!-- <view wx:if="{{!searchAll}}" style="height: 180rpx"></view> -->
</view>