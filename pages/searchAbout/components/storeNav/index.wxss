.scroll-box {
    /* position: relative; */
    width: 100%;
    padding-top: 12rpx;
    white-space: nowrap;
    /* background: linear-gradient(#fff 0%, #f6f6f6 100%); */
}
.side_arrow {
    align-items: center;
    justify-content: center;
    color: rgb(102, 102, 102);
    font-size: 20rpx;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    width: 88rpx;
    height: 108rpx;
    background: #f6f6f6;
    box-shadow: -20rpx 0 20rpx -10rpx rgba(0, 0, 0, 0.1);
}
.scroll-width {
    position: relative;
    display: flex;
    align-items: center;
}
.store-wrapper {
    height: 112rpx;
    width: 100%;
    display: flex;
    justify-content: flex-start;
    /* padding-left: 24rpx; */
    box-sizing: border-box;
}
.nav-item {
    position: relative;
    align-items: center;
    height: 96rpx;
    margin-right: 12rpx;
    font-size: 32rpx;
    color: #333;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 0 16rpx;
    position: relative;
    background: #fff;
    border-radius: 12rpx;
}
.nav-item:first-child {
    margin-left: 24rpx;
    font-size: 28rpx;
}
.store_arrow {
    position: absolute;
    left: 50%;
    bottom: -15rpx;
    transform: translateX(-50%);
    font-size: 20rpx;
    color: rgb(75, 214, 98);
}
.search_all_btn {
    padding: 0 20rpx;
}
.nav-item .logo {
    width: 68rpx;
    height: 68rpx;
    margin-right: 8rpx;
    border-radius: 8rpx;
    border: 1rpx solid rgba(0, 0, 0, 0.08);
    vertical-align: middle;
}
.nav-item .small-logo {
    width: 26rpx;
    height: 26rpx;
    margin-right: 6rpx;
    vertical-align: -1px;
}
.nav-item .title {
    max-width: 208rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.nav-item .title_size {
    font-size: 24rpx;
    font-weight: 500;
}

.nav-item .all_title_size {
  font-size: 28rpx;
  font-weight: 500;
}

.nav-item .store_tag {
    padding: 0 4rpx;
    display: inline-block;
    font-size: 20rpx;
    /* max-width: 224rpx; */
    border-radius: 4rpx;
    vertical-align: middle;
    position: relative;
    margin-top: 8rpx;
}
.nav-item .bottom-border {
    height: 4rpx;
    background: linear-gradient(135deg, #27dc0a 0%, #08c12a 100%);
    border-radius: 2rpx 2rpx 0 0;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
}
.nav-item:last-child .bottom-border {
    right: 54rpx;
}
.more_case_bg {
    position: fixed;
    left: 0;
    top: 98rpx;
    right: 0;
    bottom: 0;
    z-index: 20;
    background: rgba(0, 0, 0, 0.3);
}
.more_case_content {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 70vh;
    z-index: 20;
    background: #ffffff;
    border-radius: 0 0 32rpx 32rpx;
    overflow: hidden;
}
.more_case_title {
    padding: 18rpx 24rpx;
    align-items: center;
}
.more_case_txt {
    color: rgb(51, 51, 51);
    font-size: 28rpx;
}
.more_case_icon {
    font-size: 18rpx;
    color: #666666;
}
.more_case_list {
    position: relative;
}
.more_case_scroll {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
}
.more_case_warp {
    flex-wrap: wrap;
}
.nav_pop_item {
    padding: 0 12rpx;
    margin-bottom: 18rpx;
    width: 50%;
    box-sizing: border-box;
}
.more_case_pop_content {
    padding: 0 24rpx;
    height: 120rpx;
    background: rgb(244, 244, 244);
    border-radius: 8rpx;
    font-size: 32rpx;
    color: #333;
    display: flex;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
}
.store_right {
    overflow: hidden;
}
.nav_pop_item .logo {
    width: 68rpx;
    height: 68rpx;
    margin-right: 8rpx;
    border-radius: 8rpx;
    border: 1rpx solid rgba(0, 0, 0, 0.08);
    vertical-align: middle;
}
.nav_pop_item .small-logo {
    width: 28rpx;
    height: 28rpx;
    margin-right: 6rpx;
}
.nav_pop_item.title {
    max-width: 228rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.nav_pop_item .title_size {
    font-size: 28rpx;
}
.nav_pop_item .store_tag {
    padding: 0 8rpx;
    display: inline-block;
    font-size: 20rpx;
    border-radius: 4rpx;
}
.nav_pop_item .bottom-border {
    height: 4rpx;
    background: linear-gradient(135deg, #27dc0a 0%, #08c12a 100%);
    border-radius: 2rpx 2rpx 0 0;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
}
.selected {
    color: #ffffff;
    font-weight: bold;
    background: rgb(75, 214, 98);
}

.expansion_coupon{
  height: 32rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 32rpx;
  display: flex;
  border-radius: 4rpx;
  width: 100%;
}

.expansion_coupon_icon {
  width: 32rpx;
  height: 32rpx;
  border-radius: 4rpx;
  overflow: hidden;
}
.expansion_width {
  font-weight: 400;
  max-width: 100%;
  display: flex;
  flex: 1;
  padding-right: 4rpx;
}
.expansion_price{
  font-family: regular;
  font-size: 24rpx;
  max-width: 34rpx;
  flex: auto;
}
.nav-outer {
  position: relative;
}
.expansion-text{
  width:134rpx;
}
