<block wx:if="{{$taroCompReady}}"><view><block wx:if="{{showFlag}}"><view><block><block wx:if="{{isLoginStatus}}"><view><view class="{{style.status_box}}"><block><block wx:if="{{receiveBagsFlag == 1}}"><view class="{{style.success}}"><view class="{{style.icon}}"></view><view class="{{style.title}}">领取成功</view><view class="{{style.des}}">请出示给工作人员领取1个礼品</view><view class="{{style.time}}">{{timesStr}}后过期</view></view></block><block wx:else><view class="{{style.fail}}"><view class="{{style.icon}}"></view><view class="{{style.title}}">领取失败</view><view class="{{style.des}}">近期已经领过礼品了</view><view class="{{style.time}}"></view></view></block></block></view><block><block wx:if="{{receiveCouponFlag == 1}}"><view class="{{style.coupon_box}}"><view class="{{style.title}}">额外送您</view><view class="{{style.coupon}}">{{couponTitle}}</view><view class="{{style.btn_box}}"><view class="{{style.go_store}}" bindtap="goStore" data-e-tap-so="this">去门店看看</view><view class="{{style.go_coupon}}" bindtap="goCoupon" data-e-tap-so="this">我的优惠券</view></view></view></block><block wx:else><view class="{{[style.coupon_box, style.no_coupon_box]}}"><view class="{{style.btn_box}}"><view class="{{style.go_store}}" bindtap="goStore" data-e-tap-so="this">去门店看看</view><view class="{{style.go_coupon}}" bindtap="goCoupon" data-e-tap-so="this">我的优惠券</view></view></view></block></block></view></block><block wx:else><view class="{{style.login_box}}"><view class="{{style.joy}}"></view><view class="{{style.msg}}">抱歉，您还没有登录哦～ 登录才能领哦</view><view class="{{style.btn}}" bindtap="goLogin" data-e-tap-so="this">登录领礼品</view></view></block></block></view></block></view></block>
