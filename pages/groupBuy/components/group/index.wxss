@import "/common/css/global";

.wrap {
  padding: 0 20rpx;
}

.wrap .case {
  text-align: center;
  padding: 40rpx;
  border-radius: 12rpx;
  background: #fff;
  -webkit-box-shadow: 0 4rpx 16rpx 0 rgba(0, 0, 0, 0.06);
  box-shadow: 0 4rpx 16rpx 0 rgba(0, 0, 0, 0.06);
}

.wrap .title {
  color: #333;
  font-weight: bold;
  font-size: 48rpx;
}

.wrap .num {
  color: #ff5757;
}

.wrap .time {
  margin-top: 5rpx;
  color: #666;
  font-size: 30rpx;
}

.wrap .group {
  padding: 5rpx 10rpx;
}

.wrap .scroll {
  margin-top: 40rpx;
  width: 100%;
  height: 138rpx;
}

.wrap .item {
  margin: 0 30rpx;
  position: relative;
  width: 124rpx;
  height: 124rpx;
  border-radius: 100%;
  /* background: url("data:image/png;base64,") no-repeat center;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%; */
}

.wrap .item:first-child {
  margin-left: 0;
}

.wrap .item:last-child {
  margin-right: 0;
}

.wrap .leader_case {
  color: #fff;
  font-size: 22rpx;
  width: 112rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  position: absolute;
  left: 7rpx;
  bottom: 4rpx;
  background: url("https://storage.360buyimg.com/wximg/groupbuy/headCover.png") no-repeat center;
  /* background: url("data:image/png;base64,") no-repeat center; */
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}

.wrap .headImg {
  -webkit-box-shadow: 0 4rpx 10rpx rgba(0, 0, 0, 0.3);
  box-shadow: 0 4rpx 10rpx rgba(0, 0, 0, 0.3);
}

.wrap .img {
  width: 116rpx;
  height: 116rpx;
  border-radius: 100%;
  border: 4rpx solid #fff;
  margin-top: 3rpx;
}

.wrap .member .img {
  width: 124rpx;
  height: 124rpx;
}

.wrap .btn {
  margin-top: 30rpx;
  color: #fff;
  font-size: 32rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 50rpx;
  -webkit-box-shadow: 0 4rpx 8rpx rgba(165, 7, 9, 0.22);
  box-shadow: 0 4rpx 8rpx rgba(165, 7, 9, 0.22);
  background: linear-gradient(to right, #fd821d, #f32125);
  background: -webkit-linear-gradient(to right, #fd821d, #f32125);
}

.wrap .btn::after {
  border: none;
  /* background: #fff */
}

.wrap .tips {
  margin-top: 30rpx;
  color: #333;
  font-size: 26rpx;
}

.wrap .txt_btn {
  margin-top: 30rpx;
  color: #333;
  font-size: 30rpx;
  text-align: center;
}