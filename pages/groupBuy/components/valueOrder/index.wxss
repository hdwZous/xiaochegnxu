.orderEmpty {
  margin-top: 50 rpx;
  color: #999999;
  text-align: center;
  height: 100 rpx;
  margin-top: 30px;
}

.emptyIcon image {
  width: 320rpx;
  height: 320rpx;
}

.orderEmpty .text {
  font-size: 30rpx;
  margin-top: 11px;
}

.orderEmpty .btn {
  display: flex;
  justify-content: center;
  margin-top: 85rpx;
}

.orderEmpty .btn view {
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  font-size: 34rpx;
  text-align: center;
  background: rgba(71, 179, 79, 1);
  border-radius: 6px;
}

.order-list-container .List {
  padding: 20rpx;
  background: #fff;
  margin-bottom: 20rpx;
  font-size: 32rpx;
}

.order-list-container .List .header image {
  width: 20rpx;
  height: 20rpx;
}

.order-list-container .List .header {
  display: flex;
  justify-content: space-between;
}

.order-list-container .List .header .fail {
  color: #999999;
}

.order-list-container .List .content {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 0;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.08);
}

.order-list-container .List .goodsImg {
  position: relative;
}

.order-list-container .List .content .goodsImg image {
  width: 100rpx;
  height: 100rpx;
}

.goodsMsg {
  text-align: right;
  flex: auto;
}

.goodsMsg .skuPrice {
  color: #FF5757;
  font-size: 36rpx;
}

.goodsMsg .totalNum {
  font-size: 24rpx;
  color: #666666;
}

.footer .left {
  font-size: 24rpx;
  color: #333;
  padding-top: 25rpx;
  line-height: 60rpx;
}

.footer .left .groupMsg {
  display: flex;
  color: #333;
}

.footer .left .groupMsg view:nth-child(2) {
  margin: 8rpx 12.5rpx;
  width: 2rpx;
  background: rgba(0, 0, 0, 0.08);
  height: 20rpx;
}

.List .footer {
  display: flex;
  justify-content: space-between;
}

.List .footer .right {
  padding-top: 25rpx;
  flex: auto;
  display: flex;
  justify-content: flex-end;
}

.List .footer .left .left-time {
  margin-top: 10rpx;
}

.List .footer .right view {
  width: 160rpx;
  height: 60rpx;
  text-align: center;
  color: #ffffff;
  background: rgba(255, 87, 87, 1);
  line-height: 60rpx;
  border-radius: 4rpx;
  font-size: 28rpx;
}

.goodsMsg .tagGroup {
  display: flex;
  justify-content: flex-end;
  margin-top: 10rpx;

}

.goodsMsg .collection-tag {
  padding: 0 6rpx;
  line-height: 28rpx;
  font-size: 20rpx;
  color: #47B34F;
  border: 1rpx solid #47B34F;
  border-radius: 6rpx;
}

.goodsMsg .pre-tag {
  color: #FF5757;
  padding: 0 6rpx;
  line-height: 28rpx;
  font-size: 20rpx;
  border: 1rpx solid #ff5757;
  margin-right: 10rpx;
  border-radius: 6rpx;
}

.group_tips {
  background: -webkit-linear-gradient(135deg, rgba(254, 106, 72, 1), rgba(255, 63, 63, 1));
  background: linear-gradient(135deg, rgba(254, 106, 72, 1), rgba(255, 63, 63, 1));
  border-radius: 16rpx 2rpx 16rpx 2rpx;
  font-size: 20rpx;
  color: #fff;
  height: 32rpx;
  line-height: 35rpx;
  width: 84rpx;
  display: inline-block;
  text-align: center;
  position: absolute;
  right: 9rpx;
  margin-left: -47rpx;
  bottom: 0;
}

.flags {
  align-items: center;
  justify-content: flex-end
}

.flags view {
  display: inline-block;
  padding: 2rpx 8rpx;
  line-height: 30rpx;
  height: 30rpx;
  text-align: center;
  font-size: 20rpx;
  border-radius: 2rpx;
  margin-left: 10rpx;
}

.flags .collection {
  color: #47B34F;
  background-color: rgba(71, 179, 79, 0.1);
}

.pre-flag {
  color: #FF5757;
  background: rgba(255, 87, 87, 0.1);
}

.flags .self-tag {
  background: #ffefd6;
  color: #FB861F;
}

.flags .to-home {
  background: #ffefd6;
  color: #FB861F;
}