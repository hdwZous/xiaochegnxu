<view class="wrap">
    <!--搜索框-->
    <view class="search">
        <view class="input" bindtap="goToSearch">
            <view class="icon_search"></view>
            <view class="placeholder">请输入定位地址</view>
        </view>
    </view>
    <!--地图地位-->
    <view class="map" bindtap="goToMap">
        <view class="icon_location"></view>
        <view class="txt">{{addressInfo.poi||'点击定位当前地点'}}</view>
        <view class="icon_arrow"></view>
    </view>
    <!--地址列表-->
    <view class="address">
        <!--地址-->
        <scroll-view class="scroll" scroll-y >
            <block wx:if="{{!showEmpty}}">
                <!--可配送范围地址-->
                <view>
                    <!--地址头部-->
                    <view class="title">附近的提货门店</view>
                    <!--地址详情-->
                    <!--storeId:'123',-->
                    <!--storeName:"百果园林肯公园店",-->
                    <!--storeAddress:"百果园林肯公园店",-->
                    <!--storeOpenTimeTitle:'门店营业时间',-->
                    <!--storeOpenTime:"09:00-21:00",-->
                    <!--storeDistance: '0.6km'-->

                    <!--data-city-id="{{item.cityId}}"-->
                    <!--data-county-id="{{item.countyId}}"-->
                    <!--data-id="{{item.id}}"-->
                    <!--data-county-name="{{item.countyName}}"-->
                    <!--data-city-name="{{item.cityName}}"-->
                    <!--data-address-name="{{item.addressName}}"-->
                    <!--data-mobile="{{item.mobile}}"-->
                    <!--data-name="{{item.name}}"-->

                    <view class="item flex-col"
                          wx:for="{{storeList}}"
                          wx:for-item="item"
                          wx:for-index="index"
                          wx:key="storeList{{index}}"
                          data-latitude="{{item.latitude}}"
                          data-longitude="{{item.longitude}}"
                          data-store-Id="{{item.storeId}}"
                          data-org-code="{{item.orgCode}}"
                          bindtap="selectAddress">

                        <view class="store-name"><text>{{item.storeName}}</text><text class="toMap" catchtap="toMap"  data-latitude="{{item.latitude}}"
                                                                                      data-longitude="{{item.longitude}}"></text></view>
                        <view class="address-detail"><text class="address-detail-text ellipsis">{{item.storeAddress}}</text><text class="distance">{{item.storeDistance/1000}}km</text></view>
                        <view class="open-time">{{item.storeOpenTimeTitle}}：{{item.storeOpenTime}}</view>
                    </view>
                </view>
            </block>
            <block wx:else>
                <default type="{{type}}"
                         btnText="{{btnText}}"
                         tips="{{tips}}"
                         src="{{src}}"
                         fromPositon='groupBuy-chooseStore'
                         buriedObj="{{recommendObj}}"
                         bind:defaultBtnEvent="onDefaultBtnEvent"
                         fromPostion="grounpBuy-chooseStore"
                ></default>
            </block>
        </scroll-view>
        <!--登录默认页-->
    </view>
</view>