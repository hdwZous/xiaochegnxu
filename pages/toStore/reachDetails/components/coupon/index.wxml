<view class="conpon__wrapper">
  <floor-layout title="{{title}}" my-class="my-class">
    <view class="conpon__content" slot="content">
      <scroll-view class="d-flex nowrap" scroll-x>
        <block wx:for="{{couponList}}" wx:key="couponId" wx:for-item="item">
          <coupon-item coupon="{{item}}" index="{{index}}" traceId="{{traceId}}" buriedObj="{{buriedObj}}" />
        </block>
      </scroll-view>
    </view>
  </floor-layout>
</view>
