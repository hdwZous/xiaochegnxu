<view class="floor-item">
  <block wx:if="{{type === 1}}">
    <banner images="{{images}}" />
  </block>
  <block wx:elif="{{type === 2}}">
    <essential skuInfo="{{skuInfo}}" buttonInfo="{{buttonInfo}}" serviceTagList="{{serviceTagList}}" />
  </block>
  <block wx:elif="{{type === 3 && couponList && couponList.length}}">
    <coupon title="{{title}}" couponList="{{couponList}}" traceId="{{traceId}}" buriedObj="{{buriedObj}}"  />
  </block>
  <block wx:elif="{{type === 4 && combos && combos.length}}">
    <set-meal title="{{title}}" combos="{{combos}}" />
  </block>
  <block wx:elif="{{type === 5 && ruleList && ruleList.length}}">
    <notes title="{{title}}" ruleList="{{ruleList}}" />
  </block>
  <block wx:elif="{{type === 6}}">
    <store title="{{title}}" storeInfo="{{storeInfo}}" moreText="{{moreText}}" moreJump="{{moreJump}}" config="{{config}}" />
  </block>
  <block wx:elif="{{type === 7 && skuList && skuList.length}}">
    <recommend title="{{title}}" skuList="{{skuList}}" traceId="{{traceId}}" buriedObj="{{buriedObj}}" />
  </block>
  <block wx:elif="{{type === 8}}">
    <evaluate title="{{title}}" moreText="{{moreText}}" moreJump="{{moreJump}}" config="{{config}}" />
  </block>
</view>