
<view class="tostore-bg-wrap" aria-hidden="true">
  <view class="bg" 
      wx:if="{{url}}" 
      aria-hidden="true"
      style="height: {{(couponInfo.length || storeMemberName) ? '400rpx' : height + 'px'}}">
    <view class="logo_bg">
      <image wx:if="{{url}}" src="{{url}}"></image>
    </view>
    <view class='mask'></view>
    <!-- style="bottom: {{(promotionList.length || couponInfo.length) ? '-2rpx' : '50rpx' }}" -->
    <view class="glass"></view>
  </view>
  <!-- 兜底样式 -->
  <view class="bg1" 
        aria-hidden="true"
        wx:else>
    <view class="logo_bg1">
      <image class='logo_bg1_image filter1' src="{{logo}}"></image>
    </view>
    <view class='gradient'>
      <view class="glass1"></view>
      <view class='blank'></view>
    </view>
  </view>
  <view class="tostore-bg-blank"></view>
</view>


