<block wx:if="{{$taroCompReady}}"><block><block wx:if="{{showDefault}}"><default compid="{{$compid__188}}"></default></block><block wx:else><template is="renderMain" data="{{...anonymousState__temp}}"></template></block></block></block>
<template name="renderMain"><block><view class="{{[style.grayBg]}}"><image mode="widthFix" class="{{style.banner}}" src="{{bannelLogo}}"></image><view class="{{[style.infoBox]}}"><block><block wx:if="{{isSuccess}}"><view><view class="{{style.descriptionBig}}">恭喜！已领取京东到家会员</view><block wx:if="{{phone}}"><view class="{{style.description}}">会员已放入：<text class="{{style.phone}}">{{phone}}</text></view></block></view></block><block wx:else><view class="{{style.codeInputBox}}"><input bindinput="inputFn" maxlength="{{8}}" placeholder="请输入兑换码" placeholder-style="color:#423B37;" value="{{code}}" class="{{style.codeInput}}" data-e-input-so="this"/></view></block></block><view bindtap="getCouponFn" class="{{[style.btn, style.btn_normal]}}" data-e-tap-so="this">{{buttionId == 1 && !isSuccess ? '立即兑换' : '去京东到家逛逛'}}</view></view><image mode="widthFix" class="{{style.goods}}" src="{{vipLogo}}"></image><vip-pop-card compid="{{$compid__189}}"></vip-pop-card></view></block></template>
