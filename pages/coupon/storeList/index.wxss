
.wrap {
    height: 100vh;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
}
.wrap .search{
  flex:none;
  display:-webkit-flex;
  display:flex;
  padding: 20rpx 20rpx;
  background: #47B34F;
  -webkit-flex-direction:row;
  flex-direction:row;
}
.wrap .search .input {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
    padding: 0 20rpx;
    height: 60rpx;
    border-radius: 60rpx;
    background: #F4F4F4;
    width: 100%;
}
.wrap .search .icon_search {
    margin-right: 10rpx;
    width: 30rpx;
    height: 30rpx;
    background: url("data:image/png;base64,") center no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}
.wrap .search .placeholder {
    color: #999999;
    font-size: 24rpx;
}
.wrap .map {
    position: relative;
    -webkit-flex: none;
    flex: none;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    -webkit-align-items: center;
    align-items: center;
    height: 104rpx;
    padding: 0 30rpx;
}
.wrap .section {
  position:relative;
  -webkit-flex:auto;
  flex:auto;
  display:-webkit-flex;
  display:flex;
  -webkit-flex-direction:column;
  flex-direction:column;
}
.section .scroll {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}
.section .up {
    background: #FFFFFF;
}
.section .item {
    position: relative;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    overflow: hidden;
    padding: 30rpx 20rpx;
}
.section .item:before {
    content: "";
    position: absolute;
    bottom: 0;
    left: 20rpx;
    width: 200%;
    height: 1px;
    background: #E8E8E8;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}
.position {
	padding:30rpx 20rpx;
	line-height:30rpx;
  border-bottom:1rpx solid #eee;
}
.position image:nth-child(1){
	width:24rpx;
	height:32rpx;
	float:left;
}
.position image:nth-child(3){
	width:12rpx;
  height:24rpx;
  float:right;
  margin-top:6rpx;
}

.position text {
	font-size:30rpx;
	color:#47B34F;
	padding-left:6rpx;
}
.stroeList {
	padding:0 20rpx;
  position:relative;
  -webkit-flex:auto;
  flex:auto;
}
.title {
	font-size:34rpx;
	padding: 34rpx 20rpx;
}
.list {
	padding:30rpx 20rpx;
  border-bottom:1rpx solid #eee;
}
.list  .stroeName {
	font-size:30rpx;
	color:#333;
	padding-bottom:10rpx;
}
.list  .stroeName image {
  width: 22rpx;
  height: 30rpx;
  float: right;
  padding-top: 10rpx;
}
.list .stroePosition {
	font-size:26rpx;
	padding-bottom:10rpx;
	color:#999;
}
.list .stroePosition text:nth-child(2) {
	float: right;
}
.list  .storeWorktime {
	font-size:26rpx;
	color:#999;
}
.list  .addressDeatil {
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  width:640rpx;
  display:inline-block;
}