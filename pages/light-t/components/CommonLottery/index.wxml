<template name="renderCouponBox"><block><block wx:if="{{normalPrizeResult.prizeType && normalPrizeResult.prizeType === 3}}"><view class="{{style.couponContentBox}}"><view class="{{style.title}}">恭喜获得实物大奖</view><view class="{{style.desc1}}">获得{{normalPrizeResult.prizeName}}</view><image class="{{style.shiwu_img}}" src="{{normalPrizeResult.prizeImgUrl}}"></image><view class="{{style.desc3}}">可在“我的奖品”中查看</view><view class="{{style.button}}" bindtap="toOpenEdit" style="{{_$anonymousState__temp}}">填写地址</view></view></block><view class="{{style.couponContentBox}}" wx:else><view class="{{style.title}}">恭喜点亮{{lightName}}</view><view class="{{style.desc1}}">获得一张大额优惠券</view><view class="{{style.couponBox}}"><view class="{{style.left}}"><block wx:if="{{normalPrizeResult.couponType == 1 || normalPrizeResult.couponType == 2}}"><view class="{{style.unit1}}">￥</view></block><view class="{{style.price1}}">{{moneyList[0]}}</view><block wx:if="{{moneyList.length == 2}}"><view class="{{style.price2}}">{{'.' + moneyList[1]}}</view></block><block wx:if="{{normalPrizeResult.couponType == 5}}"><view class="{{style.unit2}}">折</view></block></view><view class="{{style.right}}"><view class="{{style.couponTitle}}">{{normalPrizeResult.couponName ? normalPrizeResult.couponName : ''}}</view><view class="{{style.couponDesc}}">{{normalPrizeResult.limit ? normalPrizeResult.limit : ''}}</view></view></view><view class="{{style.desc2}}">可在“我的奖品”中查看</view><view class="{{style.button}}" bindtap="use">去使用</view></view></block></template>
<block wx:if="{{$taroCompReady}}"><view class="{{style.commonLotteryBox}}"><block wx:if="{{isShowOpenAward}}"><view class="{{style.openAwardBox}}"><view class="{{style.diaMask}}"></view><view class="{{style.openAwardContent}}"></view></view></block><block wx:if="{{isShowResult}}"><view class="{{style.resultBox}}"><view class="{{style.diaMask}}" bindtap="anonymousFunc0"></view><view class="{{style.contentBox}}" style="{{anonymousState__temp3}}"><block wx:if="{{normalPrizeResultType == 'coupon'}}"><template is="renderCouponBox" data="{{...anonymousState__temp2}}"></template></block><block wx:if="{{normalPrizeResultType == 'no'}}"><view class="{{style.noAwardContentBox}}"><view class="{{style.title}}">哎呀，奖品飞走了</view><view class="{{style.desc}}">奖品不重要、开心最重要</view><view class="{{style.noAwardImage}}"></view><view class="{{style.button}}" bindtap="anonymousFunc1">去打卡</view></view></block><view class="{{style.closeButton}}" bindtap="anonymousFunc2"></view></view></view></block></view></block>
