.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.goods_wrap {
    width: 344rpx;
    background: #ffffff;
    border-radius: 24rpx;
}
.goods_image_case {
    position: relative;
    height: 344rpx;
    border-radius: 24rpx 24rpx 0 0;
}
.goods_image {
    width: 344rpx;
    height: 344rpx;
    border-radius: 24rpx 24rpx 0 0;
}
.goods_tag {
    font-size: 20rpx;
    border-radius: 4rpx;
    padding: 0rpx 6rpx;
    color: #ffffff;
    height: 30rpx;
    border-style: solid;
    border-width: 1px;
    margin: 0 8rpx 2rpx 0;
}

.goods_name_wrapper {
    display: flex;
    align-items: center;
    padding: 0 16rpx;
    padding-top: 8rpx;
}
.goods_name {
    flex: 1;
    color: rgb(51, 51, 51);
    font-size: 28rpx;
    font-weight: bold;
    line-height: 34rpx;
    max-height: 68rpx;
    box-sizing: border-box;
    float: left;
    box-sizing: border-box;
    max-width: 100%;
}
.goods_subtitle {
    color: rgb(153, 153, 153);
    font-size: 24rpx;
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 0 16rpx;
}
.goods_name_tag {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 28rpx;
    line-height: 28rpx;
    padding: 0 8rpx;
    font-size: 20rpx;
    border-radius: 4rpx;
    margin-right: 4rpx;
}
.goods_tag_case {
    /* height: 44rpx; */
    height: 36rpx;
    padding: 0 16rpx 0;
    overflow: hidden;
    flex-wrap: wrap;
    align-items: flex-start;
}
.coupon_txt {
    padding: 1rpx 10rpx;
    font-size: 20rpx;
    border-radius: 4rpx;
}
.coupon_gap {
    margin: 0 8rpx 20rpx 0;
}
.goods_price_old_case {
    margin-left: 4rpx;
    align-items: center;
}
.goods_price_now_case {
    align-items: baseline;
}
.goods_price_case {
    /* height: 50rpx; */
    padding: 0 16rpx;
    align-items: center;
    margin-top: 8rpx;
    /* padding-bottom: 16rpx; */
    display: flex;
}
.one-line-show {
    display: flex;
}
.dd-goby{
    margin-left: auto;
}
.goods_price_now {
    font-size: 36rpx;
    font-weight: bold;
    font-family: "regular";
}
.goods_price_old {
    font-size: 26rpx;
    font-family: "light";
}
.goods_weight {
    font-size: 24rpx;
    color: rgb(102, 102, 102);
}
.goods_price_img {
    width: 57rpx;
    height: 17rpx;
}
.goods_tips_case {
    font-size: 0;
    margin-top: 8rpx;
    margin-left: 16rpx;
    max-width: 312rpx;
}
.goods_attribute_box {
    padding: 0 16rpx;
    margin-top: 8rpx;
}
.store_info_case {
    align-items: center;
    margin: 0 16rpx;
    border-top: 1px solid rgba(246, 246, 246, 1);
    margin-top: 12rpx;
    padding-top: 12rpx;
    padding-bottom: 16rpx;
}
.store_info_right {
    overflow: hidden;
}
.store_info_logo {
    width: 56rpx;
    height: 56rpx;
    border-radius: 8rpx;
    border: 1rpx solid rgb(245, 245, 245);
    vertical-align: middle;
    margin-right: 8rpx;
}
.store_info_name {
    color: rgba(51, 51, 51, 1);
    font-size: 24rpx;
}
.store_info_desc {
    color: rgba(102, 102, 102, 1);
    font-size: 20rpx;
    height: 28rpx;
    line-height: 28rpx;
    position: relative;
}
.store_info_desc .name{
    color: rgba(102, 102, 102, 1);
    font-size: 20rpx;
    height: 28rpx;
    line-height: 28rpx;
}
.store_info_desc .free_feight{
    color: #999;
    font-size: 20rpx;
    height: 28rpx;
    line-height: 28rpx;
    text-decoration: line-through;
    display: inline-flex;
}
.tag_left_top_image_wrap {
    padding: 0 6rpx;
    position: absolute;
    left: 16rpx;
    top: 16rpx;
    align-items: center;
    height: 32rpx;
    border-radius: 4rpx;
}
.biz_tag_left_top{
  position: absolute;
  left: 18rpx;
  top: 18rpx;
}
.tag_left_top_image {
    width: 24rpx;
    height: 24rpx;
}
.tag_left_top_image_txt {
    font-size: 20rpx;
}
.tag_ad{
    position: absolute;
    right: 16rpx;
    top: 324rpx;
    width: 32rpx;
    height: 22rpx;
}
.tag_ad_image{
    width: 32rpx;
    height: 22rpx;
    position: absolute;
    left: 0;
    top: 0;
}
.goods_aword {
    font-size: 24rpx;
    line-height: 30rpx;
    color: #666666;
    margin-top: 8rpx;
    height: 30rpx;
}
.dd_sku_feed_row {
    display: flex;
    flex-direction: row;
    align-items: center;
}
.dd_sku_feed_clounm {
    display: flex;
    flex-direction: column;
}
