.label {
    font-size: 20rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #333333;
    position: relative;
    z-index: 9;
    text-align: center;
}
.time {
    color: #FC3D46;
    display: inline-flex;
}
.btn-close {
    width: 100rpx;
    height: 100rpx;
    position: absolute;
    bottom: 80rpx;
    left: 50%;
    transform: translateX(-50%);
}
.icon {
    width: 66rpx;
    height: 66rpx;
    position: absolute;
    left: 20rpx;
}
.btn-tip {
    bottom: 226rpx;
}
.btn-refresh {
    bottom: 120rpx;
}
.order-gift-left {
    width: 92rpx;
    height: 92rpx;
    position: absolute;
    left: 7rpx;
    bottom: 320rpx;
    background-repeat: no-repeat;
    background-size: 100% auto;
    background-position: center;
}
.order-img {
    width: 100%;
    height: 100%;
}
.pop6-title-tag {
    color: #fff;
}
.deliveryMan .label-wrap {
    width: 180rpx;
    display: block;
    padding-bottom: 0;
}
.deliveryMan .head-wrap  {
    width: 300rpx;
}
.buyer .icon-head {
    left: 0;
}