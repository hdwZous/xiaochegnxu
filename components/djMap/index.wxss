.map {
    position: relative;
    width: 100%;
    transition: height .2s;
}
.map.full-screen {
    position: fixed;
    left: 0;
    top: 0;
    z-index: 999;
    transition: height .2s;
}
.customCallout {
    /* border: 1px solid red; */
    display: flex;
    justify-content: center;
    flex-direction: column;
    position: relative;
}
.head-wrap {
    display: flex;
    justify-content: center;
    flex-direction: column;
    transform: translateX(-50%);
    width: 82rpx;
    height: 90rpx;
    padding: 6rpx 0 0 4rpx;
    position: relative;
    left: 50%;
    z-index: 999;
}
.bg-head {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}
.deliveryMan .bg-head {
    width: 76rpx;
    height: 90rpx;
    position: absolute;
    left: 115rpx;
    top: 0;
    z-index: 1;
}
.icon-head {
    background-color: #f2f1ed;
    border-radius: 50%;
    overflow: hidden;
    border: 0;
    width: 78rpx;
    height: 78rpx;
    justify-self: center;
    display: flex;
    position: absolute;
    left: 115rpx;
    bottom: 10rpx;
    /* border: 1px solid blueviolet; */
}
.icon-location {
    width: 20rpx;
    height: 20rpx;
    position: absolute;
    left: 144rpx;
    bottom: -2rpx;
    z-index: 1;
}
.label-wrap {
    background-color: white;
    border: 1rpx solid #e9e9e9;
    border-radius: 20rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    position: relative;
    z-index: 99;
    left: 50rpx;
    top: 0rpx;
    margin: 0 30rpx 10rpx;
    padding: 10rpx 20rpx ;
    width: auto;
    height: 40rpx;
    overflow: visible;
}
