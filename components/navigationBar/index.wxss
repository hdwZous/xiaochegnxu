.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.flex{
    display: -webkit-flex;
    display: flex;
}
.flex_row {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
}
.just_content_center{
    justify-content: center;
    -webkit-justify-content: center;
}
.text_center{
    text-align:center;
}

.flex_col {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
}

.flex_none {
    -webkit-flex: none;
    flex: none;
}

.flex_auto {
    -webkit-flex: auto;
    flex: auto;
}
.align_item_center{
    -webkit-align-items:center;
    align-items:center;
}
