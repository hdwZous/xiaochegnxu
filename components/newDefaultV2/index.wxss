@import "/common/css/global.wxss";
@import "/common/css/regular.wxss";
.default {
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-align-items: center;
  align-items: center;
  box-sizing: border-box;
}
.default .text {
  color: #666666;
  font-size: 28rpx;
  line-height: 28rpx;
  width: 100%;
  text-align: center;
  font-weight: 500;

}
.default .subtext {
  color: #999;
  font-size: 24rpx;
  line-height: 32rpx;
  width: 100%;
  margin: 12rpx auto 0;
  text-align: center;
}

.default .btn {
  margin-top: 24rpx !important;
  margin-left: 32rpx;
  margin-right: 32rpx;
  color: #ffffff;
  font-size: 28rpx;
  border-radius: 12rpx;
  display: inline-block;
  height: 80rpx;
  line-height:80rpx;
  background: #00CF37;
  border-radius: 44rpx;
  width: 232rpx;
  text-align: center;
  font-family: regular;
  min-width: 240rpx;
}
.default .box{
  width: 100%;
}
