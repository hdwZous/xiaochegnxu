.rank-wrap {
  display: inline-block;
  max-width: 100%;
  overflow: hidden;
  font-size: 0;
}

.ranking-list {
  height: 36rpx;
  padding: 4rpx 8rpx;
  border-radius: 4rpx;
}

.ranking-list .ranking-icon {
  flex-shrink: 0;
  flex-grow: 0;
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}

.ranking-list .ranking-desc {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
}

.ranking-right-icon {
  flex-shrink: 0;
  flex-grow: 0;
  width: 16rpx;
  height: 16rpx;
  margin-left: 6rpx;
  /* margin-top: 4rpx; */
}